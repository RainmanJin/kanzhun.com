html {
  height: 100%;
}
body {
  font: normal 14px/1.5 Helvetica, "Microsoft YaHei", STHeiti, "Droid Sans Fallback", sans-serif;
  color: #343434;
  overflow-x: hidden;
  height: 100%;
}
a {
  color: #343434;
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
}
a.blue {
  color: #26abbe;
}
h1,
h2,
h3 {
  font-size: 14px;
  line-height: 1;
}
.wrapper {
  width: 100%;
}
.none {
  display: none;
}
.maskui_dialog {
  width: 90%;
  background-color: #fff;
  border-radius: 8px;
  display: none;
}
.maskui_dialog .dialog_con {
  position: relative;
  min-height: 100px;
  color: #666;
  padding: 20px 10px;
}
.maskui_dialog .dialog_con h1 {
  font-size: 18px;
  font-weight: normal;
  text-align: center;
  line-height: 1.3;
}
.maskui_dialog .dialog_con h1 em {
  font-weight: bold;
}
.maskui_dialog .dialog_con .p_info {
  font-size: 12px;
  line-height: 1.2;
}
.maskui_dialog .dialog_con h3 {
  font-size: 16px;
  line-height: 1;
  font-weight: normal;
}
.maskui_dialog .dialog_close {
  width: 23px;
  height: 23px;
  display: inline-block;
  position: absolute;
  right: 10px;
  top: 10px;
  -webkit-transition: all .2s linear;
  -moz-transition: all .2s linear;
  -ms-transition: all .2s linear;
  -o-transition: all .2s linear;
  transition: all .2s linear;
}
.maskui_dialog .dialog_close:hover {
  background-color: #eee;
}
.maskui_dialog .dialog_ac {
  padding: 10px 0 25px 0;
  text-align: center;
  /*  p {
     margin-top: 20px;
   } */
}
.maskui_dialog ul.news_btn {
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  overflow: hidden;
  border-top: 1px solid #cbcbcb;
}
.maskui_dialog ul.news_btn li {
  float: left;
  width: 50%;
}
.maskui_dialog ul.news_btn li a {
  display: block;
  height: 34px;
  line-height: 34px;
  text-align: center;
}
.maskui_dialog ul.news_btn li:first-child a {
  border-right: 1px solid #cbcbcb;
}
.m_dialog_con {
  padding: 15px;
}
.m_dialog_btn {
  height: 42px;
  line-height: 42px;
  border-top: 1px solid #ccc;
  font-size: 16px;
}
.m_dialog_btn .btn {
  width: 50%;
  text-align: center;
  float: left;
  height: 42px;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  -ms-box-sizing: border-box;
  box-sizing: border-box;
  border: 0;
  background-color: #ffffff;
}
.m_dialog_btn .btn_block {
  width: 100%;
  border-bottom-left-radius: 8px;
  border-bottom-right-radius: 8px;
}
.m_dialog_btn .btn_ok {
  border-left: 1px solid #ccc;
  color: #7cb228;
}
.animated {
  -webkit-animation-duration: 1s;
  animation-duration: 1s;
  -webkit-animation-fill-mode: both;
  animation-fill-mode: both;
}
.infinite {
  -webkit-animation-iteration-count: infinite;
  animation-iteration-count: infinite;
}
.hinge {
  -webkit-animation-duration: 2s;
  animation-duration: 2s;
}
@-webkit-keyframes fadeInUp {
  0% {
    opacity: 0;
    -webkit-transform: translate3d(0, 50%, 0) scale(0.2);
    transform: translate3d(0, 50%, 0) scale(0.2);
  }
  100% {
    opacity: 1;
    -webkit-transform: scale(1);
    transform: scale(1);
  }
}
.fadeInUp {
  -webkit-animation-name: fadeInUp;
  animation-name: fadeInUp;
  -webkit-animation-timing-function: ease-out;
  animation-timing-function: ease-out;
  -webkit-animation-duration: .3s;
  animation-duration: .3s;
  -webkit-animation-fill-mode: both;
  animation-fill-mode: both;
}
@-webkit-keyframes bounceInDown {
  0% {
    opacity: 0;
    -webkit-transform: translate3d(0, -50px, 0);
    transform: translate3d(0, -50px, 0);
  }
  60% {
    opacity: 1;
    -webkit-transform: translate3d(0, 25px, 0);
    transform: translate3d(0, 25px, 0);
  }
  75% {
    -webkit-transform: translate3d(0, 0px, 0);
    transform: translate3d(0, 0px, 0);
  }
  90% {
    -webkit-transform: translate3d(0, 2px, 0);
    transform: translate3d(0, 2px, 0);
  }
  100% {
    -webkit-transform: none;
    transform: none;
  }
}
.bounceInDown {
  -webkit-animation-timing-function: ease-in;
  animation-timing-function: ease-in;
  -webkit-animation-name: bounceInDown;
  animation-name: bounceInDown;
  -webkit-animation-duration: .6s;
  animation-duration: .6s;
}
@-webkit-keyframes fly {
  0% {
    -webkit-transform: translate3d(0, 0, 0);
    transform: translate3d(0, 0, 0);
  }
  50% {
    -webkit-transform: translate3d(80, 0, 0);
    transform: translate3d(80px, 0, 0);
  }
  100% {
    -webkit-transform: translate3d(0, 0, 0);
    transform: translate3d(0, 0, 0);
  }
}
.fly {
  -webkit-animation-name: fly;
  animation-name: fly;
  -webkit-animation-duration: 30s;
  animation-duration: 30s;
  -webkit-animation-timing-function: linear;
  animation-timing-function: linear;
}
@-webkit-keyframes flySlowly {
  0% {
    -webkit-transform: translate3d(0, 0, 0);
    transform: translate3d(0, 0, 0);
  }
  50% {
    -webkit-transform: translate3d(-30px, 0, 0);
    transform: translate3d(-30px, 0, 0);
  }
  100% {
    -webkit-transform: translate3d(0, 0, 0);
    transform: translate3d(0, 0, 0);
  }
}
.flySlowly {
  -webkit-animation-name: flySlowly;
  animation-name: flySlowly;
  -webkit-animation-duration: 15s;
  animation-duration: 15s;
  -webkit-animation-timing-function: linear;
  animation-timing-function: linear;
}
@-webkit-keyframes jump {
  0% {
    -webkit-transform: translate3d(0, 0, 0);
    transform: translate3d(0, 0, 0);
  }
  50% {
    -webkit-transform: translate3d(0, -3px, 0);
    transform: translate3d(0, -3px, 0);
  }
  100% {
    -webkit-transform: translate3d(0, 0, 0);
    transform: translate3d(0, 0, 0);
  }
}
.jump {
  -webkit-animation-name: jump;
  animation-name: jump;
  -webkit-animation-duration: 1s;
  animation-duration: 1s;
  -webkit-animation-timing-function: ese-out;
  animation-timing-function: ese-out;
}
.jumpSlowly {
  -webkit-animation-name: jump;
  animation-name: jump;
  -webkit-animation-duration: 2s;
  animation-duration: 2s;
  -webkit-animation-timing-function: ese-out;
  animation-timing-function: ese-out;
}
@-webkit-keyframes bounceInUp {
  0%,
  60%,
  75%,
  90%,
  100% {
    -webkit-transition-timing-function: cubic-bezier(0.215, 0.61, 0.355, 1);
    transition-timing-function: cubic-bezier(0.215, 0.61, 0.355, 1);
  }
  0% {
    opacity: 0;
    -webkit-transform: translate3d(0, 10px, 0);
    transform: translate3d(0, 10px, 0);
  }
  60% {
    opacity: 1;
    -webkit-transform: translate3d(0, -5px, 0);
    transform: translate3d(0, -5px, 0);
  }
  75% {
    -webkit-transform: translate3d(0, 5px, 0);
    transform: translate3d(0, 5px, 0);
  }
  90% {
    -webkit-transform: translate3d(0, -5px, 0);
    transform: translate3d(0, -5px, 0);
  }
  100% {
    -webkit-transform: translate3d(0, 0, 0);
    transform: translate3d(0, 0, 0);
  }
}
.bounceInUp {
  -webkit-animation-name: bounceInUp;
  animation-name: bounceInUp;
}
@-webkit-keyframes rise {
  100% {
    height: 0px;
  }
}
.rise {
  -webkit-animation-name: rise;
  animation-name: rise;
}
@-webkit-keyframes shine {
  0% {
    opacity: .2;
  }
  50% {
    opacity: .8;
  }
  60% {
    opacity: .3;
  }
  65% {
    opacity: .5;
  }
  70% {
    opacity: .4;
  }
  65% {
    opacity: .8;
  }
  100% {
    opacity: 1;
  }
}
.shine {
  -webkit-animation-name: shine;
  animation-name: shine;
  -webkit-animation-duration: 3s;
  animation-duration: 3s;
}
@-webkit-keyframes fadeInUpSlowly {
  0% {
    opacity: 0;
    -webkit-transform: translate3d(0, 20%, 0);
    transform: translate3d(0, 20%, 0);
  }
  100% {
    opacity: 1;
    -webkit-transform: none;
    transform: none;
  }
}
.fadeInUpSlowly {
  -webkit-animation-name: fadeInUpSlowly;
  animation-name: fadeInUpSlowly;
  -webkit-animation-duration: 1s;
  animation-duration: 1s;
  -webkit-animation-fill-mode: both;
  animation-fill-mode: both;
}
@-webkit-keyframes fadeInRight {
  0% {
    opacity: 0;
    -webkit-transform: translate3d(5%, 0, 0);
    transform: translate3d(5%, 0, 0);
  }
  100% {
    opacity: 1;
    -webkit-transform: none;
    transform: none;
  }
}
.fadeInRight {
  -webkit-animation-name: fadeInRight;
  animation-name: fadeInRight;
  -webkit-animation-duration: .7s;
  animation-duration: .7s;
  -webkit-animation-fill-mode: both;
  animation-fill-mode: both;
}
@-webkit-keyframes rubberBand {
  0% {
    -webkit-transform: scale3d(1, 1, 1);
    transform: scale3d(1, 1, 1);
  }
  30% {
    -webkit-transform: scale3d(1.25, 0.75, 1);
    transform: scale3d(1.25, 0.75, 1);
  }
  40% {
    -webkit-transform: scale3d(0.75, 1.25, 1);
    transform: scale3d(0.75, 1.25, 1);
  }
  50% {
    -webkit-transform: scale3d(1.15, 0.85, 1);
    transform: scale3d(1.15, 0.85, 1);
  }
  65% {
    -webkit-transform: scale3d(0.95, 1.05, 1);
    transform: scale3d(0.95, 1.05, 1);
  }
  75% {
    -webkit-transform: scale3d(1.05, 0.95, 1);
    transform: scale3d(1.05, 0.95, 1);
  }
  100% {
    -webkit-transform: scale3d(1, 1, 1);
    transform: scale3d(1, 1, 1);
  }
}
.rubberBand {
  -webkit-animation-name: rubberBand;
  animation-name: rubberBand;
}
@-webkit-keyframes colorShine {
  0% {
    background-color: #a993d8;
  }
  50% {
    background-color: #957ccc;
  }
  100% {
    background-color: #a993d8;
  }
}
.colorShine {
  padding-left: 15px;
  padding-right: 15px;
  -webkit-animation-name: colorShine;
  animation-name: colorShine;
  -webkit-animation-duration: 2s;
  animation-duration: 2s;
  -webkit-animation-fill-mode: both;
  animation-fill-mode: both;
}
@-webkit-keyframes roll {
  0% {
    margin-left: 20px;
    -webkit-transform: rotate(360deg);
    transform: rotate(360deg);
  }
  100% {
    margin-left: 10px;
    -webkit-transform: rotate(0deg);
    transform: rotate(0deg);
  }
}
.roll {
  -webkit-animation-name: roll;
  animation-name: roll;
  -webkit-animation-duration: 1.5s;
  animation-duration: 1.5s;
  -webkit-animation-fill-mode: both;
  animation-fill-mode: both;
}
body,
html {
  height: 100%;
  background-color: #333;
}
body.index {
  background: url("/images/activity/survey/index_bg.jpg") no-repeat #342a2a;
  background-size: 100%;
}
body.index section.content {
  position: absolute;
  bottom: 25px;
  margin: 0 10px;
  padding: 10px;
  background-color: rgba(29, 24, 24, 0.6);
  color: #fff;
}
.bt,
input[type=submit].bt {
  display: block;
  background-color: #d62d32;
  padding: 10px 0;
  text-align: center;
  font-size: 20px;
  font-weight: bold;
  color: #fff;
  border: 0;
  width: 100%;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  -ms-box-sizing: border-box;
  box-sizing: border-box;
  border-radius: 5px;
}
.ipt {
  border: 1px solid #999;
  padding: 10px 10px;
  font-size: 20px;
  display: block;
  width: 100%;
  border-radius: 5px;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  -ms-box-sizing: border-box;
  box-sizing: border-box;
}
section.title {
  position: fixed;
  top: 0;
  text-align: center;
  color: #fff;
  font-size: 16px;
  width: 100%;
}
section.title .p2 em {
  font-size: 20px;
  font-weight: bold;
}
section.title ul li {
  display: none;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  -ms-box-sizing: border-box;
  box-sizing: border-box;
  padding-top: 10px;
  padding-bottom: 10px;
}
section.title ul li.on {
  display: block;
}
section.title ul li:nth-of-type(1) {
  background-color: #ff7167;
}
section.title ul li:nth-of-type(2) {
  background-color: #fc9151;
}
section.title ul li:nth-of-type(3) {
  background-color: #9dcf7a;
}
section.title ul li:nth-of-type(4) {
  background-color: #f0b13b;
}
section.title ul li:nth-of-type(5) {
  background-color: #84dbd7;
}
section.title ul li:nth-of-type(6) {
  width: 100%;
  background-color: #7fa9ef;
}
.page {
  padding: 10px 0;
  width: 100%;
}
.page div.title {
  position: relative;
  left: 50%;
  margin-left: -6em;
  padding-bottom: 10px;
  text-align: left;
  color: #fff;
  font-size: 16px;
  display: inline-block;
}
.page div.title .p2 em {
  font-size: 20px;
  font-weight: bold;
}
.page ul {
  background-color: #fff;
  margin: 0 10px 0;
  padding: 0 10px;
  border-radius: 5px;
}
.page ul li {
  padding: 10px 0;
  border-bottom: 1px solid #d1d1d1;
}
.page ul li i {
  float: left;
  width: 13px;
  height: 13px;
  line-height: 13px;
  display: inline-block;
  margin-right: 10px;
  margin-top: 2px;
  border: 1px solid #c4c4c4;
  background-color: #f4f4f4;
  border-radius: 50%;
  text-align: center;
}
.page ul li i span.icon_ok {
  display: none;
}
.page ul li input[type=checkbox] {
  display: none;
}
.page ul li.on i {
  background-color: #db3d3d;
  border: 1px solid #db3d3d;
}
.page ul li.on i span.icon_ok {
  display: inline-block;
  background: url("/images/activity/survey/ok.png") no-repeat;
  background-size: 9px;
  width: 9px;
  height: 6px;
}
.page ul li:last-of-type {
  border-bottom: 0;
}
.page:nth-of-type(1) {
  background-color: #ff7167;
}
.page:nth-of-type(2) {
  background-color: #fc9151;
}
.page:nth-of-type(3) {
  background-color: #9dcf7a;
}
.page:nth-of-type(4) {
  background-color: #f0b13b;
}
.page:nth-of-type(5) {
  background-color: #84dbd7;
}
.page:nth-of-type(6) {
  width: 100%;
  background-color: #7fa9ef;
}
.bottom_set {
  padding: 15px 10px;
  background-color: #fff;
}
.bt_fixed {
  display: none;
  position: fixed;
  top: 50%;
  right: 0;
  margin-top: -60px;
  display: inline-block;
  padding: 10px 10px;
  width: 3em;
  background-color: rgba(0, 0, 0, 0.4);
  border-radius: 50%;
  color: #fff;
  text-align: center;
}
i.green_ok {
  display: inline-block;
  width: 22px;
  height: 17px;
  background: url("/images/activity/survey/green_ok.png") no-repeat;
  background-size: 22px;
  vertical-align: middle;
}
i.arrow_up {
  position: absolute;
  top: -82px;
  left: 50%;
  display: inline-block;
  width: 63px;
  height: 72px;
  background: url("/images/activity/survey/arrow_up.png") no-repeat;
  background-size: 63px;
}
.slide_up {
  position: absolute;
  bottom: 5px;
  left: 50%;
  margin-left: -10px;
  z-index: 20;
}
.slide_up i {
  display: inline-block;
  width: 20px;
  height: 15px;
  background: url("/images/activity/survey/up.png") no-repeat;
  background-size: 20px;
  -webkit-animation: arrowsUp 1s infinite linear;
}
@-webkit-keyframes arrowsUp {
  0% {
    -webkit-transform: translate(0, 0);
  }
  50% {
    -webkit-transform: translate(0, -5px);
  }
  100% {
    -webkit-transform: translate(0, 0);
  }
}
