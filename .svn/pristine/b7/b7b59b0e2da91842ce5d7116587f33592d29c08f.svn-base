dl.c_s_result {
  padding-top: 22px;
  zoom: 1;
}
dl.c_s_result dt.anonymous_portrait {
  float: left;
  width: 57px;
  height: 57px;
}
dl.c_s_result dt.anonymous_portrait img {
  width: 57px;
  height: 57px;
}
dl.c_s_result dd.c_s_result_detail {
  margin-left: 75px;
}
dl.c_s_result dd.c_s_result_detail h2 {
  padding-top: 5px;
  padding-bottom: 4px;
  font-size: 16px;
  font-weight: normal;
}
dl.c_s_result dd.c_s_result_detail h2 span {
  font-weight: bold;
}
dl.c_s_result dd.c_s_result_detail p.view_address {
  padding-bottom: 20px;
}
dl.c_s_result dd.c_s_result_detail p.view_address i.cmp_address {
  margin-right: 5px;
  vertical-align: -2px;
}
dl.c_s_result dd.c_s_result_detail .c_s_result_text {
  line-height: 2em;
  color: #666666;
}
dl.c_s_result dd.c_s_result_detail .c_s_result_text em {
  color: #333333;
}
dl.c_s_result dd.c_s_result_detail dt {
  float: left;
  width: 55px;
  color: #333333;
  font-weight: bold;
}
dl.c_s_result dd.c_s_result_detail dd {
  margin-left: 55px;
}
dl.c_s_result dd.c_s_result_detail div.u_c_s {
  padding: 15px 0;
}
div.c_s_v_result {
  padding: 8px 0 10px 20px;
  margin-top: 22px;
  background-color: #f6f6f6;
  position: relative;
}
div.c_s_v_result em {
  margin-right: 45px;
}
div.c_s_v_result span.green_s_star_2 {
  vertical-align: -2px;
}
div.c_s_v_result i.i_rect {
  display: inline-block;
  width: 12px;
  height: 12px;
  border-radius: 2px;
  vertical-align: middle;
}
div.c_s_v_result i.i_rect.color_g {
  background-color: #7cb228;
}
div.c_s_v_result i.i_rect.color_r {
  background-color: #e85253;
}
div.c_s_v_result i.i_rect.color_y {
  background-color: #ffcc00;
}
div.c_s_v_result i.tangle_init {
  display: inline-block;
  border-width: 5px 5px 0 5px;
  border-style: dashed solid;
  border-color: #999999 transparent transparent transparent;
  margin: 0 5px;
  vertical-align: 3px;
  -webkit-transition: all ease 0.2s;
  -moz-transition: all ease 0.2s;
  -ms-transition: all ease 0.2s;
  -o-transition: all ease 0.2s;
  transition: all ease 0.2s;
}
div.c_s_v_result i.fronts_a_hover {
  -webkit-transform: rotate(180deg);
  -moz-transform: rotate(180deg);
  -ms-transform: rotate(180deg);
  -o-transform: rotate(180deg);
  transform: rotate(180deg);
}
div.c_s_v_result div.grade_detail {
  top: 50px;
  left: -27px;
}
div.nologin {
  margin-top: 36px;
}
div.i_filterblur {
  height: 215px;
  background: url("/images/view_v2-img/view_info.jpg") no-repeat left center;
  padding-top: 1px;
  margin-left: -10px;
  margin-bottom: -15px;
}
div.send_info {
  width: 260px;
  margin: 0 auto;
  padding: 20px 0 20px 120px;
  text-align: center;
  border: 1px solid #cfcfcf;
  background: #ffffff url("/images/ugc_interrupt.jpg") no-repeat 15px bottom;
}
div.send_info p.tt {
  padding-bottom: 5px;
}
div.grade_detail {
  display: none;
  border: 1px solid #e2e2e2;
  width: 140px;
  padding: 15px 15px 20px;
  background-color: #fff;
  position: absolute;
}
div.grade_detail ul li {
  margin-top: 5px;
}
div.grade_detail .tangle_greyb_up {
  position: absolute;
  top: -7px;
  right: 25px;
}
div.login_register {
  width: 350px;
  margin: -6px auto 0 auto;
  padding: 20px 0 0 0;
  background-color: #fff;
  border: 1px solid #cfcfcf;
}
div.login_register h5 {
  font-weight: normal;
  font-size: 14px;
  text-align: center;
}
div.login_register ul {
  padding-left: 16px;
}
div.login_register li.l_other {
  height: 90px;
  margin: 15px 0 5px 0;
  background: url("/images/ugc_interuptline.jpg") no-repeat right center;
}
div.login_register li.l_other a {
  display: inline-block;
  position: relative;
  width: 46px;
  font-size: 0;
  height: 46px;
  border-radius: 30px;
  background-color: #f2f2f2;
  text-align: center;
  margin-right: 10px;
  -webkit-transition: top 0.3s ease;
  -moz-transition: top 0.3s ease;
  -ms-transition: top 0.3s ease;
  -o-transition: top 0.3s ease;
  transition: top 0.3s ease;
}
div.login_register li.l_other a i {
  display: inline-block;
  height: 23px;
  margin-top: 11px;
  background-image: url("/images/auth.png");
  background-repeat: no-repeat;
}
div.login_register li.l_other a i.qq {
  background-position: -63px 0;
  width: 22px;
}
div.login_register li.l_other a i.rr {
  background-position: -87px 0;
  width: 23px;
}
div.login_register li.l_other a i.wb {
  background-position: -114px 0;
  width: 29px;
}
div.login_register li.l_other a:hover {
  top: -2px;
}
div.login_register li.options {
  padding-top: 45px;
}
div.bind_email {
  width: 408px;
  margin: 0 auto;
  padding: 20px 0;
  text-align: center;
  border: 1px solid #cfcfcf;
  background-color: #fff;
}
div.bind_email h3 {
  font-size: 22px;
  font-weight: normal;
}
.auth_btn {
  display: inline-block;
  text-align: center;
  text-decoration: none;
  width: 322px;
  height: 42px;
  border: 0;
  background-color: #7cb228;
  color: #fff;
  line-height: 42px;
  font-size: 20px;
  cursor: pointer;
  border-radius: 2px;
  -webkit-transition: background-color 0.2s ease;
  -moz-transition: background-color 0.2s ease;
  -ms-transition: background-color 0.2s ease;
  -o-transition: background-color 0.2s ease;
  transition: background-color 0.2s ease;
}
.auth_btn:hover {
  background-color: #96ca46;
  text-decoration: none;
}
