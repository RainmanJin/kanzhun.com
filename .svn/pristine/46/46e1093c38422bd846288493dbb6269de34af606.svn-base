}
ul.job_edit li label em {
  color: #d60000;
}
ul.job_edit li div.ipt_wrap {
  margin-left: 66px;
}
ul.job_edit li div.ipt_wrap p.center_line,
ul.job_edit li div.ipt_wrap em {
