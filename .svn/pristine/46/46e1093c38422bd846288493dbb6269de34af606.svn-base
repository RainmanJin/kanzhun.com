}
table.tb_resume {
  table-layout: fixed;
  border: 1px solid #ecebeb;
  border-width: 0 1px 1px 1px;
}
table.tb_resume tr.tb_tt {
  background-color: #f0f7e5;
}
table.tb_resume tr.tb_tt td {
  height: 35px;
}
table.tb_resume tr.tb_tt td p {
  text-overflow: ellipsis;
  -o-text-overflow: ellipsis;
  -ms-text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  width: 100%;
  word-wrap: normal;
  width: 95%;
  margin: 0 auto;
}
table.tb_resume tr.tb_tt td p.cmp_name {
  padding-left: 10px;
}
table.tb_resume tr.tb_tt a.btn_detail {
  float: right;
  width: 78px;
  height: 24px;
  line-height: 24px;
  font-size: 14px;
}
table.tb_resume tr.e_detail {
  overflow: hidden;
  *zoom: 1;
}
table.tb_resume tr.e_detail td {
  position: relative;
  padding: 20px 0 10px 0;
}
table.tb_resume tr.e_detail td a.btn_yellow_b {
  position: absolute;
  right: 10px;
  top: 50%;
  margin-top: -13px;
  height: 26px;
  line-height: 26px;
  font-size: 14px;
}
table.tb_resume tr.e_detail ul {
  /*&.educations {
          width: 335px;
          padding: 0 30px 0 15px;
        }*/
}
table.tb_resume tr.e_detail ul li {
  height: 20px;
  line-height: 20px;
  margin-bottom: 10px;
  text-align: left;
  text-overflow: ellipsis;
  -o-text-overflow: ellipsis;
  -ms-text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  width: 100%;
  word-wrap: normal;
}
table.tb_resume tr.e_detail ul li em {
  color: #d5d5d5;
  margin: 0 5px;
  vertical-align: 1px;
}
table.tb_resume tr.e_detail ul.experiences {
  padding-right: 130px;
  padding-left: 10px;
}
table.tb_resume th {
  height: 40px;
  background-color: #7cb228;
  color: #fff;
  font-weight: normal;
  font-size: 14px;
}
table.tb_resume td {
  padding: 0;
  margin: 0;
  text-align: center;
  /*&.interval {
      border-bottom: 1px solid #ecebeb;
      border-top: 1px solid #ecebeb;
    }*/
  /*&.paging {
      padding-left: 15px;
      border-top: 1px solid #ecebeb;
    }*/
}
table.tb_resume td.t_left {
  text-align: left;
}
table.tb_resume td.noresult i {
  vertical-align: -11px;
}
table.tb_hadsee {
