.shine {
  -webkit-animation: twinkle 0.6s ease none;
  -moz-animation: twinkle 0.6s ease none;
  -ms-animation: twinkle 0.6s ease none;
  -o-animation: twinkle 0.6s ease none;
  animation: twinkle 0.6s ease none;
}
.angle_shine {
  -webkit-animation: angle_twinkle 0.6s ease none;
  -moz-animation: angle_twinkle 0.6s ease none;
  -ms-animation: angle_twinkle 0.6s ease none;
  -o-animation: angle_twinkle 0.6s ease none;
  animation: angle_twinkle 0.6s ease none;
}
