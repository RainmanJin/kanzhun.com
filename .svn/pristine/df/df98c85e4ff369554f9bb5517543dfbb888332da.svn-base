body {
  font: normal 14px/1.5 Arial, STHeiti, Microsoft YaHei, simsun, Helvetica, sans-serif;
  color: #333333;
  background-color: #f6f6f6;
}
section.manageContent {
  width: 950px;
  margin: 0px auto 40px;
  padding: 20px;
  background-color: #ffffff;
  min-height: 351px;
  overflow: hidden;
}
section.manageContent .manageLeft {
  width: 204px;
  float: left;
}
section.manageContent .manageRight {
  float: left;
  width: 705px;
  margin-left: 20px;
  padding-left: 20px;
  border-left: 1px solid #e7e7e7;
  min-height: 600px;
}
div.container {
  width: 990px;
  background-color: #fff;
  margin: 0 auto;
  padding-bottom: 30px;
  margin-top: 15px;
  min-height: 1100px;
  _height: 1100px;
}
div.findpws {
  padding: 20px;
}
section.l_con {
  width: 645px;
  float: left;
  padding: 0 20px 20px 20px;
  margin-top: 20px;
  border-right: 1px solid #e7e7e7;
  min-height: 1100px;
  _height: 1100px;
}
section.r_con {
  width: 264px;
  padding: 20px;
  float: right;
}
input,
textarea:-webkit-input-placeholder {
  color: #999999;
}
input,
textarea:-moz-placeholder {
  color: #999999;
}
input,
textarea:-ms-input-placeholder {
  color: #999999;
}
input.onholder {
  color: #999999;
}
span.onholder {
  color: #999999;
  position: absolute;
  z-index: 8;
}
input,
textarea {
  font: normal 14px/1.5 Arial, STHeiti, Microsoft YaHei, simsun, Helvetica, sans-serif;
}
a {
  color: #2c84cc;
  outline-style: none;
}
a.a_grey {
  color: #999999;
}
a.a_green {
  color: #7cb228;
}
a.a_underline {
  text-decoration: underline;
}
a.links:hover {
  color: #2472b4;
  text-decoration: none;
}
iframe.overlay {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  z-index: 999;
  background-color: transparent;
}
