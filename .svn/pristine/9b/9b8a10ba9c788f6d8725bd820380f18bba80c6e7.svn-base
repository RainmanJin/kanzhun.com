@import "common/var";
@import "module/reset";
@import "common/common";
@import "module/idangerous-swiper";
@import "module/animate";

html{
  font-size: 14px;
}


body{
  background: #00cdae url("@{img-path}/bg.jpg") no-repeat;
  .background-size(100%);
}
.page{
  position: relative;
}

div.wrap{
  width: 85.3125%;
  margin-left: 7.34375%;
}

header{
  img{
    display: block;
    margin: 1rem auto;
    width: 100%;
  }
}

span.middle-helper{
  display: inline-block;
  width: 1px;
  height: 100%;
  margin-right: -1px;
  vertical-align: middle;
}

.middle-content{
  display: inline-block;
  vertical-align: middle;
  width: 100%;
}

.page>.middle-content{
  margin-top: -10%;
}

section.white-box{
  position: relative;
  img.box-bg{
    display: block;
    width: 100%;
    margin: 0 auto;
  }

  .box-con{
    width: 100%;
    position: absolute;
    height: 100%;
    left: 0;
    top: 0;
  }
}

#swiperWrapper{
  .effect{
    display: none;
  }
}



.s1{
  white-space: nowrap;
  .p1-icon{
    width: 100%;
    text-align: center;

    >div{
      display: inline-block;
      margin: 1.5rem 0 0;
      text-align: center;
      font-size: 1rem;
      line-height: 1.2em;
      color: #ffffff;

      &:nth-of-type(1){
        margin-right: 2rem;
      }

      img{
        width: 65px;
      }
    }
  }
}
.page1{
  font-size: 1.142rem;
  color: #ffffff;
  padding-left: 2rem;
  .box-sizing();
  margin-top: 3rem;

  li{
    height: 2rem;
    line-height: 2rem;

    em{
      color: #00ffc9;
      font-size: 1.5rem;
      display: inline-block;
    }
  }
}


.s2{
  .p2-tt{
    position: absolute;
    left: 50%;
    width: 46px;
    margin-left: -28px;
    top: -20px;
  }
}

.s3{
  span{
    display: inline-block;
    font-size: 0;
    &.p3-1{
      position: absolute;
      right: 20%;
      top: 12%;
    }

    &.p3-2{
      position: absolute;
      left: 25%;
      top: 42%;
    }

    &.p3-3{
      position: absolute;
      right: 15%;
      bottom: 8%;
    }
  }
}


.s4{
  .page4{
    img {
      width: 100%;
      display: block;
    }

    li{
      position: absolute;

      &:nth-of-type(1){
        top: 15%;
      }
      &:nth-of-type(2){
        top: 30%;
      }
      &:nth-of-type(3){
        top: 45%;
      }
      &:nth-of-type(4){
        top: 60%;
      }
      &:nth-of-type(5){
        top: 80%;
      }
    }
  }
}

.s5{
  .page5{
    img {
      width: 100%;
      display: block;
    }

    li{
      position: absolute;

      &:nth-of-type(1){
        top: 15%;
        width: 60%;
      }
      &:nth-of-type(2){
        top: 30%;
      }
      &:nth-of-type(3){
        top: 50%;
      }
      &:nth-of-type(4){
        top: 75%;
      }
    }
  }
}

.s6{
  .page6{
    img {
      display: block;
    }

    li{
      position: absolute;

      &:nth-of-type(1){
        top: 18%;
        left: 12%;
      }
      &:nth-of-type(2){
        top: 50%;
        left: 16%;
      }
      &:nth-of-type(3){
        top: 35%;
        right: 12%;
      }
      &:nth-of-type(4){
        top: 80%;
      }
    }
  }

  .middle-content{
    background: url("@{img-path}/p6-1.png") no-repeat 0 0;
    .background-size(cover);
    img{
      display: block;
    }
  }
}
.hidden{
  visibility: hidden;
}

.page7{
  color: #ffffff;

  li{
    &:nth-of-type(1){
      width: 80%;
      margin-top: 10%;
      margin-left: 10%;
      font-size: 1.5rem;
      text-align: center;
      line-height: 1.2em;

      strong{
        color: #00ffc9;
        font-weight: normal;
        font-size: 2rem;
        display: inline-block;
      }

      p{
        margin-top: 1rem;
        font-size: 1rem;
      }
    }

    &.times{
      color: #00ffc9;
      text-align: center;

      em{
        background-color: #15b3b0;
        display: inline-block;
        padding: 10px 5px;
        margin: 0 1px;
        border-radius: 3px;
        border: 1px solid #00ffc9;
//        .box-shadow(inset 0 0 2px #00ffc9);
        font-size: 0;

        b{
          background-image: url("@{img-path}/numbers.png");
          background-repeat: no-repeat;
          .background-size(180px);
          display: inline-block;
          width: 18px;
          height: 22.5px;
          vertical-align: -5px;

          &.zero{
            background-position: 1px 0;
          }
          &.one{
            background-position: -18px 0;
          }
          &.two{
            background-position: -36px 0;
          }
          &.three{
            background-position: -54px 0;
          }
          &.four{
            background-position: -72px 0;
          }
          &.five{
            background-position: -90px 0;
          }
          &.six{
            background-position: -108px 0;
          }
          &.seven{
            background-position: -126px 0;
          }
          &.eight{
            background-position: -144px 0;
          }
          &.night{
            background-position: -162px 0;
          }
        }
      }
    }

    &:nth-of-type(3){
      padding: 10% 0 0;
      a{
        background-color: rgba(0, 255, 201, .2);
        border: 1px solid #00ffc9;
        border-radius: 5px;
        text-align: center;
        display: block;
        width: 50%;
        margin-left: 25%;
        padding: .4rem 0;
        color: #00ffc9;
        font-size: 1.8rem;
      }
    }

    &:nth-of-type(4){
      padding: 5% 0 0;
      a{
        background-color: rgba(255,255,255,.2);
        border: 1px solid #fff;
        border-radius: 5px;
        text-align: center;
        display: block;
        width: 40%;
        margin-left: 30%;
        padding: .3rem 0;
        color: #fff;
        font-size: 1.2rem;
      }
    }
  }
}




//
div.single{
  display: none;
  color: #00ffc9;
  font-size: 1.4rem;
  padding: 8%;
  line-height: 1.8em;
  .box-sizing();

  .single-tt{

    span{
      height: 64px;
      width: 8em;
      white-space: nowrap;
      line-height: 64px;
      overflow: hidden;
    }

    img{
      width: 60px;
      height: 60px;
      border: 2px solid #ffffff;
      float: right;
    }
  }

  p.w{
    color: #ffffff;
    font-size: 15px;
  }

  .p1-icon{
    margin: 0 -5%;
    text-align: center;

    >div{
      display: inline-block;
      margin: 1rem 0 ;
      width: 50%;
      text-align: center;
      font-size: 0.8rem;
      line-height: 2em;
      color: #ffffff;

      img{
        width: 65px;
      }
    }
  }
}


//ip4s
@media (device-width: 320px) and (-webkit-min-device-pixel-ratio:2) {
  .page7 {
    li.times{
      em{
        padding: 8px 3px;
      }
    }
  }


    div.single{
      font-size: 1.2rem;

      .p1-icon > div img{
        width: 60px;
        height: 60px;
      }
    }
}


@media (min-width: 330px){
  .page7 li.times{
    margin-top: 5%;
  }
  .page7 li:nth-of-type(3){
    padding-top: 15%;
  }

  .s1 .p1-icon{
    margin-top: 10%;
  }
}

@media screen and (max-height: 480px){
  html{
    font-size: 12px;
  }

  div.wrap{
    width: 80%;
    margin-left: 10%;
  }

  div.single{

  }
}


/*
#page1,#page2, #page3, #page4, #page5, #page6{
  display: none;
}*/


#loading{
  position: absolute;
  z-index: 10;
  padding: 20px;
  left: 50%;
  top: 50%;
  margin: -22px 0 0 -22px;
  border-radius: 5px;
  background-color: rgba(0,0,0,.5);

  img{
    display: block;

  }
}

.slide_up{
  position: absolute;
  bottom: .5rem;
  left: 50%;
  margin-left: -15px;
  i{
    display: inline-block;
    width: 30px;
    height: 30px;
    background: url("@{img-path}/slider.png") no-repeat;
    .background-size(100%);
    -webkit-animation: arrowsUp 1s infinite linear;
  }
}
@-webkit-keyframes arrowsUp{
  0%{
    -webkit-transform: translate(0,0);
  }
  50%{
    -webkit-transform: translate(0,-5px);
  }
  100%{
    -webkit-transform: translate(0,0);
  }
}


//报名流程
div.apply{
  height: 100%;
  position: relative;

  .middle-content{
    margin-top: -10%;
  }
}

.apply-progress{
  padding: 1.5rem 2rem;
  dt{
    overflow: hidden;
    text-align: center;

    &.pg{
      span{
        font-size: 1.5rem;
        text-align: center;
        height: 2rem;
        width: 2rem;
        line-height: 2rem;
        border: 1px solid #c5c4db;
        border-radius: 50%;
        color: #c5c4db;
        display: inline-block;

        &.current{
          border: 1px solid #00ffc9;
          color: #00ffc9;
        }
      }
      
      i{
        display: inline-block;
        width: 4rem;
        border-top: 1px solid #c5c4db;
        vertical-align: .5rem;
        height: 0;
        overflow: hidden;

        &.current{
          border-top: 1px solid #00ffc9;
        }
      }
    }
  }
}

.form-control{
  padding-top: .5rem;

  label{
    color: #fff;
    line-height: 2em;
  }

  input[type=text]{
    background-color: #fff;
    border: 0;
    border-radius: .4rem;
    padding: .4rem .5rem;
    .box-sizing();
    width: 100%;
    margin-bottom: 1rem;
  }

  .err{
    text-align: center;
    color: #ff8080;
    padding: .8rem 0;
  }

  a.next{
    display: inline-block;
    font-size: 1.2rem;
    color: #00ffc9;
    border: 2px solid #00ffc9;
    background-color: rgba(0,255,201,.1);
    border-radius: .5rem;
    text-align: center;
    padding: .3rem 3rem;
  }
}

.rule{
  position: absolute;
  bottom: 5px;
  text-align: center;
  width: 100%;
  color: #fff;
  font-size: 1.142rem;
}

.apply-end{
  text-align: center;
  color: #fff;
  font-size: 1.2rem;

  strong{
    font-weight: normal;
    font-size: 1.6rem;
  }

  p:nth-of-type(1){
    padding: 2rem 0 1rem;
  }

  p:nth-of-type(2){
    margin-bottom: 4rem;
  }
}

.apply-success{
  padding: 2rem 2rem 0;
  dt{
    color: #00ffc9;
    font-size: 1.8rem;
  }

  dd{
    font-size: 1.142rem;

    p:nth-of-type(1){
      color: #fff;
      
      em{
        color: #00ffc9;
      }
    }

    p:nth-of-type(2){
      color: #00ffc9;
      text-align: center;
    }
  }
}



//ip4s
@media (device-width: 320px) and (-webkit-min-device-pixel-ratio:2) {
  .form-control{
    label{
      line-height: 1.5em;
    }

    input[type=text]{
      margin-bottom: .5rem;
    }

    .err{
      padding: .5rem 0;
    }
  }

  .apply-success {
    padding: 1rem 2rem 0;
    dt {
      font-size: 1.5rem;
    }

    dd{
      font-size: 1rem;
    }
  }
}

@media screen and (max-height: 480px){
  .rule{
    display: none;
  }
}




//转发提示
.mask{
  position: fixed;
  z-index: 9;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, .8);
  display: none;
}
.transmitDialog{
  display: none;
  position: fixed;
  z-index: 10;
  top: 150px;
  min-height: 150px;
  left: 50%;
  margin-left: -128px;
  margin-top: -61px;
  background: url(http://m.kanzhun.com/images/oval_bg.png) no-repeat;
  background-size: 257px;
  width: 257px;
  i.arrows{
    display: inline-block;
    background: url(http://m.kanzhun.com/images/arrows.png) no-repeat;
    background-size: 63px;
    width: 63px;
    height: 65px;
    position: absolute;
    top: -70px;
    right: -5px;
    -webkit-animation: arrowsUp2 1s infinite linear;
  }
  p{
    margin-top: 30px;
    color: #fff;
    text-align: center;
    font-size: 15px;
  }
}
@-webkit-keyframes arrowsUp2{
  0%{
    -webkit-transform: translate(0,0);
  }
  50%{
    -webkit-transform: translate(10px,-10px);
  }
  100%{
    -webkit-transform: translate(0,0);
  }
}

















