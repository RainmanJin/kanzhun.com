@import "common/var";
@import "module/company_header";
@import "module/manageLeft";
@import "module/maskui-v2";
@import "module/webuploader";
@import "module/uploadPortrait_dialog";
@import "module/co_left_review_item";
@import "module/use_comment_share";
@import "module/paging-v2";

div {
  &.s_wrap_default {
    padding: 10px 13px 10px 13px;
    margin-bottom: 15px;
    background-color: #f6f6f6;
    border: 1px solid #e2e2e2;
    .box-shadow(0 1px 2px #f5f5f5);

    p {
      &.cmp_tt {
        padding-bottom: 5px;
      }

      &.ta_hint {
        padding-top: 5px;
      }
    }
  }

  &.no_bg {
    background-color: transparent;
    padding-bottom: 20px;
  }

  &.this_detail {
    padding-bottom: 10px;
    padding-top: 5px;
    margin-bottom: 15px;
    border-bottom: 1px solid #e5e5e5;

    span {
      font-weight: bold;
    }
  }
}

ul {
  &.P_picture {
    overflow: hidden;
    margin-right: -20px;

    li {
      float: left;
      width: 214px;
      height: 214px;
      border: 1px solid #e2e2e2;
      margin-right: 15px;
      margin-bottom: 15px;

      & > div {
        position: relative;
        img {
          width: 214px;
          height: 214px;
        }

        div.masking {
          position: absolute;
          top: 0;
          left: 0;
          z-index: 2;
          width: 214px;
          height: 214px;
          background-color: #000;
          opacity: .5;

          a {
            float: right;
            width: 20px;
            height: 20px;
            padding: 5px;
          }
        }

        a.add_p {
          display: block;
        }
        .uploading{
          img{
            width: 70px;
            height: 71px;
            margin-top: 80px;
          }
        }

        &:hover {
          div.masking {
            display: block;
          }
        }
      }
    }
  }

  //发展历程
  &.development {
    li {
      float: left;

      dl {
        float: left;
        //overflow: hidden;
        margin-right: 10px;
      }

      dl.s_other {
        width: 80px;
      }

      dl.s_year {
        width: 100px;
      }
      .err{
        display: none;
      }
    }
  }

  //媒体新闻
  &.sub_news {
    li {
      .err{
        display: none;
      }
    }
  }

  //基本信息
  &.abc_ipt {
    label {
      display: inline-block;
      width: 125px;
      margin-right: 10px;
      height: 35px;
      line-height: 35px;
      text-align: right;
    }

    li {
      overflow: hidden;
      margin-bottom: 20px;
      padding-left: 20px;
      .err{
        display: none;
      }
      &.logo {
        label {
          float: left;
          padding-top: 15px;
        }

        & > div {
          float: left;
          position: relative;
          width: 98px;
          height: 98px;
          border: 1px solid #e7e7e7;
          cursor: pointer;

          img {
            width: 98px;
            height: 98px;
          }
          div.bg {
            position: absolute;
            top: 0;
            left: 0;
            height: 98px;
            width: 98px;
            z-index: 2;
            background-color: #000;
            opacity: .6;
          }

          p {
            position: absolute;
            top: 0;
            left: 0;
            height: 98px;
            width: 98px;
            line-height: 98px;
            z-index: 3;
            color: #fff;
            text-align: center;
          }

          &:hover {
            div, p {
              display: block;
            }
          }
          &.uploading{
            text-align: center;
            display: none;
            img{
              width: 70px;
              height: 71px;
              margin-top: 15px;
            }
          }
        }
      }

      &.has_select {
        overflow: visible;

        label {
          float: left;
        }

        dl.select {
          width: 300px;
          float: left;
        }
      }

      &.sbm {
        padding-left: 154px;
        /*span {
          width: 130px;
          height: 42px;
          line-height: 42px;
          padding: 0;
        }*/

        /*input {
          height: 42px;
          line-height: 42px;
        }*/

      }

      &.has_border {
        padding-top: 20px;
        border-top: 1px dashed #d2d2d2;
      }
    }
  }

  //上传营业执照
  &.license_ipt {
    li {
      overflow: hidden;
      margin-bottom: 20px;
      .err{
        display: none;
      }
    }

    label {
      display: inline-block;
      height: 35px;
      line-height: 35px;
      width: 150px;
      margin-right: 10px;
      text-align: right;
    }

    .l_p {
      margin-bottom: 25px;

      label {
        float: left;
      }

      div {
        float: left;

        &.hear_upload {
          position: relative;
          margin-right: 50px;
          width: 207px;
          height: 300px;
          border-width: 1px;
          border-style: solid;
          border-color: #cccccc;

          p {
            padding: 95px 10px 102px 10px;
          }
          >div{
            height: 258px;
            width: 100%;
            text-align: center;
            img{
              display: inline;
            }
            .uploading {
              display: none;
              margin-top: 90px;
            }
          }
          a {
            position: absolute;
            bottom: 0;
            left: 0;
            width: 100%;
            height: 40px;
            line-height: 40px;
            color: #fff;
            text-align: center;
            background-color: @grey_99;

            &:hover {
              text-decoration: none;
            }
          }
        }

        &.sample {
          div {
            float: left;
            width: 207px;
            height: 300px;
            border: 1px solid #cccccc;

            img {
              width: 207px;
              height: 300px;
            }
          }

          span {
            float: left;
          }
        }
      }
      p.err{
        padding-left: 160px;
      }
    }

    .sbm {
      padding-left: 160px;

      span {
        width: 164px;
        height: 42px;
        line-height: 42px;
        padding: 0;

        input {
          height: 42px;
          line-height: 42px;
        }
      }
    }
  }
}

dl {
  &.news_list {
    overflow: hidden;

    dt {
      float: right;
      dd {
        margin-right: 50px;
      }
    }
  }

  &.CEO_info {
    overflow: hidden;

    dt {
      position: relative;
      float: left;
      width: 64px;
      height: 64px;
      padding: 2px;
      border: 1px solid #e7e7e7;
      cursor: pointer;

      img {
        width: 64px;
        height: 64px;
      }

      div {
        position: absolute;
        left: 0px;
        top: 0px;
        width: 68px;
        height: 68px;
        line-height: 68px;
        z-index: 2;
        background-color: #000;
        opacity: .6;
      }

      p {
        position: absolute;
        left: 0px;
        top: 0px;
        width: 68px;
        height: 68px;
        line-height: 68px;
        z-index: 3;
        color: #ffffff;
        text-align: center;
      }

      &:hover {
        & > div {
          display: block;
        }

        & > p {
          display: block;
        }
      }
    }

    dd {
      margin-left: 85px;
    }
  }
  &.development_list{
    dt{
      float: left;
      width: 90px;
    }
    dd{
      margin-left: 90px;
      margin-right: 40px;
      position: relative;
      a{
        position: absolute;
        top: 0;
        right: -40px;
      }
    }
  }
}

.green_v3 {
  color: #22ac38;
}

.license_show {
  width: 207px;
  height: 300px;
  border: 1px solid #cccccc;
  margin-left: 156px;

  img {
    width: 207px;
    height: 300px;
  }
}

.has_poptt {
  position: relative;

  div {
    width: 220px;
    left: -15px;
    top: 28px;
    z-index: 2;
    position: absolute;
    padding: 10px 28px 10px 15px;
    border: 1px solid #cccccc;
    border-radius: 5px;
    background-color: #fff;
    img {
      position: absolute;
      top: -8px;
      left: 35px;
      z-index: 3;
    }

    i {
      position: absolute;
      top: 8px;
      right: 12px;
      cursor: pointer;
    }

    p {
      color: @grey_99;
    }
  }
}

//公司管理评论页面
div.cmp_comment_wrap {
  border: 1px solid #dcdcdc;
  padding: 0 15px;
}

strong {
  &.ok {
    color: @green;
  }
  
  &.err {
    color: red;
  }
}

section.comment_pop {
  h3 {
    font-weight: normal;
    font-size: 20px;
    padding-bottom: 10px;
  }
  span.btn_grey_b {
    height: 31px;
    line-height: 31px;
  }
  input[type=submit] {
    height: 31px;
    line-height: 31px;
  }
}

div.promppt_info {
  padding: 15px 0;

  p {
    padding-left: 10px;
  }
}

//合并公司
h3.cmp_merge_tt {
  position: relative;
  margin-bottom: 30px;
  padding-left: 8px;
  border-left: 4px solid #7cb228;
  height: 1em;
  line-height: 1em;
  font-size: 16px;
  zoom: 1;

  a.btn_yellow_b {
    position: absolute;
    right: 0;
    top: -10px;
    width: 140px;
  }
}

//表格显示简历信息简略
table.cmp_merge_tb {
  table-layout: fixed;
  border-left: 1px solid #f3f3f3;
  margin-bottom: 10px;

  tr {
    height: 40px;
    border-bottom: 1px solid #f3f3f3;
    background-color: #ffffff;

    &:nth-child(odd) {
      background-color: #f6f6f6;
    }

    td,th {
      border-right: 1px solid #f3f3f3;
    }

    th {
      height: 40px;
      background-color: @green;
      color: #fff;
      font-weight: normal;
      font-size: 14px;
      border-right-color: #88b93d;

      &.status {
        border-right: none;
      }
    }

    td {
      padding: 0;
      margin: 0;
      text-align: center;
      height: 38px;

      p {
        .ellipsis(ellipsis);
        width: 95%;
        margin: 0 auto;
      }
    }
  }
}

//选择合并公司
p.cmp_merge_label {
  padding-bottom: 30px;
  label {
    width: 8em;
    margin-right: 10px;
  }
}

dl.search_merge_cmp {
  overflow: hidden;
  padding: 20px 0;
  border-bottom: 1px solid #e7e7e7;

  dt {
    float: left;
    width: 43px;
    padding-left: 15px;
    padding-top: 25px;
  }

  dd {
    margin-left: 58px;

    & > a {
      float: left;
      width: 65px;
      height: 65px;
      line-height: 65px;
      font-size: 0;
      overflow: hidden;
      background-color: #fff;
      border: 1px solid #e7e7e7;

      img {
        display: inline-block;
        vertical-align: middle;
        width: 65px;
      }

      span {
        display: inline-block;
        width: 0;
        height: 100%;
        vertical-align: middle;
      }
    }

    & > div {
      margin-left: 75px;
      padding-top: 8px;

      em {
        color: #e7e7e7;
        margin: 0 10px;
      }

      p {
        line-height: 1.8em;
      }
    }
  }
}

//点评管理
nav.view_tab {
  height: 42px;
  border-bottom: 1px solid #e7e7e7;
  line-height: 42px;

  p {
    height: 43px;
    margin-bottom: -1px;
    background: url(../images/v2/cmp_merge_line.png) no-repeat 133px bottom;
    .transition(all, ease .3s);
  }

  span {
    display: inline-block;
    padding: 0 10px;
    cursor: pointer;

    &.current {
      color: @grey_33;
    }
  }
}

ul.apply_cmp {
  position: relative;
  padding: 24px 15px;
  border: 1px solid #e7e7e7;

  a.cloase_btn {
    position: absolute;
    right: 5px;
    top: 5px;
    width: 20px;
    height: 20px;
  }

  li {
    padding-bottom: 20px;

    label {
      float: left;
      width: 7em;
      margin-right: 10px;

      span {
        display: inline-block;
        width: 16px;
        height: 16px;
        line-height: 16px;
        text-align: center;
        border: 1px solid @yellow;
        border-radius: 999px;
        color: @yellow;
      }

      &.lone {
        padding-top: 17px;
      }
    }

    div.cmp_info {
      float: left;

      & > a {
        float: left;
        width: 65px;
        height: 65px;
        line-height: 65px;
        font-size: 0;
        overflow: hidden;
        background-color: #fff;
        border: 1px solid #e7e7e7;

        img {
          display: inline-block;
          vertical-align: middle;
          width: 65px;
        }

        span {
          display: inline-block;
          width: 0;
          height: 100%;
          vertical-align: middle;
        }
      }

      & > div {
        margin-left: 75px;
        padding-top: 8px;

        em {
          color: #e7e7e7;
          margin: 0 10px;
        }

        p {
          line-height: 1.8em;
        }
      }
    }

    div.img_wrap {
      float: left;
      position: relative;
      width: 118px;
      height: 88px;
      border: 1px solid #e7e7e7;

      img {
        width: 118px;
        height: 88px;
      }

      i {
        position: absolute;
        top: -5px;
        right: -5px;
      }
    }

    &:last-child {
      padding-bottom: 0;
    }

    a.btn_grey_b {
      padding: 0 15px;
    }
  }
}