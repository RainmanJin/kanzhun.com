.w_990{width:990px}body{font:normal 12px/1.5 Arial,Microsoft Yahei,simsun;background-color:#fff;color:#343434}.f_blue{color:#007ead}.f_red{color:#f00}a{color:#343434}a.blue{color:#007ead}a.red{color:#f00}a.green{color:#7ab12d}.onholder{color:#666}header{border-bottom:6px solid #7ab12d;position:relative;height:70px;padding:15px 15px 0;min-width:990px}header .logo{float:left}header .u{float:right}header .u span{margin:0 10px;color:#cdcdcd}header .u a{color:#007ead}header nav{position:absolute;bottom:0;right:20px;overflow:hidden;padding-left:2px}header nav a{height:32px;line-height:32px;text-align:center;padding:0 25px;border:1px solid #9a9a9a;border-bottom:0;float:left;border-top-left-radius:5px;border-top-right-radius:5px;margin-left:-1px;background-color:#f5f5f5;background:linear-gradient(0deg, #f5f5f5 0, #fff 100%)}header nav a.current{color:#fff;background-color:#7ab12d;background:linear-gradient(0deg, #7ab12d 0, #8fcb32 100%);text-decoration:none;border-color:#70a61b}header nav a:hover{text-decoration:none}section.container{padding:15px}div.pager{padding:15px 0}div.pager a{height:22px;line-height:22px;padding:0 8px;display:inline-block;margin:0 5px;border:1px solid #ddd;color:#007ead}div.pager a.next{margin-right:20px}div.pager a.current{background-color:#7ab12d;text-align:center;line-height:22px;color:#fff;text-decoration:none}table.tb_grey{border-top:1px solid #9a9a9a;border-left:1px solid #9a9a9a}table.tb_grey th,table.tb_grey td{height:30px;vertical-align:middle;text-align:center;border-bottom:1px solid #9a9a9a;border-right:1px solid #9a9a9a}table.tb_grey th{background-color:#dedede}.new{width:16px;height:19px;background:url(../images/new.jpg) no-repeat;display:inline-block;vertical-align:middle;margin-left:5px}