html {
  height: 100%;
}
body {
  font: normal 14px/1.5 Arial, Helvetica, "Microsoft YaHei", STHeiti, "Droid Sans Fallback", sans-serif;
  color: #343434;
  height: 100%;
}
input,
button,
textarea,
select {
  font: normal 14px/1.5 Arial, Helvetica, "Microsoft YaHei", STHeiti, "Droid Sans Fallback", sans-serif;
}
a {
  color: #343434;
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
}
a.a_ul {
  text-decoration: underline;
}
h1,
h2,
h3,
h4,
h5,
h6 {
  font-size: 1em;
  line-height: 1;
}
body {
  width: 100%;
  height: 100%;
}
.slideout-menu {
  position: fixed;
  left: 0;
  top: 0;
  bottom: 0;
  right: 0;
  z-index: 0;
  overflow-y: auto;
  -webkit-overflow-scrolling: touch;
  display: none;
}
.slideout-panel {
  position: relative;
  z-index: 1;
}
.slideout-open,
.slideout-open body,
.slideout-open .slideout-panel {
  overflow: hidden;
}
.slideout-open .slideout-menu {
  display: block;
}
.icons {
  background-image: url("../images/icons.png");
  background-repeat: no-repeat;
  -webkit-background-size: 50px;
  background-size: 50px;
}
html {
  font-size: 14px;
}
.slide-menu {
  background-color: #1d1e26;
  width: 100%;
}
.wrapper {
  width: 100%;
}
.container {
  height: auto;
  width: 100%;
  min-height: 100%;
  background: #02458a url("../images/bg.jpg") no-repeat 0 45px;
  -webkit-background-size: 100%;
  background-size: 100%;
}
.slide-menu h2 {
  text-align: right;
  padding: 10px 58px 0 1rem;
  height: 45px;
  line-height: 45px;
  -webkit-box-sizing: border-box;
  -ms-box-sizing: border-box;
  box-sizing: border-box;
  border-bottom: 1px solid #444;
}
.slide-menu ul {
  background: url("../images/menu.png") no-repeat 1rem 0;
  -webkit-background-size: auto 100%;
  background-size: auto 100%;
}
.slide-menu li > a {
  margin-left: 62px;
  -webkit-box-sizing: border-box;
  -ms-box-sizing: border-box;
  box-sizing: border-box;
  border-bottom: 1px solid #555;
  display: block;
  color: #efefef;
  height: 60px;
  line-height: 60px;
}
.slide-menu .feedback {
  text-align: right;
  padding: 1rem 58px 1rem 1rem;
}
.slide-menu .feedback a {
  color: #efefef;
}
.slide-menu .feedback i {
  width: 27px;
  height: 20px;
  background-image: url("../images/icons.png");
  background-repeat: no-repeat;
  -webkit-background-size: 50px;
  background-size: 50px;
  display: inline-block;
  vertical-align: middle;
}
header.rank {
  overflow: hidden;
  background-color: #efefef;
  height: 45px;
  line-height: 45px;
  text-align: center;
  position: relative;
}
header.rank select {
  background-color: transparent;
  border: 0;
  padding: 0 20px;
  background: url("../images/select.png") no-repeat right 8px;
  -webkit-background-size: 13.5px;
  background-size: 13.5px;
}
section.search-float {
  display: none;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.6);
  z-index: 20;
  position: fixed;
  left: 0;
  top: 0;
}
section.search-float > div {
  width: 100%;
  position: absolute;
  height: 45px;
  background-color: rgba(0, 0, 0, 0.6);
  left: 0;
  top: 0;
  padding: 8px 10px 7px;
  -webkit-box-sizing: border-box;
  -ms-box-sizing: border-box;
  box-sizing: border-box;
}
section.search-float input[type=text] {
  width: 100%;
  background: #ffffff;
  display: block;
  height: 30px;
  line-height: 30px;
  border: 0;
  border-radius: 4px;
  -webkit-box-sizing: border-box;
  -ms-box-sizing: border-box;
  box-sizing: border-box;
  padding: 0 10px;
}
div.menu {
  padding: 12px 1rem 0;
  float: left;
  font-size: 0;
}
div.menu i {
  width: 20px;
  height: 18px;
  display: inline-block;
  background-image: url("../images/icons.png");
  background-repeat: no-repeat;
  -webkit-background-size: 50px;
  background-size: 50px;
  background-position: 0 -26px;
}
div.search {
  padding: 12px 1rem 0;
  float: right;
  font-size: 0;
}
div.search i {
  width: 18px;
  height: 18px;
  display: inline-block;
  background-image: url("../images/icons.png");
  background-repeat: no-repeat;
  -webkit-background-size: 50px;
  background-size: 50px;
  background-position: 0 -45px;
}
dl.rank {
  padding-bottom: 50px;
}
dl.rank dt {
  height: 60px;
  line-height: 60px;
  color: #ffffff;
  font-size: 16px;
  padding: 0 10px;
  -webkit-box-sizing: border-box;
  -ms-box-sizing: border-box;
  box-sizing: border-box;
  border-bottom: 1px solid #ddd;
  border-left: 5px solid transparent;
}
dl.rank dt img {
  width: 30px;
  height: 30px;
  vertical-align: middle;
  margin: 0 1rem 0 1rem;
}
dl.rank dt div {
  float: right;
  font-size: 0.857rem;
}
dl.rank dt div span {
  width: 1.5rem;
  height: 1.5rem;
  border-radius: 50%;
  display: inline-block;
  border: 1px solid #ffffff;
  font-size: 0;
  line-height: 1em;
  text-align: center;
  overflow: hidden;
  margin-left: .5rem;
  vertical-align: -0.35rem;
}
dl.rank dt div span i {
  margin: .3rem 0 0 0;
  width: .6rem;
  height: .6rem;
  display: inline-block;
  border-top: 1px solid #ffffff;
  border-right: 1px solid #ffffff;
  -webkit-transform: rotate(135deg);
  -ms-transform: rotate(135deg);
  transform: rotate(135deg);
}
dl.rank dt.active {
  border-bottom: 1px solid transparent;
  border-left: 5px solid #4b8ece;
}
dl.rank dt.active div span {
  -webkit-transform: rotate(180deg);
  -ms-transform: rotate(180deg);
  transform: rotate(180deg);
}
dl.rank dd {
  background-color: rgba(0, 0, 0, 0.6);
  padding: 10px 0 10px 0;
  font-size: 0.7rem;
  -webkit-box-sizing: border-box;
  -ms-box-sizing: border-box;
  box-sizing: border-box;
  position: relative;
}
dl.rank dd p:nth-of-type(1) {
  color: #efefef;
  padding: 0 10px 0 60px;
}
dl.rank dd p:nth-of-type(2) {
  color: #666666;
  text-align: center;
  margin: .5rem 0 1rem;
}
dl.rank dd i.flag {
  width: 30px;
  height: 32.5px;
  display: inline-block;
  position: absolute;
  background-image: url("../images/icons.png");
  background-repeat: no-repeat;
  -webkit-background-size: 50px;
  background-size: 50px;
  background-position: 0 -64px;
  top: 0px;
  left: 15px;
}
dl.rank dd .btn-orange {
  background-color: #e77200;
  border-radius: .3rem;
  color: #ffffff;
  text-align: center;
  font-size: 1.43rem;
  padding: .4rem 0;
  -webkit-box-sizing: border-box;
  -ms-box-sizing: border-box;
  box-sizing: border-box;
  width: 45%;
  margin-left: 3.3%;
  display: inline-block;
}
dl.rank dd .btn-yellow {
  display: inline-block;
  background-color: #4564ba;
  border-radius: .3rem;
  color: #ffffff;
  text-align: center;
  font-size: 1.43rem;
  padding: .4rem 0;
  -webkit-box-sizing: border-box;
  -ms-box-sizing: border-box;
  box-sizing: border-box;
  width: 45%;
  margin-right: 3.3%;
  margin-left: 3.4%;
}
footer.btns {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  z-index: 3;
}
footer.btns a {
  width: 50%;
  display: inline-block;
  color: #ffffff;
  font-size: 1.43rem;
  text-align: center;
  padding: .5rem 0;
}
footer.btns a:nth-of-type(1) {
  background-color: #4564ba;
}
footer.btns a:nth-of-type(2) {
  background-color: #3d3d8b;
}
