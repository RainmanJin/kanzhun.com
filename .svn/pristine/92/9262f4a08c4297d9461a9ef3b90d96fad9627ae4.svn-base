html {
  height: 100%;
}
body {
  font: normal 14px/1.5 Arial, Helvetica, "Microsoft YaHei", STHeiti, "Droid Sans Fallback", sans-serif;
  color: #343434;
  height: 100%;
}
input,
button,
textarea,
select {
  font: normal 14px/1.5 Arial, Helvetica, "Microsoft YaHei", STHeiti, "Droid Sans Fallback", sans-serif;
}
a {
  color: #343434;
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
}
a.a_ul {
  text-decoration: underline;
}
h1,
h2,
h3,
h4,
h5,
h6 {
  font-size: 1em;
  line-height: 1;
}
.wrapper {
  width: 100%;
  min-height: 100%;
  height: auto;
}
body {
  background: url("../images/q1/content_bg.jpg") no-repeat #9bc3df;
  background-size: 100% 100%;
  overflow: hidden;
}
header {
  background-color: #4f93d0;
  height: 45px;
  color: #fff;
  text-align: center;
}
header h3 {
  line-height: 45px;
  font-size: 23px;
}
header .person {
  position: absolute;
  top: 10px;
  right: 20px;
  width: 25%;
}
header .logo {
  position: absolute;
  width: 13%;
  margin: 3%;
  top: 0px;
  left: 0px;
}
#content .avatar {
  width: 58px;
  height: 58px;
  border-radius: 29px;
  margin: 30px 10px 15px 10px;
  float: left;
}
#content .declar {
  margin-top: 15px;
  width: 203px;
}
#content .person {
  margin: 10px 0px;
  text-align: center;
}
#content .person i {
  color: #4c8fce;
}
#content .pros {
  width: 90%;
  margin: 0 auto;
}
#content .pros .progress {
  position: relative;
  background-color: #fff;
  height: 35px;
  line-height: 35px;
  margin-bottom: 10px;
  padding-left: 10px;
}
#content .pros .progress i {
  color: #4c8fce;
  font-size: 20px;
  padding: 0px 3px;
}
#content .pros .progress .finish {
  position: absolute;
  top: 0px;
  left: 0px;
  opacity: 0.5;
  height: 35px;
  background-color: #ffe628;
  -webkit-transition: 1s linear;
  -moz-transition: 1s linear;
  transition: 1s linear;
  -o-transition: 1s linear;
  -webkit-transition-property: width, background-color;
  -moz-transition-property: width, background-color;
  -o-transition-property: width, background-color;
  transition-property: width, background-color;
}
#content .pros .progress .r_icon {
  height: 28px;
  margin-right: 10px;
  margin-top: 3px;
  float: right;
}
#content .obtain_wrap {
  margin: 0 auto;
  width: 85%;
}
#content .obtain_wrap .obtain {
  position: relative;
}
#content .obtain_wrap .obtain .word {
  background-color: #ffe628;
  margin: 15px 0px;
  font-size: 18px;
  border-radius: 5px;
  padding: 5px;
}
#content .obtain_wrap .obtain .word .after {
  float: right;
  margin-right: 8px;
}
#content .obtain_wrap .obtain img {
  width: 60%;
  position: absolute;
  top: 0;
  left: 4.5em;
}
#oper {
  width: 90%;
  margin: 0 auto;
}
#oper a {
  background-color: #f27506;
  color: #fff;
  width: 40%;
  text-align: center;
  font-size: 18px;
  font-weight: bolder;
  padding: 8px;
  border-radius: 6px;
}
#oper .generate {
  float: left;
}
#oper .rank {
  float: right;
}
#obtain .name1 {
  animation: myscale 1s linear alternate;
  -webkit-animation: myscale 2s ease-out alternate;
}
@keyframes myscale {
  0% {
    transform: scale(1, 1);
  }
  50% {
    transform: scale(1.8, 1.8);
  }
  100% {
    transform: scale(1, 1);
  }
}
@-webkit-keyframes myscale {
  0% {
    -webkit-transform: scale(1, 1);
  }
  50% {
    -webkit-transform: scale(1.8, 1.8);
  }
  100% {
    -webkit-transform: scale(1, 1);
  }
}
