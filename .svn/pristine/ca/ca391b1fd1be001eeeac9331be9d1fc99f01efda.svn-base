.p_dialog {
  width: 480px;
  background-color: #e9e9e9;
  background-color: rgba(233, 233, 233, 0.8);
  padding: 8px;
  border-radius: 8px;
  -webkit-box-shadow: 0 0 10px #666666;
  -moz-box-shadow: 0 0 10px #666666;
  -ms-box-shadow: 0 0 10px #666666;
  box-shadow: 0 0 10px #666666;
  display: none;
}
.p_dialog h3.poptitle {
  font-size: 18px;
  font-weight: normal;
  margin-bottom: 0;
}
.p_dialog .dialog_con {
  position: relative;
  border: 1px solid #a0a0a0;
  min-height: 100px;
  _height: 100px;
  background-color: #fff;
  padding: 10px 20px 20px 20px;
}
.p_dialog .dialog_close {
  width: 23px;
  height: 23px;
  display: inline-block;
  position: absolute;
  background: transparent url(../../images/manage_icons.png) no-repeat -120px 0;
  right: 10px;
  top: 10px;
  -webkit-transition: all 0.2s linear;
  -moz-transition: all 0.2s linear;
  -ms-transition: all 0.2s linear;
  -o-transition: all 0.2s linear;
  transition: all 0.2s linear;
}
.p_dialog .dialog_close:hover {
  background-color: #eee;
}
.p_dialog h3 {
  font-size: 18px;
  font-weight: normal;
}
.p_dialog .dialog_alert {
  padding: 20px 0;
  text-align: center;
}
.p_dialog .dialog_alert p {
  margin-top: 20px;
}
.p_dialog .dialog_resume_ret p.d_con {
  margin-top: 20px;
}
.p_dialog .dialog_resume_ret .email_con {
  width: 310px;
  height: 80px;
  padding: 10px;
  border: 1px solid #999999;
  overflow-y: auto;
  zoom: 1;
  display: inline-block;
  vertical-align: top;
  font-size: 12px;
  line-height: 1.5em;
}
.p_dialog .dialog_resume_ret ul li {
  margin-bottom: 10px;
  overflow: hidden;
  zoom: 1;
}
.p_dialog .dialog_resume_ret ul li div.email_con {
  float: right;
  width: 310px;
  margin-right: 35px;
}
.p_dialog .dialog_resume_ret ul li div.email_con textarea {
  font-size: 14px;
  width: 310px;
}
.p_dialog .dialog_resume_ret ul li.options {
  margin: 30px 0 0 71px;
}
.p_dialog .r_confirm {
  text-align: left;
}
.p_dialog .r_confirm strong {
  display: block;
  font-size: 16px;
  padding-bottom: 10px;
}
.p_dialog .reset {
  border: 0;
  color: #007ead;
  background-color: transparent;
  cursor: pointer;
}
.p_dialog p.err {
  color: #d60000;
  padding: 2px 0 2px;
  display: none;
}
.p_dialog .resume_err p.err {
  padding: 2px 0 2px 6em;
}
.interview_notice {
  width: 590px;
}
.interview_notice .dialog_con {
  padding-right: 10px;
}
.select_point p {
  margin-right: -10px;
}
.select_point a {
  display: inline-block;
  zoom: 1;
  width: 8em;
  height: 30px;
  line-height: 30px;
  padding: 0 2px;
  margin: 0 10px 10px 0;
  background-color: #ededed;
  text-align: center;
