i.i {
  display: inline-block;
  background: url("../images/icons.png") no-repeat;
  -webkit-background-size: 100px;
  -moz-background-size: 100px;
  background-size: 100px;
}
i.i.i_back {
  width: 9px;
  height: 17px;
}
i.i.i_eq {
  width: 9px;
  height: 9px;
  background-position: -20px 0;
}
i.i.i_tdown {
  width: 7px;
  height: 6px;
  background-position: -40px 0;
}
i.i.i_hand {
  width: 15px;
  height: 13px;
  background-position: -20px -60px;
}
i.i.i_hand_grey {
  width: 15px;
  height: 13px;
  background-position: 0 -60px;
}
i.i.i_edit {
  width: 15px;
  height: 15px;
  background-position: -82px -39px;
}
i.i.i_star {
  width: 17px;
  height: 16px;
  background-position: -60px -60px;
}
i.i.i_star_grey {
  width: 17px;
  height: 16px;
  background-position: -40px -60px;
}
i.i.i_down_blue {
  width: 14px;
  height: 7px;
  background-position: -60px 0;
}
i.i.i_r_grey {
  width: 9px;
  height: 14px;
  background-position: -80px -20px;
  vertical-align: middle;
}
i.i_rect {
  display: inline-block;
  background: url("../images/icons.png") no-repeat;
  -webkit-background-size: 100px;
  -moz-background-size: 100px;
  background-size: 100px;
  width: 78px;
  height: 14px;
  background-position: 0 -20px;
  vertical-align: -2px;
  font-size: 0;
}
i.i_rect em {
  display: inline-block;
  background: url("../images/icons.png") no-repeat;
  -webkit-background-size: 100px;
  -moz-background-size: 100px;
  background-size: 100px;
  width: 78px;
  height: 14px;
  background-position: 0 -40px;
}
i.i_rect .em_0 {
  display: none;
}
i.i_rect .em_1 {
  width: 20%;
}
i.i_rect .em_2 {
  width: 40%;
}
i.i_rect .em_3 {
  width: 60%;
}
i.i_rect .em_4 {
  width: 80%;
}
i.i_rect .em_5 {
  width: 100%;
}
.grade_star {
  width: 69px;
  height: 13px;
  display: inline-block;
  font-size: 0;
  vertical-align: -2px;
  overflow: hidden;
  background: transparent url("../images/m_grade.png") no-repeat 0px 0;
  -webkit-background-size: auto 26px;
  -moz-background-size: auto 26px;
  background-size: auto 26px;
}
.grade_star i {
  height: 13px;
  float: left;
  background: transparent url("../images/m_grade.png") no-repeat 0 -13.5px;
  -webkit-background-size: auto 26px;
  -moz-background-size: auto 26px;
  background-size: auto 26px;
}
.grade_star i.s_1 {
  width: 13px;
}
.grade_star i.s_2 {
  width: 27px;
}
.grade_star i.s_3 {
  width: 41px;
}
.grade_star i.s_4 {
  width: 54px;
}
.grade_star i.s_5 {
  width: 69px;
}
