body {
  font-size: 14px;
}
section.resumeContent {
  width: 950px;
  margin: 0 auto 40px;
  padding: 20px;
  background-color: #fff;
  min-height: 351px;
  overflow: hidden;
}
section.resumeContent div.resumeRight {
  float: left;
  width: 708px;
  padding-left: 20px;
  min-height: 600px;
  border-left: 1px solid #e7e7e7;
}
label.lb_tt {
  float: left;
  margin-right: 15px;
  width: 4em;
  text-align: right;
  height: 35px;
  line-height: 35px;
}
h3.p_tt {
  padding-left: 8px;
  border-left: 4px solid #7cb228;
  line-height: 1em;
  font-size: 16px;
}
div.sorting {
  height: 46px;
  padding: 0 10px;
  line-height: 46px;
  border: 1px solid #eaeae8;
}
div.sorting p {
  float: right;
}
div.sorting p strong {
  margin: 0 5px;
}
div.sorting a {
  display: inline-block;
  padding: 0 10px;
  height: 26px;
  line-height: 26px;
  border: 1px solid #eaeae8;
  color: #343434;
}
div.sorting a:hover,
div.sorting a.current {
  color: #7cb228;
  text-decoration: none;
}
