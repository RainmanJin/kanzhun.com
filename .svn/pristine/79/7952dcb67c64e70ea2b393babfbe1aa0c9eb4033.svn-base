aside.prompt {
  padding: 10px;
  margin-top: 15px;
  background-color: #fcf5e1;
  border: 1px solid #feebb7;
  border-radius: 5px;
}
aside.prompt p {
  color: #999999;
}
label.ttlabel {
  display: inline-block;
  width: 4em;
  margin-right: 15px;
  font-size: 14px;
  height: 34px;
  line-height: 34px;
  text-align: right;
  vertical-align: middle;
}
div.searchinfo {
  margin: 20px -20px 0 -20px;
  padding: 0 20px;
  height: 50px;
  line-height: 50px;
  background-color: #f3f3f4;
  color: #999999;
  font-size: 14px;
}
div.searchinfo p {
  float: right;
}
div.searchinfo p a {
  text-decoration: underline;
}
.line_only {
  border-bottom: 1px solid #e7e7e7;
  height: 0;
  overflow: hidden;
}
.line {
  border-bottom: 1px solid #e7e7e7;
  height: 0;
  overflow: hidden;
  margin: 15px 0;
}
