section.top-search {
  border: 1px solid #dcdcdc;
  background-color: #fff;
  -webkit-box-shadow: 0 1px 0 #e6e6e6;
  -moz-box-shadow: 0 1px 0 #e6e6e6;
  -ms-box-shadow: 0 1px 0 #e6e6e6;
  box-shadow: 0 1px 0 #e6e6e6;
  width: 988px;
  margin: 15px auto;
}
section.top-search.active {
  background-color: #fff;
}
section.top-search .top-search-form {
  padding: 15px;
  overflow: hidden;
}
section.top-search .top-search-form fieldset {
  border: 1px solid #cccccc;
  height: 32px;
  background-color: #fafafa;
  float: left;
}
section.top-search .top-search-form fieldset input[type=text] {
  float: left;
  border: 0;
  padding: 8px 10px;
  width: 380px;
  height: 16px;
  line-height: 16px;
  background-color: transparent;
}
section.top-search .top-search-form fieldset input[type=submit] {
  border: 0;
  background: transparent url("/images/v2/search_icon.png") no-repeat 0 6px;
  height: 32px;
  line-height: 32px;
  padding: 0 10px 0 25px;
  float: left;
  cursor: pointer;
  color: #cccccc;
}
section.top-search .top-search-form p {
  float: left;
  margin: 6px 0 0 30px;
  white-space: nowrap;
  overflow: hidden;
}
section.top-search .top-search-form p a {
  color: #666;
  margin: 0 6px;
}
.top-search-nav {
  overflow: hidden;
  border-top: 1px solid #eaeaea;
  background-color: #fafafa;
  -webkit-box-shadow: 0 1px 0 #e6e6e6;
  -moz-box-shadow: 0 1px 0 #e6e6e6;
  -ms-box-shadow: 0 1px 0 #e6e6e6;
  box-shadow: 0 1px 0 #e6e6e6;
  width: 988px;
  margin: -16px auto 15px;
}
.top-search-nav a {
  padding: 10px 30px;
  float: left;
  border-right: 1px solid #eaeaea;
  color: #333;
}
.top-search-nav a.current {
  font-weight: bold;
  background-color: #fff;
}
.top-search-nav a:hover {
  text-decoration: none;
}
div.C-toggler {
  display: inline-block;
  cursor: pointer;
  font-size: 12px;
  color: #999999;
}
div.C-toggler i {
  display: inline-block;
  font-size: 0;
  line-height: 0;
  overflow: hidden;
  width: 0;
  height: 0;
  margin-left: 5px;
  border-width: 4px 4px 0 4px;
  border-style: solid dashed;
  border-color: #999999 transparent transparent transparent;
  vertical-align: 2px;
}
div.C-toggler.active i {
  border-width: 0 4px 4px 4px;
  border-color: transparent transparent #999999 transparent;
}
div.C-toggler.active:hover i {
  border-bottom-color: #7cb228;
}
div.C-toggler:hover {
  color: #7cb228;
}
div.C-toggler:hover i {
  border-top-color: #7cb228;
}
div.container-search {
  width: 990px;
  margin: 15px auto 0 auto;
}
.wrap_style {
  padding: 0 15px;
  border: 1px solid #dcdcdc;
  background-color: #ffffff;
  -webkit-box-shadow: 0 1px 0 #e6e6e6;
  -moz-box-shadow: 0 1px 0 #e6e6e6;
  -ms-box-shadow: 0 1px 0 #e6e6e6;
  box-shadow: 0 1px 0 #e6e6e6;
}
section.s_r_con {
  float: right;
  width: 711px;
}
section.s_l_con {
  float: left;
  width: 264px;
}
a {
  color: #2c84cc;
}
a.links:hover {
  color: #2472b4;
  text-decoration: none;
}
a.hover_deff:hover {
  color: #343434;
}
.tangle_init {
  width: 0;
  height: 0;
  line-height: 0;
  font-size: 0;
}
dl.search-options {
  padding: 15px;
}
dl.search-options dt strong {
  font-size: 16px;
}
dl.search-options dt strong span {
  color: #d60000;
}
dl.search-options dt a {
  float: right;
}
dl.search-options dt a span {
  color: #d60000;
}
dl.search-options dd {
  margin: 15px -15px 0;
  padding: 15px 15px 0;
  border-top: 1px solid #e2e2e2;
}
dl.search-options dd span {
  color: #fff;
  background-color: #7cb228;
  padding: 4px 5px;
  border-radius: 2px;
  margin-right: 10px;
  font-size: 12px;
}
dl.search-options dd span a {
  color: #fff;
  margin-left: 4px;
  padding: 2px 0 2px 2px;
  vertical-align: 1px;
}
dl.search-options dd span a:hover {
  text-decoration: none;
}
.search-list-co-brief {
  padding: 15px 0;
}
.search-list-co-brief img {
  width: 50px;
  height: 50px;
  border: 1px solid #E7E7E7;
  vertical-align: middle;
  margin-right: 10px;
}
.search-list-co-brief-grey {
  padding: 15px;
  margin: 0 -15px;
  background-color: #f6f6f6;
}
.search-noresult {
  text-align: center;
  padding: 100px 0 200px;
}
.search-noresult > img {
  vertical-align: top;
}
.search-noresult > div {
  margin-left: 20px;
  text-align: left;
  display: inline-block;
  *zoom: 1;
  *display: inline;
  vertical-align: top;
}
div.plr_twenty {
  padding: 0 20px 15px;
}
div.plr_twenty h2 {
  margin-top: 15px;
  padding: 15px 0;
  font-size: 16px;
  color: #333333;
  font-weight: normal;
  border-top: 1px dashed #e3e3e3;
}
div.plr_twenty h2:first-child {
  border-top: 0;
  margin-top: 0;
}
div.hot_cmp {
  overflow: hidden;
  margin-right: -15px;
}
div.hot_cmp a {
  float: left;
  margin-right: 20px;
  line-height: 2em;
  color: #666666;
  white-space: nowrap;
}
div.hot_cmp a span {
  color: #999999;
}
div.hot_cmp a:hover {
  color: #2472b4;
  text-decoration: none;
}
div.hot_cmp a:hover span {
  color: #2472b4;
}
div.block-items a {
  display: block;
  float: none;
}
div.host_city {
  overflow: hidden;
  margin-right: -15px;
}
div.host_city a {
  white-space: nowrap;
  float: left;
  margin-right: 20px;
  margin-bottom: 10px;
  color: #666666;
}
div.host_city p.tt {
  height: 36px;
  line-height: 36px;
  color: #333333;
  background-color: #f6f6f6;
  text-align: center;
  margin-right: 15px;
}
div.host_city ul.first_letter {
  overflow: hidden;
  margin-right: -5px;
  padding-bottom: 5px;
}
div.host_city ul.first_letter li {
  float: left;
  width: 22px;
  height: 22px;
  line-height: 22px;
  text-align: center;
  background-color: #f6f6f6;
  color: #333333;
  margin: 0 11px 10px 0;
  cursor: pointer;
  border-radius: 2px;
}
div.host_city ul.first_letter li:hover,
div.host_city ul.first_letter li.current {
  background-color: #7cb228;
  color: #ffffff;
}
div.host_city .search-opts-cities li {
  display: none;
}
div.see_more {
  width: 222px;
}
div.see_more.no-bd {
  border-bottom: 0;
}
.s-opts-more {
  clear: both;
  display: none;
}
ul.industry_field li {
  position: relative;
  z-index: 3;
}
ul.industry_field li a.default {
  color: #333333;
  position: relative;
  cursor: pointer;
  display: block;
  height: 30px;
  line-height: 30px;
  margin: 0 -20px;
  padding-left: 20px;
}
ul.industry_field li a.default i.i_right {
  background: url("/images/icons_v2.png") no-repeat -220px 0;
  width: 5px;
  height: 10px;
  display: inline-block;
  vertical-align: middle;
  position: absolute;
  right: 21px;
  top: 50%;
  margin-top: -5px;
}
ul.industry_field li a.default:hover {
  text-decoration: none;
}
ul.industry_field li div.more_list {
  display: none;
  position: absolute;
  top: 0px;
  left: 232px;
  padding-top: 15px;
  padding-left: 20px;
  width: 480px;
  min-height: 10em;
  background: #fafafa;
  border: 1px solid #e7e7e7;
  -webkit-box-shadow: 0 0 4px #d5d4d4;
  -moz-box-shadow: 0 0 4px #d5d4d4;
  -ms-box-shadow: 0 0 4px #d5d4d4;
  box-shadow: 0 0 4px #d5d4d4;
}
ul.industry_field li div.more_list s {
  position: absolute;
  left: -5px;
  top: 0;
  width: 10px;
  height: 28px;
  background: #fafafa;
}
ul.industry_field li div.more_list span {
  display: inline-block;
  width: 160px;
  float: left;
  padding-bottom: 20px;
  line-height: 1;
}
ul.industry_field li div.more_list span a {
  display: inline-block;
  width: 11em;
  height: 1em;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
ul.industry_field li div.more_list span a:hover {
  color: #2472b4;
}
ul.industry_field li.current div.more_list {
  display: block;
}
ul.industry_field li.current a.default {
  height: 28px;
  line-height: 28px;
  border-width: 1px 0 1px 1px;
  border-style: solid;
  border-color: #e7e7e7;
  background-color: #fafafa;
  -webkit-box-shadow: 0 0 4px #d5d4d4;
  -moz-box-shadow: 0 0 4px #d5d4d4;
  -ms-box-shadow: 0 0 4px #d5d4d4;
  box-shadow: 0 0 4px #d5d4d4;
  text-decoration: none;
  margin: 0 -21px;
}
ul.industry_field li.current a.default i.i_right {
  display: none;
}
table {
  width: 100%;
}
table.salary-desc {
  table-layout: fixed;
  border-top: 1px solid #f3f3f3;
}
table.salary-desc th {
  height: 34px;
  font-weight: normal;
  vertical-align: middle;
  text-align: center;
  border-bottom: 1px solid #f3f3f3;
}
table.salary-desc th:nth-of-type(1) {
  text-align: center;
}
table.salary-desc th p {
  padding-left: 5px;
  padding-right: 25px;
  overflow: hidden;
}
table.salary-desc th span {
  width: 80px;
  text-align: center;
}
table.salary-desc th span.fright {
  width: 85px;
}
table.salary-desc th:first-child {
  padding: 0 5px;
}
table.salary-desc tr.ps_top {
  background-color: #f5f5f5;
}
table.salary-desc tr.ps_top th.salary {
  text-align: left;
  margin-top: 30px;
}
table.salary-desc tr.ps_top p {
  position: relative;
  padding-left: 60px;
  margin-right: 100px;
  overflow: visible;
  margin-left: 0;
  font-size: 14px;
}
table.salary-desc tr.ps_top p a.logo {
  display: block;
  position: absolute;
  left: 10px;
  top: -18px;
  width: 30px;
  height: 30px;
  padding: 2px;
  background-color: #fff;
  border: 1px solid #f3f3f3;
  font-size: 0;
  text-align: center;
  overflow: hidden;
}
table.salary-desc tr.ps_top p a.logo img {
  width: 30px;
  display: inline-block;
  vertical-align: middle;
}
table.salary-desc tr.ps_top p a.logo span {
  display: inline-block;
  width: 0;
  height: 100%;
  vertical-align: middle;
}
table.salary-desc td {
  border-bottom: 1px solid #f3f3f3;
  cursor: pointer;
  height: 50px;
}
table.salary-desc td:first-child,
table.salary-desc td:last-child {
  padding: 0 5px;
}
table.salary-desc td.s-d-average {
  border-left: 1px solid #f3f3f3;
  border-right: 1px solid #f3f3f3;
  text-align: center;
}
table.salary-desc tr.on td {
  background-color: #f3f3f3;
}
table.salary-desc tr.on td i.s-d-show {
  display: block;
}
table.salary-desc tr.show td i.s-d-show {
  display: block;
}
table.salary-desc tr.show_flip td i.s-d-show {
  display: block;
  -webkit-transform: rotate(180deg);
  -moz-transform: rotate(180deg);
  -ms-transform: rotate(180deg);
  -o-transform: rotate(180deg);
  transform: rotate(180deg);
}
table.salary-desc tr.s-d-c td {
  cursor: inherit;
}
table.salary-desc div.s-d-digit {
  height: 20px;
  padding: 15px 30px 15px 80px;
  position: relative;
  width: 208px;
}
table.salary-desc div.s-d-digit p {
  width: 208px;
  height: 18px;
  padding-top: 1px;
  padding-bottom: 1px;
  position: relative;
  background: url("/images/salary/salary-bar.png") no-repeat left center;
}
table.salary-desc div.s-d-digit p em {
  position: absolute;
  top: 0;
  display: block;
  text-align: center;
}
table.salary-desc div.s-d-digit p em.s-d-l {
  width: 80px;
  left: -80px;
}
table.salary-desc div.s-d-digit p em.s-d-r {
  width: 85px;
  right: -85px;
}
table.salary-desc div.s-d-digit .loading {
  position: absolute;
  right: -85px;
  top: -10px;
  display: block;
  width: 40px;
}
table.salary-desc div.s-d-digit span {
  width: 8px;
  height: 12px;
  margin-top: 4px;
  display: block;
  background: url('/images/salary/salary-i.png') no-repeat 0 -103px;
}
table.salary-desc div.s-d-digit i.s-d-show {
  width: 13px;
  height: 7px;
  background: url('/images/salary/salary-i.png') no-repeat 0 -84px;
  display: none;
  overflow: hidden;
  position: absolute;
  right: -74px;
  top: 21px;
  -webkit-transition: all ease 0.2s;
  -moz-transition: all ease 0.2s;
  -ms-transition: all ease 0.2s;
  -o-transition: all ease 0.2s;
  transition: all ease 0.2s;
}
table.salary-desc .s-d-digit-min {
  position: absolute;
  left: 35px;
  top: 15px;
}
table.salary-desc .s-d-digit-max {
  position: absolute;
  right: -20px;
  top: 15px;
}
table.salary-desc .s-d-chart {
  overflow: hidden;
  padding: 10px;
}
table.salary-desc .s-d-chart ul {
  width: 165px;
  float: left;
  margin-top: 20px;
}
table.salary-desc .s-d-chart li {
  padding-left: 20px;
}
table.salary-desc .s-d-chart li.s-d-low {
  background: url('/images/salary/salary-i.png') no-repeat 0 2px;
  margin-bottom: 5px;
}
table.salary-desc .s-d-chart li.s-d-high {
  background: url('/images/salary/salary-i.png') no-repeat 0 -18px;
  margin-bottom: 20px;
}
table.salary-desc .s-d-chart li.s-d-mark a {
  display: inline-block;
  padding: 5px 20px;
  border: 1px solid #f3f3f3;
  color: #999999;
}
table.salary-desc .s-d-chart li.s-d-mark a i {
  background: url('/images/salary/salary-i.png') no-repeat 0 -41px;
  width: 13px;
  display: inline-block;
  vertical-align: middle;
  height: 12px;
  margin-right: 3px;
}
table.salary-desc .s-d-chart li.s-d-mark a:hover {
  text-decoration: none;
  border: 1px solid #7cb228;
}
table.salary-desc .s-d-chart li.s-d-mark a:hover i {
  background-position: 0 -61px;
}
table.salary-desc .s-d-chart li.s-d-mark a.marked i {
  background-position: 0 -61px;
}
table.salary-desc .s-d-chart li.s-d-mark a.marked:hover {
  border: 1px solid #f3f3f3;
}
table.salary-desc .s-d-chart li.s-d-mark a.marked:hover i {
  background-position: 0 -61px;
}
table.salary-desc .s-d-chart section {
  float: right;
  width: 420px;
  height: 190px;
  margin-right: 17px;
  display: inline;
}
table.salary-desc-th {
  margin-bottom: 0;
}
table.salary-desc-th td {
  border: 0;
}
span.s-c-flag {
  display: inline-block;
  width: 60px;
  vertical-align: middle;
  height: 16px;
  background-color: #bcd891;
  margin-right: 10px;
}
span.s-c-flag2 {
  width: 10px;
  background-color: #81b924;
  margin-left: 50px;
}
.salary-desc {
  margin-bottom: 15px;
}
