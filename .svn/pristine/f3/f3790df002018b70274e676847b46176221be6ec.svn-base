div.barrage {
  position: fixed;
  left: 0;
  top: 40%;
  width: 100%;
  overflow: hidden;
  z-index: 10;
  color: #ffffff;
}
div.barrage a {
  color: #fff;
}
div.barrage p {
  width: 3000px;
  white-space: nowrap;
  line-height: 3em;
}
div.barrage p span {
  margin: 0 10px;
}
div.barrage p:nth-of-type(1) {
  font-size: 16px;
}
div.barrage p:nth-of-type(1) span:nth-child(3n) {
  color: #ffffff;
  font-weight: bold;
}
div.barrage p:nth-of-type(1) span:nth-child(4n) {
  font-size: 16px;
}
div.barrage p:nth-of-type(1) span:nth-child(5n) {
  color: orange;
  font-size: 26px;
  font-weight: bold;
  -webkit-text-shadow: 0, 0, 3px, #ffffff;
  -ms-text-shadow: 0, 0, 3px, #ffffff;
  text-shadow: 0, 0, 3px, #ffffff;
}
div.barrage p:nth-of-type(1) span:nth-child(7n) {
  color: blue;
  font-size: 20px;
}
div.barrage p:nth-of-type(1) span:nth-child(11n) {
  color: red;
  font-size: 30px;
}
div.barrage p:nth-of-type(2) {
  font-size: 15px;
}
div.barrage p:nth-of-type(2) span:nth-child(4n) {
  color: green;
}
div.barrage p:nth-of-type(2) span:nth-child(6n) {
  font-size: 20px;
  font-weight: bold;
}
div.barrage p:nth-of-type(2) span:nth-child(8n) {
  color: blue;
  font-weight: bold;
  font-size: 30px;
  -webkit-text-shadow: 0, 0, 3px, #ffffff;
  -ms-text-shadow: 0, 0, 3px, #ffffff;
  text-shadow: 0, 0, 3px, #ffffff;
}
div.barrage p:nth-of-type(2) span:nth-child(11n) {
  font-size: 24px;
}
div.barrage p:nth-of-type(3) {
  font-size: 15px;
}
div.barrage p:nth-of-type(3) span:nth-child(3n) {
  color: #ffff00;
}
div.barrage p:nth-of-type(3) span:nth-child(7n) {
  font-size: 33px;
  color: purple;
  font-weight: bold;
}
div.barrage p:nth-of-type(3) span:nth-child(9n) {
  color: red;
  font-size: 19px;
  -webkit-text-shadow: 0, 0, 3px, #ffffff;
  -ms-text-shadow: 0, 0, 3px, #ffffff;
  text-shadow: 0, 0, 3px, #ffffff;
}
div.barrage p:nth-of-type(3) span:nth-child(11n) {
