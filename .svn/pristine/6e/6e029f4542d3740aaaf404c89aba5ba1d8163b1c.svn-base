.grade_star_b {
  width: 118px;
  height: 22px;
  overflow: hidden;
  display: inline-block;
  font-size: 0;
  background: url(../images/manage_icons.png) no-repeat 0 0;
  vertical-align: middle;
  line-height: 0;
}
.grade_star_b i {
  display: inline-block;
  width: 118px;
  height: 22px;
  overflow: hidden;
  font-size: 0;
  background: url(../images/manage_icons.png) no-repeat 0 -24px;
}
section.co_header {
  position: relative;
  width: 990px;
  background-color: #fff;
  margin: 0 auto 0 auto;
}
section.co_header .co_logo {
  position: absolute;
  width: 150px;
  height: 150px;
  display: block;
  padding: 1px;
  background-color: #fff;
  border: 1px solid #e7e7e7;
  top: 20px;
  left: 20px;
  overflow: hidden;
  font-size: 0;
}
section.co_header .co_logo:after {
  content: '';
}
section.co_header .co_logo:after,
section.co_header .co_logo span {
  display: inline-block;
  *display: inline;
  *zoom: 1;
  width: 0;
  height: 100%;
  vertical-align: middle;
}
section.co_header .co_logo img {
  width: 150px;
  display: inline-block;
  *display: inline;
  *zoom: 1;
  vertical-align: middle;
}
section.co_header .co_info {
  padding: 40px 0 25px 195px;
}
section.co_header .co_info h2 {
  font: bold 24px/1.5 Arial, STHeiti, Microsoft YaHei, simhei, simsun;
}
section.co_header .co_info em {
  margin: 0 8px;
  color: #e7e7e7;
}
section.co_header .msg {
  font-size: 14px;
  height: 22px;
  line-height: 22px;
  padding: 5px 0;
  color: #999999;
}
section.co_header .msg strong {
  font-size: 18px;
  color: #666666;
}
section.co_header .msg .grade_star_b {
  vertical-align: -5px;
  *vertical-align: middle;
  margin-right: 10px;
}
section.co_header .params {
  color: #999999;
  font-size: 14px;
}
section.co_header nav {
  height: 40px;
  line-height: 40px;
  background-color: #6e6e6e;
  padding-left: 200px;
}
section.co_header nav a {
  font-size: 16px;
  padding: 0 35px;
  float: left;
  color: #fff;
  text-decoration: none;
  border-right: 1px solid #999999;
  white-space: nowrap;
}
section.co_header nav a:hover {
  text-decoration: none;
}
section.co_header nav a.current {
  background-color: #7cb228;
  border: 1px solid #7cb228;
  border-radius: 2px;
  position: relative;
  top: -1px;
  left: -1px;
}
aside.resumeLeft {
  float: left;
  width: 206px;
  padding-right: 15px;
}
aside.resumeLeft dl.l_nav {
  border: 1px solid #e7e7e7;
  border-bottom: none;
}
aside.resumeLeft dl.l_nav dt {
  height: 40px;
  line-height: 40px;
  color: #999999;
  padding-left: 15px;
  border-bottom: 1px solid #e7e7e7;
}
aside.resumeLeft dl.l_nav dt span {
  font-size: 14px;
  vertical-align: -2px;
  margin-left: 10px;
}
aside.resumeLeft dl.l_nav dd {
  height: 40px;
  line-height: 40px;
  border-bottom: 1px solid #e7e7e7;
}
aside.resumeLeft dl.l_nav dd a {
  color: #000;
  display: block;
  padding-left: 55px;
}
aside.resumeLeft dl.l_nav dd.current {
  background-color: #ededed;
}
aside.resumeLeft dl.l_nav dd.current a {
  color: #7cb228;
}
aside.resumeLeft dl.l_nav dd.current a:hover {
  text-decoration: none;
}
aside.resumeLeft dl.l_nav dd strong {
  margin: 0 5px;
}
aside.resumeLeft dl.l_nav dd span {
  display: inline-block;
  width: 57px;
  padding: 0 5px;
  font-weight: bold;
  text-align: right;
}
aside.resumeLeft dl.history_search {
  border: 1px solid #e7e7e7;
}
aside.resumeLeft dl.history_search dt {
  padding: 10px 15px;
}
aside.resumeLeft dl.history_search dd a {
  display: block;
  padding: 10px 0;
  margin: 0 15px;
  border-top: 1px dashed #e7e7e7;
}
aside.manageLeft {
  width: 204px;
}
aside.manageLeft a.send_jobs {
  background-color: #f38100;
}
aside.manageLeft a.send_jobs:hover {
  background-color: #fa8712;
}
aside.manageLeft dl {
  border: 1px solid #e7e7e7;
}
aside.manageLeft dl dt {
  height: 40px;
  line-height: 40px;
  color: #999999;
  padding-left: 15px;
}
aside.manageLeft dl dt span {
  font-size: 14px;
  vertical-align: -2px;
  margin-left: 5px;
}
aside.manageLeft dl dd {
  height: 40px;
  line-height: 40px;
  border-top: 1px solid #e7e7e7;
}
aside.manageLeft dl dd a {
  color: #000;
  display: block;
  padding-left: 50px;
}
aside.manageLeft dl dd.current {
  background-color: #ededed;
}
aside.manageLeft dl dd.current a {
  color: #7cb228;
}
aside.manageLeft dl dd.current a:hover {
  text-decoration: none;
}
.page_wrap {
  overflow: hidden;
  *zoom: 1;
  padding: 10px 0;
}
.f_pager {
  overflow: hidden;
  *zoom: 1;
}
.f_pager a {
  float: left;
  width: 26px;
  height: 26px;
  margin-left: 6px;
  line-height: 26px;
  text-align: center;
  border: 1px solid #cccccc;
  background-color: #ffffff;
  font-size: 14px;
  color: #666666;
  border-radius: 2px;
}
.f_pager a:hover {
  border-color: #85d487;
  background-color: #85d487;
  color: #ffffff;
  font-weight: bold;
  text-decoration: none;
}
.f_pager a.current {
  border-color: #67bf58;
  background-color: #67bf58;
  color: #ffffff;
  font-weight: bold;
}
.f_pager a.p_first i {
  width: 8px;
  height: 10px;
  display: block;
  margin: 9px 0 0 9px;
  *margin-left: 3px;
  text-indent: -99999px;
  background: url(../images/icons_v2.png) no-repeat -114px 0;
}
.f_pager a.p_first:hover i {
  background-position: -126px 0;
}
.f_pager a.p_prev i {
  display: block;
  margin: 8px 0 0 10px;
  *margin-left: 3px;
  text-indent: -99999px;
  width: 6px;
  height: 10px;
  background: url(../images/icons_v2.png) no-repeat -139px 0;
}
.f_pager a.p_prev:hover i {
  background-position: -150px 0;
}
.f_pager a.p_next i {
  display: block;
  margin: 8px 0 0 10px;
  *margin-left: 3px;
  text-indent: -99999px;
  width: 6px;
  height: 10px;
  background: url(../images/icons_v2.png) no-repeat -160px 0;
}
.f_pager a.p_next:hover i {
  background-position: -170px 0;
}
table.tb_evaluation {
  table-layout: fixed;
  border-left: 1px solid #f3f3f3;
  margin-bottom: 10px;
}
table.tb_evaluation tr {
  height: 40px;
  border-bottom: 1px solid #f3f3f3;
  background-color: #ffffff;
}
table.tb_evaluation tr:nth-child(odd) {
  background-color: #f6f6f6;
}
table.tb_evaluation tr td,
table.tb_evaluation tr th {
  border-right: 1px solid #f3f3f3;
}
table.tb_evaluation tr th {
  height: 40px;
  background-color: #7cb228;
  color: #fff;
  font-weight: normal;
  font-size: 14px;
  border-right-color: #88b93d;
}
table.tb_evaluation tr th.status {
  border-right: none;
}
table.tb_evaluation tr td {
  padding: 0;
  margin: 0;
  text-align: center;
  height: 38px;
}
table.tb_evaluation tr td p {
  text-overflow: ellipsis;
  -o-text-overflow: ellipsis;
  -ms-text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  width: 100%;
  word-wrap: normal;
  width: 95%;
  margin: 0 auto;
}
div.five_evaluation {
  padding: 15px 0;
}
div.five_evaluation p {
  padding-left: 10px;
}
.get_account {
  width: 310px;
  background-color: #f6f6f6;
  border: 1px solid #e7e7e7;
  padding: 20px 15px;
}
.get_account dd.get_btn a,
.get_account dd.get_btn span {
  display: block;
  height: 42px;
  line-height: 42px;
}
.get_account dd.get_btn span {
  background-color: #cccccc;
  border-radius: 4px;
  color: #ffffff;
  font-size: 18px;
  text-align: center;
}
dl.promppt_info {
  padding-top: 15px;
  padding-bottom: 5px;
}
dl.promppt_info dt,
dl.promppt_info dd {
  padding-left: 10px;
  padding-bottom: 10px;
}
