dl.sort_select {
  height: 32px;
  border: 1px solid #cfcfcf;
  border-width: 2px 2px 1px 1px;
  position: relative;
  z-index: 3;
  border-radius: 2px;
}
dl.sort_select dt {
  text-align: center;
  vertical-align: middle;
  height: 32px;
  background-color: #fff;
  padding: 0 40px 0 0;
}
dl.sort_select dt input {
  height: 32px;
  line-height: 32px;
  float: left;
  border: 0;
  width: 100%;
  color: #343434;
  padding-left: 10px;
  background-color: transparent;
  text-align: left;
}
dl.sort_select dt span {
  width: 32px;
  height: 18px;
  padding-top: 14px;
  position: absolute;
  right: 0;
  top: 0;
  display: block;
  border-left: 1px solid #e6e6e6;
  font-size: 0;
  background-color: #f6f6f6 ;
  text-align: center;
  cursor: pointer;
  zoom: 1;
}
dl.sort_select dt span i {
  display: inline-block;
  font-size: 0;
  zoom: 1;
  width: 9px;
  height: 5px;
  overflow: hidden;
  -webkit-transition: all 0.2s linear;
  -moz-transition: all 0.2s linear;
  -ms-transition: all 0.2s linear;
  -o-transition: all 0.2s linear;
  transition: all 0.2s linear;
  -webkit-transform: rotate(0deg);
  -moz-transform: rotate(0deg);
  -ms-transform: rotate(0deg);
  -o-transform: rotate(0deg);
  transform: rotate(0deg);
  background: transparent url(../images/trangle.png) no-repeat;
}
dl.sort_select dt span.on i {
  -webkit-transform: rotate(180deg);
  -moz-transform: rotate(180deg);
  -ms-transform: rotate(180deg);
  -o-transform: rotate(180deg);
  transform: rotate(180deg);
}
dl.sort_select dd {
  position: absolute;
  top: 33px;
  left: -1px;
  background-color: #fff;
  width: 590px;
  border: 1px solid #e7e7e7;
  padding: 14px 15px 0 15px;
  -webkit-box-shadow: 1px 1px 5px #e8e8e8;
  -moz-box-shadow: 1px 1px 5px #e8e8e8;
  -ms-box-shadow: 1px 1px 5px #e8e8e8;
  box-shadow: 1px 1px 5px #e8e8e8;
}
dl.sort_select dd div.close {
  position: absolute;
  top: -10px;
  right: -10px;
  cursor: pointer;
}
dl.sort_select dd div.btn {
  position: absolute;
  top: 15px;
  right: 15px;
  cursor: pointer;
}
dl.sort_select dd div.btn a {
  padding: 0 15px;
}
dl.sort_select dd div.s_tt {
  overflow: hidden;
}
dl.sort_select dd div.s_tt ul {
  border-bottom: 1px solid #e7e7e7;
  height: 33px;
}
dl.sort_select dd div.s_tt ul li {
  float: left;
  position: relative;
  margin-right: 10px;
  height: 33px;
  line-height: 33px;
  font-size: 12px;
  border-width: 0 1px 1px 1px;
  border-color: #e7e7e7;
  border-style: solid;
}
dl.sort_select dd div.s_tt ul li a {
  display: block;
  padding: 0 25px 0 9px;
  border-width: 1px 0 0 0;
  border-color: #e7e7e7;
  border-style: solid;
}
dl.sort_select dd div.s_tt ul li a i {
  position: absolute;
  top: 14px;
  right: 8px;
  width: 0;
  height: 0;
  line-height: 0;
  font-size: 0;
  border-width: 4px 4px 0 4px;
  border-color: #ccc transparent transparent transparent;
  border-style: solid dashed;
}
dl.sort_select dd div.s_tt ul li a:hover {
  text-decoration: none;
  color: #2472b4;
}
dl.sort_select dd div.s_tt ul li.current {
  border-bottom-color: #fff;
}
dl.sort_select dd div.s_tt ul li.current a {
  border-top: 2px solid #7cb228;
  height: 31px;
  line-height: 30px;
  margin: 0 -1px;
}
dl.sort_select dd div.s_tt_con ul {
  overflow: hidden;
  padding-top: 10px;
}
dl.sort_select dd div.s_tt_con li {
  float: left;
  margin-bottom: 10px;
  height: 20px;
  line-height: 20px;
  width: 196px;
}
dl.sort_select dd div.s_tt_con li a {
  display: inline-block;
  padding: 0 5px;
  font-size: 12px;
}
dl.sort_select dd div.s_tt_con li a:hover,
dl.sort_select dd div.s_tt_con li a.current {
  background-color: #2c84cc;
  color: #fff;
  text-decoration: none;
}
dl.sort_select.focus {
  border-color: #7cb228;
}
dl.sort_select.error {
  border-color: #e85253;
}
.select-tree dd {
