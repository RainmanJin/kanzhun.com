#loading {
  position: absolute;
  z-index: 10;
  padding: 20px;
  left: 50%;
  top: 50%;
  margin: -22px 0 0 -22px;
  border-radius: 5px;
  background-color: rgba(0, 0, 0, 0.5);
}
#loading img {
  display: block;
}
.slide_up {
  position: absolute;
  bottom: .5rem;
  left: 50%;
  margin-left: -15px;
}
.slide_up i {
  display: inline-block;
  width: 30px;
  height: 30px;
  background: url("../images/slider.png") no-repeat;
  -webkit-background-size: 100%;
  background-size: 100%;
  -webkit-animation: arrowsUp 1s infinite linear;
}
@-webkit-keyframes arrowsUp {
  0% {
    -webkit-transform: translate(0, 0);
  }
  50% {
    -webkit-transform: translate(0, -5px);
  }
  100% {
    -webkit-transform: translate(0, 0);
  }
}
div.apply {
  height: 100%;
  position: relative;
}
div.apply .middle-content {
  margin-top: -10%;
}
.apply-progress {
  padding: 1.5rem 2rem;
}
.apply-progress dt {
  overflow: hidden;
  text-align: center;
}
.apply-progress dt.pg span {
  font-size: 1.5rem;
  text-align: center;
  height: 2rem;
  width: 2rem;
  line-height: 2rem;
  border: 1px solid #c5c4db;
  border-radius: 50%;
  color: #c5c4db;
  display: inline-block;
}
.apply-progress dt.pg span.current {
  border: 1px solid #00ffc9;
  color: #00ffc9;
}
.apply-progress dt.pg i {
  display: inline-block;
  width: 4rem;
  border-top: 1px solid #c5c4db;
  vertical-align: .5rem;
  height: 0;
  overflow: hidden;
}
.apply-progress dt.pg i.current {
  border-top: 1px solid #00ffc9;
}
.form-control {
  padding-top: .5rem;
}
.form-control label {
  color: #fff;
  line-height: 2em;
}
.form-control input[type=text] {
  background-color: #fff;
  border: 0;
  border-radius: .4rem;
  padding: .4rem .5rem;
  -webkit-box-sizing: border-box;
  -ms-box-sizing: border-box;
  box-sizing: border-box;
  width: 100%;
  margin-bottom: 1rem;
}
.form-control .err {
  text-align: center;
  color: #ff8080;
  padding: .8rem 0;
}
.form-control a.next {
  display: inline-block;
  font-size: 1.2rem;
  color: #00ffc9;
  border: 2px solid #00ffc9;
  background-color: rgba(0, 255, 201, 0.1);
  border-radius: .5rem;
  text-align: center;
  padding: .3rem 3rem;
}
.rule {
  position: absolute;
  bottom: 5px;
  text-align: center;
  width: 100%;
  color: #fff;
  font-size: 1.142rem;
}
.apply-end {
  text-align: center;
  color: #fff;
  font-size: 1.2rem;
}
.apply-end strong {
  font-weight: normal;
  font-size: 1.6rem;
}
.apply-end p:nth-of-type(1) {
  padding: 2rem 0 1rem;
}
.apply-end p:nth-of-type(2) {
  margin-bottom: 4rem;
}
.apply-success {
  padding: 2rem 2rem 0;
}
.apply-success dt {
  color: #00ffc9;
  font-size: 1.8rem;
}
.apply-success dd {
  font-size: 1.142rem;
}
.apply-success dd p:nth-of-type(1) {
  color: #fff;
}
.apply-success dd p:nth-of-type(1) em {
  color: #00ffc9;
}
.apply-success dd p:nth-of-type(2) {
  color: #00ffc9;
  text-align: center;
}
@media (device-width: 320px) and (-webkit-min-device-pixel-ratio: 2) {
  .form-control label {
    line-height: 1.5em;
  }
  .form-control input[type=text] {
    margin-bottom: .5rem;
  }
  .form-control .err {
    padding: .5rem 0;
  }
  .apply-success {
    padding: 1rem 2rem 0;
  }
  .apply-success dt {
    font-size: 1.5rem;
  }
  .apply-success dd {
    font-size: 1rem;
  }
}
@media screen and (max-height: 480px) {
  .rule {
    display: none;
  }
}
