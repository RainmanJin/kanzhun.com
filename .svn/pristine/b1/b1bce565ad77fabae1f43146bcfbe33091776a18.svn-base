html {
  height: 100%;
}
body {
  font: normal 14px/1.5 Arial, Helvetica, "Microsoft YaHei", STHeiti, "Droid Sans Fallback", sans-serif;
  color: #343434;
  height: 100%;
}
input,
button,
textarea,
select {
  font: normal 14px/1.5 Arial, Helvetica, "Microsoft YaHei", STHeiti, "Droid Sans Fallback", sans-serif;
}
a {
  color: #343434;
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
}
a.a_ul {
  text-decoration: underline;
}
h1,
h2,
h3,
h4,
h5,
h6 {
  font-size: 1em;
  line-height: 1;
}
.wrapper {
  width: 100%;
  min-height: 100%;
  height: auto;
}
body {
  background: url("../images/q1/content_bg.jpg") no-repeat #9bc3df;
  background-size: 100% 100%;
  overflow: hidden;
}
.swiper-container {
  height: 100%;
}
header {
  position: relative;
  margin-bottom: 60px;
  background-color: #4f93d0;
  height: 45px;
  color: #fff;
  text-align: center;
}
header h3 {
  line-height: 45px;
  font-size: 23px;
}
header .person {
  position: absolute;
  top: 10px;
  right: 20px;
  width: 25%;
}
header .logo {
  position: absolute;
  width: 13%;
  margin: 3%;
  top: 0px;
  left: 0px;
}
#page1 {
  /*background: url("../images/q1/content_bg.jpg") no-repeat;
  background-size: 100%;*/
  /*display: none;*/
  /* padding-top: @page_top;*/
  position: relative;
}
#page1 .indus-wrap {
  overflow: hidden;
}
#page1 h2 {
  font-size: 32px;
  color: #4b8ece;
  font-weight: bold;
  text-align: center;
  margin-bottom: 10px;
}
#page1 .indus {
  width: 90%;
  margin: 0 auto;
}
#page1 .indus li {
  text-align: center;
  float: left;
  width: 25%;
  box-sizing: border-box;
  padding: 1%;
  /*.icon1{
        background: url("../images/q1/icons1.png") #ffe628 no-repeat ;
        background-size: 100% 100%;
      }*/
}
#page1 .indus li div {
  background-color: #ffe628;
  border-radius: 5px;
  height: 80px;
  position: relative;
}
#page1 .indus li div p {
  position: absolute;
  bottom: 2px;
  text-align: center;
  font-size: 12px;
  width: 100%;
}
#page1 .indus li div:hover {
  background-color: #4b8ece;
  color: #ffffff;
}
#page1 .indus li img {
  width: 100%;
  display: block;
}
#page1 .indus li span {
  font-size: 12px;
}
#page2 {
  /*display: none;*/
  /* padding-top:@page_top;*/
  text-align: center;
}
#page2 h2 {
  font-size: 32px;
  color: #4b8ece;
  font-weight: bold;
  text-align: center;
  margin-bottom: 10px;
}
#page2 .days {
  width: 80%;
  margin: 0 auto;
}
#page2 a {
  display: block;
  width: 100%;
  font-size: 26px;
  font-weight: bolder;
  color: #666666;
  background-color: #ffe628;
  margin: 20px 0px;
  padding: 10px 0px;
  border-radius: 5px;
}
#page2 a:hover {
  color: #ffffff;
  background-color: #5095d1;
}
#page3 {
  /*display:none;*/
  padding-bottom: 100px;
  /* padding-top: @page_top;*/
}
#page3 h2 {
  font-size: 25px;
  color: #4b8ece;
  font-weight: bolder;
  text-align: center;
  margin-bottom: 10px;
}
#page3 .result {
  text-align: center;
}
#page3 .submit {
  background-color: #f27506;
  height: 60px;
  line-height: 60px;
  width: 40%;
  color: #fff;
  font-size: 21px;
  border-radius: 5px;
  display: inline-block;
}
#page3 .times {
  width: 80%;
  margin: 0 auto;
}
#page3 .times button {
  position: relative;
  background-color: #ffe628;
  margin-bottom: 15px;
  height: 60px;
  border-radius: 5px;
  width: 100%;
  text-align: left;
}
#page3 .times button i {
  color: #4c8fce;
  font-size: 25px;
  padding: 0px 1px;
}
#page3 .times button:hover {
  color: #ffffff;
  background-color: #4b8ece;
}
#page3 .times button:hover i {
  color: #ffe628;
}
#page3 .times button .word {
  padding-left: 15px;
  width: 60%;
  font-size: 20px;
  font-weight: bolder;
}
#page3 .times button .tip {
  height: 120%;
  position: absolute;
  bottom: 0px;
  right: 0px;
}
.slide_up {
  position: absolute;
  bottom: 20px;
  /*  left: 50%;
  margin-left: -13px;
  z-index: 20;*/
}
.slide_up i {
  display: inline-block;
  width: 25px;
  height: 15px;
  background: url("../images/up.png") no-repeat;
  background-size: 25px;
  -webkit-animation: arrowsUp 1s infinite linear;
}
@-webkit-keyframes arrowsUp {
  0% {
    -webkit-transform: translate(0, 0);
  }
  50% {
    -webkit-transform: translate(0, -5px);
  }
  100% {
    -webkit-transform: translate(0, 0);
  }
}
