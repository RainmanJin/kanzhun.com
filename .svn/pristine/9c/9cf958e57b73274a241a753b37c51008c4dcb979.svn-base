html,
body,
div,
ul,
ol,
li,
dl,
dt,
dd,
h1,
h2,
h3,
h4,
h5,
pre,
form,
p,
blockquote,
input,
textarea,
abbr,
article,
aside,
figcaption,
figure,
footer,
header,
mark,
meter,
menu,
nav,
output,
progress,
section,
summary,
time {
  margin: 0;
  padding: 0;
  vertical-align: baseline;
}
ul,
ol {
  list-style: none;
}
iframe,
a,
img,
button {
  border: 0;
}
table {
  border-collapse: collapse;
  border-spacing: 0;
  border: 0;
}
article,
aside,
footer,
header,
nav,
section,
figure,
figcaption {
  display: block;
}
a,
a:hover {
  text-decoration: none;
}
.clear {
  clear: both;
}
.fleft {
  float: left;
}
.fright {
  float: right;
}
.clearfix:before,
.clearfix:after {
  content: "";
  display: table;
}
.clearfix:after {
  clear: both;
  overflow: hidden;
}
.none {
  display: none;
}
.t-left {
  text-align: left;
}
.t-center {
  text-align: center;
}
.t-right {
  text-align: right;
}
.inline-block {
  display: inline-block;
}
.middle {
  vertical-align: middle;
}
em,
i,
s {
  font-style: normal;
}
body {
  word-wrap: break-word;
  -webkit-text-size-adjust: none;
}
input[type=submit],
input[type=button],
input[type=text],
textarea,
button,
select {
  font-size: 1em;
  outline: none;
  -webkit-appearance: none;
  border-radius: 0;
}
::-webkit-scrollbar {
  width: 0;
  height: 0;
}
body,
html {
  color: #fff;
  height: 100%;
}
.swiper-container {
  height: 100%;
}
.swiper-wrapper {
  height: 100%;
}
.page {
  width: 100%;
  height: 100%;
  position: relative;
}
#page3 {
  z-index: 2;
}
#page4 {
  width: 100%;
  background: url("../images/page4.jpg") no-repeat #9db9d6;
}
#page4 .content {
  width: 100%;
  text-align: center;
}
#page4 .content .addr {
  width: 90%;
}
@media screen and (min-height: 480px) {
  #page4 .content .addr {
    width: 90%;
    margin-top: 20px;
  }
}
.sign_wrap {
  position: absolute;
  text-align: center;
  width: 100%;
  top: 74%;
}
.sign_wrap .wrap {
  width: 60%;
  margin: 0 auto;
  padding-bottom: 2px;
  border: 1px solid #bfd1e4;
  border-radius: 20px;
}
.sign_wrap .wrap .sign {
  width: 98%;
  margin: 0 auto;
  background-color: #fff;
  border-radius: 16px;
  padding: 8px 0px;
  color: #739fd0;
  display: block;
  font-size: 20px;
  font-weight: bolder;
}
.sign_wrap img {
  width: 20%;
  margin-top: 10px;
}
#page5 {
  background: url("../images/end_bg.jpg") no-repeat #5073a9;
  background-size: 100%;
}
#link .a_wrap {
  width: 60%;
  margin: 0 auto;
  text-align: center;
  margin-top: 90%;
}
#link .a_wrap img {
  width: 45px;
  margin-right: 10px;
}
#cooperation {
  text-align: center;
  color: #fff;
  margin-top: 20px;
}
#url {
  width: 30%;
  margin: 0 auto;
  margin-top: 40px;
}
#url img {
  width: 100%;
}
.slide_up {
  position: absolute;
  bottom: 10px;
  left: 50%;
  margin-left: -13px;
  z-index: 20;
}
.slide_up i {
  display: inline-block;
  width: 25px;
  height: 15px;
  background: url("../images/up.png") no-repeat;
  background-size: 25px;
  -webkit-animation: arrowsUp 1s infinite linear;
}
@-webkit-keyframes arrowsUp {
  0% {
    -webkit-transform: translate(0, 0);
  }
  50% {
    -webkit-transform: translate(0, -5px);
  }
  100% {
    -webkit-transform: translate(0, 0);
  }
}
