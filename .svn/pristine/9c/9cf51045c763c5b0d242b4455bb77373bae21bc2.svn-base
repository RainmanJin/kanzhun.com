html {
  height: 100%;
}
body {
  font: normal 14px/1.5 Arial, Helvetica, "Microsoft YaHei", STHeiti, "Droid Sans Fallback", sans-serif;
  color: #343434;
  height: 100%;
}
input,
button,
textarea,
select {
  font: normal 14px/1.5 Arial, Helvetica, "Microsoft YaHei", STHeiti, "Droid Sans Fallback", sans-serif;
}
a {
  color: #343434;
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
}
a.a_ul {
  text-decoration: underline;
}
h1,
h2,
h3,
h4,
h5,
h6 {
  font-size: 1em;
  line-height: 1;
}
html {
  font-size: 14px;
}
body {
  background: url("../images/rank_bg.jpg") #06488e no-repeat top center;
  background-size: 100% auto;
  position: absolute;
  overflow: hidden;
}
.nav {
  height: 30px;
  line-height: 30px;
  background-color: #fefefe;
  width: 100%;
}
.nav .home {
  margin-left: 15px;
}
.nav .home img {
  width: 50px;
  vertical-align: top;
}
.nav .l_trangle {
  width: 0;
  height: 0;
  border: 5px dashed transparent;
  border-right: 5px dashed #333333;
  display: inline-block;
  margin-right: 5px;
}
.nav .tip {
  font-size: 14px;
  color: #333333;
  font-weight: bold;
}
.content {
  position: relative;
}
.content .title {
  margin-top: 50px;
  text-align: center;
}
.content .title .mark {
  width: 80%;
}
.content .title .vertical {
  margin-top: -20px;
  width: 75%;
}
.content .title .word {
  position: relative;
  top: -1.6em;
  font-size: 1.5rem;
  color: red;
  font-weight: bold;
}
.content .reasons {
  padding-bottom: 40px;
}
.content .reasons ul li {
  background-color: #ffffff;
  border-radius: 10px;
  width: 90%;
  margin: 0 auto;
  margin-bottom: 10px;
  overflow: hidden;
  position: relative;
}
.content .reasons ul li img {
  vertical-align: top;
  width: 66px;
  float: left;
  padding: 10px;
}
.content .reasons ul li .arrow {
  position: absolute;
  right: 5px;
  height: 30px;
  width: 20px;
  top: 50%;
  margin-top: -25px;
}
.content .reasons ul li .h2 {
  font-size: 1.5rem;
  font-weight: bolder;
}
.foot {
  position: fixed;
  bottom: 0px;
  width: 100%;
}
.foot a {
  display: inline-block;
  font-size: 1.5rem;
  font-weight: bolder;
  text-align: center;
  color: #ffffff;
  padding: 10px 0px;
  margin: 0px;
  border: 0px;
  width: 50%;
}
.foot .share {
  background-color: #4564ba;
}
.foot .company {
  background-color: #3d3d8b;
}
.popup-wrap {
  position: fixed;
  overflow: hidden;
  bottom: 51px;
  right: 10px;
  width: 50%;
  height: 180px;
}
.popup-wrap .popup {
  position: relative;
  top: 180px;
}
.popup-wrap .popup p {
  border-radius: 6px;
  background-color: #333333;
  opacity: 0.6;
  margin-bottom: 10px;
  padding-bottom: 10px;
  color: #ffffff;
}
.popup-wrap .popup p img {
  float: left;
  width: 40px;
  margin: 10px;
}
.circle {
  width: 40px;
  height: 40px;
  border: 5px solid #333333;
  border-radius: 50%;
  background-color: #ffffff;
  position: fixed;
  bottom: 240px;
  right: 10px;
  text-align: center;
  font-weight: bolder;
  font-size: 2rem;
}
/*三角  上 up   下down 左left 右right */
