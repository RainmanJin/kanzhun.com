@import "common/var";
@import "common/reset";
@import "common/common";
//image 目录
//@img_path:"../images/"; //fe目录
@img_path:"/images/activity/enterprise/"; //工程目录
body{
  &.index{
    background: url("@{img_path}bg_1.png") repeat;
    .top_section{
      position: relative;
      img{
        display: block;
      }
      .img_1{
        position: absolute;
        top:0;
      }
    }
    .bottom_section{
      padding: 10px ;
      p{
        margin-top: 8%;
        color: #27585c;
        text-align: center;
      }
    }
  }
  &.quest{
    background-color: #487e82;
    .wrapper{
      background-color: #005752;
      border: 1px solid #362e2b;
      border-radius: 25px;
      margin: 15px 10px 25px;
      padding: 10px;
      position: relative;
      .timing{
        position:absolute;
        top: 0;
        left: 50%;
        margin-left: -76px;
        background-color: #313131;
        color: #cdcdcd;
        font-size: 20px;
        padding: 5px;
        em{
          background-color: #535353;
          display: inline-block;
          width: 19px;
          height: 28px;
          line-height: 28px;
          text-align: center;
          margin: 0 1px;
          border-radius: 3px;
          position: relative;
          float: left;
          i{
            display: block;
            width: 100%;
            border-bottom: 1px solid #313131;
            position: absolute;
            top: 50%;
            z-index: 0;
          }
          span{
            position: relative;
            z-index: 1;
          }
        }
        >span{
          float: left;
          margin: 0 1px;
          color: #535353;
          font-weight: bold;
        }
      }
      .header{
        height: 10px;
        background-color: #d2d2d2;
        border-radius: 8px;
        .line{
          padding-top:3px;
          margin: 0 5px;
          border-bottom: 2px solid #959595;
        }
      }
      .c {
        background-color: #fff;
        padding: 25px 10px 20px;
        margin: -5px 8px 10px;
        .q{
          color: #ec6941;
          font-size: 16px;
          i{
            display: inline-block;
            width: 32px;
            height: 32px;
            line-height: 32px;
            text-align: center;
            background-color: #ec6941;
            border-radius: 50%;
            color: #fff;
            margin-right: 8px;
          }
        }
        .q_img{
          border: 1px dashed #ec6941;
          margin: 35px 15px 0;
          img{
            display: block;
            width: 100%;
          }
        }
        .answers{
          margin-top: 15px;
          li{
            a{
              display: block;
              background-color: #fae7df;
              padding: 5px;
              color: #ec6941;
              i{
                display: inline-block;
                background-color: #ffddcf;
                border-radius: 50%;
                width: 22px;
                height: 22px;
                border: 6px solid #fff;
                .box-sizing();
                vertical-align:middle;
                float: left;
                margin-right: 5px;
              }
              border-bottom: 1px solid #ec6941;
              border-radius: 3px;
            }
            &:last-of-type{
              a{
                border-bottom: 0;
              }
            }
            &.on{
              a{
                background-color: #f3c8b6;
                i{
                  background-color: #fdb99d;
                }
              }
            }
          }
        }
      }
      .b_l{
        position: absolute;
        left: 10%;
        bottom: -22px;
      }
      .b_r{
        position: absolute;
        right: 10%;
        bottom: -22px;
      }
      .result{
        background: #fff;
        padding: 40px 10px 10px;
        margin: -5px 8px 10px;
        h2{
          img{
            display: block;
          }
        }
        table{
          margin-top: 20px;
          width: 100%;
          tr {
            td {
              border: 1px solid #487e82;
              height: 23px;
              text-align: center;
              color: #005752;
              img{
                vertical-align: middle;
              }
            }
          }
        }
        p{
          text-align: center;
          font-size: 18px;
          color: #959595;
          &.p1{
            margin-top: 35px;
            span{
              color: #e5865e;
            }
            em{
              font-size: 24px;
              color: #e5865e;
            }
          }
          &.p2{
            margin-top: 15px;
          }
          &.p3{
            margin-top: 15px;
            color: #ffb190;
            font-size: 14px;
          }
        }
      }
    }
  }
}
.btn_enter{
  position: relative;
  z-index: 2;
  background-color: #27585c;
  height: 45px;
  line-height: 45px;
  display: block;
  text-align: center;
  font-size: 20px;
  font-weight: bold;
  color: #fff;
  cursor: pointer;
  white-space: nowrap;
  border: 0;
  border-radius: 4px;
  &.orange{
    background-color: #e5865e;
  }
}
.i{
  display: inline-block;
  &.icon_1{
    background: url('@{img_path}img_2.png') no-repeat;
    background-size: 100%;
    width: 22px;
    height: 22px;
  }
  &.icon_2{
    background: url('@{img_path}img_4.png') no-repeat;
    background-size: 100%;
    width: 24px;
    height: 22px;
  }
}




//转发提示
.mask{
  position: fixed;
  z-index: 9;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, .8);
}
.transmitDialog{
  position: fixed;
  z-index: 10;
  top: 150px;
  min-height: 81px;
  left: 50%;
  margin-left: -124px;
  margin-top: -21px;
  background: url('@{img_path}share_bg.png') no-repeat;
  background-size: 248px;
  width: 248px;
  font-size: 18px;
  i.arrows{
    display: inline-block;
    background: url('@{img_path}share_up.png') no-repeat;
    background-size: 75px;
    width: 75px;
    height: 84px;
    position: absolute;
    top: -80px;
    right: -20px;
    //-webkit-animation: arrowsUp 1s infinite linear;
  }
  p{
    margin-top: 10px;
    color: #fff;
    text-align: center;
  }
}
@-webkit-keyframes arrowsUp{
  0%{
    -webkit-transform: translate(0,0);
  }
  50%{
    -webkit-transform: translate(10px,-10px);
  }
  100%{
    -webkit-transform: translate(0,0);
  }
}