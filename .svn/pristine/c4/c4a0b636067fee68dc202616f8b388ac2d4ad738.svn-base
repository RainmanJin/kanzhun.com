aside.prompt {
  padding: 10px;
  margin-top: 15px;
  background-color: #fcf5e1;
  border: 1px solid #feebb7;
  border-radius: 5px;
}
aside.prompt p {
  color: #999999;
}
label.ttlabel {
  display: inline-block;
  width: 4em;
  margin-right: 15px;
  font-size: 14px;
  height: 34px;
  line-height: 34px;
  text-align: right;
  vertical-align: middle;
}
div.searchinfo {
  padding: 0 20px;
  height: 50px;
  line-height: 50px;
  background-color: #f3f3f4;
  color: #999999;
  font-size: 14px;
  margin-bottom: 20px;
}
div.searchinfo p a {
  text-decoration: none;
}
.line_only {
  border-bottom: 1px solid #e7e7e7;
  height: 0;
  overflow: hidden;
}
.line {
  border-bottom: 1px solid #e7e7e7;
  height: 0;
  overflow: hidden;
  margin: 15px 0;
}
.promppt_info {
  background-color: #fffedf;
  border-left: 4px solid #ffc962;
}
h3.p_tt {
  position: relative;
  padding-left: 8px;
  border-left: 4px solid #7cb228;
  height: 1em;
  line-height: 1em;
  font-size: 16px;
  zoom: 1;
}
h3.p_tt span.explain {
  font-weight: normal;
}
.tangle_init {
  width: 0;
  height: 0;
  line-height: 0;
  font-size: 0;
}
@-webkit-keyframes twinkle {
  0% {
    background-color: #fff;
  }
  20% {
    background-color: #eaacac;
  }
  40% {
    background-color: #fff;
  }
  60% {
    background-color: #eaacac;
  }
  80% {
    background-color: #fff;
  }
  100% {
    background-color: #eaacac;
  }
}
@-moz-keyframes twinkle {
  0% {
    background-color: #fff;
  }
  20% {
    background-color: #eaacac;
  }
  40% {
    background-color: #fff;
  }
  60% {
    background-color: #eaacac;
  }
  80% {
    background-color: #fff;
  }
  100% {
    background-color: #eaacac;
  }
}
@-ms-keyframes twinkle {
  0% {
    background-color: #fff;
  }
  20% {
    background-color: #eaacac;
  }
  40% {
    background-color: #fff;
  }
  60% {
    background-color: #eaacac;
  }
  80% {
    background-color: #fff;
  }
  100% {
    background-color: #eaacac;
  }
}
@keyframes twinkle {
  0% {
    background-color: #fff;
  }
  20% {
    background-color: #eaacac;
  }
  40% {
    background-color: #fff;
  }
  60% {
    background-color: #eaacac;
  }
  80% {
    background-color: #fff;
  }
  100% {
    background-color: #eaacac;
  }
}
.shine {
  -webkit-animation: twinkle 0.6s ease none;
  -moz-animation: twinkle 0.6s ease none;
  -ms-animation: twinkle 0.6s ease none;
  -o-animation: twinkle 0.6s ease none;
  animation: twinkle 0.6s ease none;
}
