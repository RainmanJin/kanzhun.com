      {{~it.investigationQuestionInfoList: value:index}}
      {{var qType = value.type;}}

      <dl>
        <dt><strong>{{=value.name}}</strong></dt>
        {{var name = value.id;}}

          {{~value.investigationAnswerInfoList: v:i}}
            {{var id = v.id;}}

            {{?qType == 1}}
            <dd><input type="radio" disabled name="q_{{=name}}"><label>{{=v.name}}</label>

            {{??qType == 2}}
            <dd><input type="checkbox" disabled name="q_{{=name}}"><label>{{=v.name}}</label>
            {{?}}

			{{var color = parseInt(i) + 1;}}
            <div class="ratio">
              <p><span style="width: {{=v.percentage}}%;" class="ratio-{{=color}}"></span></p>
              <strong>{{=v.percentage}}%</strong>
            </div>
            </dd>
          {{~}}
      </dl>
      {{~}}
    </script>

