html,
body,
div,
ul,
ol,
li,
dl,
dt,
dd,
h1,
h2,
h3,
h4,
h5,
pre,
form,
p,
blockquote,
input,
textarea,
abbr,
article,
aside,
figcaption,
figure,
footer,
header,
mark,
meter,
menu,
nav,
output,
progress,
section,
summary,
time {
  margin: 0;
  padding: 0;
  vertical-align: baseline;
}
ul,
ol {
  list-style: none;
}
iframe,
a,
img,
button {
  border: 0;
}
table {
  border-collapse: collapse;
  border-spacing: 0;
}
article,
aside,
footer,
header,
hgroup,
nav,
section,
figure,
figcaption {
  display: block;
}
a,
a:hover {
  text-decoration: none;
}
.clear {
  clear: both;
}
.fleft {
  float: left;
}
.fright {
  float: right;
}
.clearfix:before,
.clearfix:after {
  content: "";
  display: table;
}
.clearfix:after {
  clear: both;
  overflow: hidden;
}
.hidden {
  display: none;
}
.t-left {
  text-align: left;
}
.t-center {
  text-align: center;
}
.t-right {
  text-align: right;
}
em,
i,
s {
  font-style: normal;
}
body {
  word-wrap: break-word;
  -webkit-text-size-adjust: none;
}
input[type=submit],
input[type=button],
input[type=text],
textarea,
button,
select {
  font-size: 1em;
  outline: none;
  -webkit-appearance: none;
}
::-webkit-scrollbar {
  width: 0;
  height: 0;
}
.mt5 {
  margin-top: 5px;
}
.mt10 {
  margin-top: 10px;
}
.mt15 {
  margin-top: 15px;
}
.mt20 {
  margin-top: 20px;
}
.mb10 {
  margin-bottom: 10px;
}
.mb15 {
  margin-bottom: 15px;
}
.mb20 {
  margin-bottom: 20px;
}
.ml5 {
  margin-left: 5px;
}
.ml10 {
  margin-left: 10px;
}
.ml15 {
  margin-left: 15px;
}
.ml20 {
  margin-left: 20px;
}
.mr10 {
  margin-right: 10px;
}
.mr15 {
  margin-right: 15px;
}
.mr20 {
  margin-right: 20px;
}
.pt10 {
  padding-top: 10px;
}
.pt15 {
  padding-top: 15px;
}
.p20 {
  padding: 20px;
}
.p10 {
  padding: 10px;
}
.green {
  color: #7cb228;
}
.grey {
  color: #999999;
}
.blue {
  color: #26abbe;
}
.red {
  color: #ff0000;
}
.orange {
  color: #fd9f25;
}
.f12 {
  font-size: 12px;
}
.f16 {
  font-size: 16px;
}
.f18 {
  font-size: 18px;
}
.f20 {
  font-size: 20px;
}
.f22 {
  font-size: 22px;
}
input:-webkit-input-placeholder {
  color: #999999;
}
input:-moz-placeholder {
  color: #999999;
}
input:-ms-input-placeholder {
  color: #999999;
}
.cube {
  -webkit-transform: translateZ(0);
  -moz-transform: translateZ(0);
  -ms-transform: translateZ(0);
  -o-transform: translateZ(0);
  transform: translateZ(0);
}
.display-flex {
  display: -webkit-flex;
  display: flex;
}
.box-sizing {
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  -ms-box-sizing: border-box;
  box-sizing: border-box;
}
html {
  height: 100%;
}
body {
  font: normal 14px/1.5 Arial, Helvetica, "Microsoft YaHei", STHeiti, "Droid Sans Fallback", sans-serif;
  color: #343434;
  overflow-x: hidden;
  height: 100%;
}
a {
  color: #343434;
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
}
a.blue {
  color: #26abbe;
}
h1,
h2,
h3 {
  font-size: 14px;
  line-height: 1;
}
.wrapper {
  width: 100%;
}
body {
  background: transparent url(../images/bg.jpg) repeat-y;
  -webkit-background-size: 100%;
  -moz-background-size: 100%;
  -o-background-size: 100%;
  background-size: 100%;
}
body.que {
  background: transparent url(../images/bg2.jpg) repeat-y;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  -ms-box-sizing: border-box;
  box-sizing: border-box;
  padding: 10px;
}
.wrapper {
  min-height: 100%;
  height: auto;
}
.wrapper2 {
  min-height: 100%;
  height: auto;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  -ms-box-sizing: border-box;
  box-sizing: border-box;
  padding: 5px;
  background-color: #fff;
}
section.index {
  padding: 200px 10px 20px;
  background: url(../images/flower.jpg) no-repeat;
  -webkit-background-size: 100%;
  -moz-background-size: 100%;
  -o-background-size: 100%;
  background-size: 100%;
}
section.index p {
  text-align: center;
  color: #ffffff;
  font-size: 15px;
  line-height: 1.8em;
  padding-bottom: 40px;
}
section.content {
  background-color: #ffffff;
  border: 1px dashed #be2aa4;
}
section.content .con {
  padding: 0 15px 40px;
}
section.content header {
  color: #be2aa4;
  font-size: 18px;
  text-align: center;
  border-bottom: 1px solid #be2aa4;
}
section.content header strong {
  font-size: 22px;
  color: #5f17a7;
  font-weight: normal;
}
section.content header b {
  font-size: 40px;
  color: #be2aa4;
}
section.content .progress {
  width: 2.5em;
  height: 2.5em;
  background-color: #be2aa4;
  color: #ffffff;
  line-height: 2.5em;
  text-align: center;
  font-size: 16px;
  margin-right: 10px;
  border-radius: 1.25em;
  float: left;
}
section.content ul.answers {
  outline: none;
  margin-top: 20px;
  margin-bottom: 20px;
}
section.content ul.answers li {
  font-size: 13px;
}
section.content ul.answers li:last-child {
  border: 0;
}
section.content ul.answers li a {
  background: #f2def3;
  outline: none;
  border-radius: 3px;
  border-bottom: 1px solid #ca9ccc;
  padding: 10px;
  display: block;
  overflow: hidden;
}
section.content ul.answers li a i {
  float: left;
  margin-right: 10px;
  width: 12px;
  margin-top: 4px;
  height: 12px;
  border: 8px solid #ffffff;
  background-color: #d4b2d5;
  border-radius: 20px;
}
section.content ul.answers li.on {
  background-color: #deabe0;
}
section.content ul.answers li.on a i {
  background-color: #be2aa4;
}
section.content .q {
  color: #7a7a7a;
  padding: 10px 0;
  font-size: 16px;
  overflow: hidden;
}
section.content .tt {
  overflow: hidden;
  padding: 10px 0;
}
section.content .tt img {
  float: left;
  width: 145px;
}
section.content .tt p {
  margin-left: 150px;
  padding-top: 30px;
}
section.content .tt p strong {
  color: #8221ac;
  margin-top: 5px;
  font-size: 30px;
  font-weight: normal;
}
section.content .intro {
  color: #7a7a7a;
  padding: 10px 0;
}
.btn {
  border: 0;
  clear: both;
  display: block;
  background-color: #8221ac;
  padding: 5px 0;
  text-align: center;
  color: #ffffff;
  border-radius: 5px;
  width: 100%;
  font: bold 20px/1.5 Arial, Helvetica, "Microsoft YaHei", STHeiti, "Droid Sans Fallback", sans-serif;
}
.mask {
  position: fixed;
  z-index: 9;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.8);
}
.transmitDialog {
  position: fixed;
  z-index: 10;
  top: 150px;
  min-height: 150px;
  left: 50%;
  margin-left: -128px;
  margin-top: -61px;
  background: url(../images/oval_bg.png) no-repeat;
  background-size: 257px;
  width: 257px;
}
.transmitDialog i.arrows {
  display: inline-block;
  background: url(../images/arrows.png) no-repeat;
  background-size: 63px;
  width: 63px;
  height: 65px;
  position: absolute;
  top: -70px;
  right: -5px;
  -webkit-animation: arrowsUp 1s infinite linear;
}
.transmitDialog p {
  margin-top: 30px;
  color: #fff;
  text-align: center;
}
@-webkit-keyframes arrowsUp {
  0% {
    -webkit-transform: translate(0, 0);
  }
  50% {
    -webkit-transform: translate(10px, -10px);
  }
  100% {
    -webkit-transform: translate(0, 0);
  }
}
