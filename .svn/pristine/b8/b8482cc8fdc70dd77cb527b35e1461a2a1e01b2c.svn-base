
.indy_comment ul.industry{
  list-style:disc inside url('../images/dot.jpg');
}

.indy_comment ul.industrys{
  display: none;
}

.indy_comment ul.comment{
  margin: 0px 0px 20px 0px;
}

.indy_comment ul.industry li{
  display: list-item;
  width: 120px;
  margin-bottom: 15px;
  float: left;
}


.scroll_wrap{
  /*height: 400px;*/
  overflow: hidden;
  margin-bottom: 25px;
  position: relative;
}
.scroll_inner{
  position: relative;
}

.indy_comment ul.comment li{
  border-bottom: 1px dashed #e3e3e3;
  padding-bottom: 10px;
  margin-bottom: 10px;
  font-size: 14px;
  color:rgb(51,51,51);
}

.indy_comment ul.industry a,.indy_comment ul.comment a{
  color: #343434;
}

.indy_comment ul.comment a{
  padding-left: 25px;
  display: block;
}

.indy_comment .c1{
  margin-left: 10px;
  margin-bottom: 20px;
}
.indy_comment span.from{
  font-size: 12px;
  font-family: "Microsoft YaHei";
  color: rgb( 153, 153, 153 );
  line-height: 1.667;
  text-align: left;
  height: 13px;
}


.toggle{
  position: relative;
  cursor: pointer;
  margin-top: 10px;
  color: rgb( 153, 153, 153 );
  margin-bottom: 20px;
  font-size: 12px;
  font-family: "Microsoft YaHei";
}

.comment li i.s{ /*薪资icon*/
    margin: 5px 5px 0px 0px;
    width: 20px;
    height:21px;
    font-size: 0;
    float:left;
    background: url("../images/icons.png") no-repeat 0px -580px;
}

