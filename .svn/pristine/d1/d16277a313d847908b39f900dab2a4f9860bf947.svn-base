    <div class="swiper-slide page s1">
        <span class="middle-helper">
        </span><div class="wrap middle-content">
      <header>
        <img src="images/mt-tt.png">
      </header>

      <section class="white-box">
        <img src="images/box.png" class="box-bg">

        <div class="box-con">
          <div>
            <ul class="page1 effect pe1">
              <li>我是<em class="ml10 zoomIn2">张磊</em></li>
              <li>我是<em class="ml10 zoomIn2">看准网VP</em></li>
              <li>我正在参加</li>
