@import "../../../../css/common/var";
@import "../../var";

.banner_bg{
  background: url("@{img}/activity/jinping/wel_banner.jpg") center no-repeat;
  height: 925px;
  .banner_content{
    padding-top: 520px;
    div.slipTime{
      text-align: center;
      em {
        background: url("@{img}/activity/jinping/time_bg.png") no-repeat;
        display: inline-block;
        width: 128px;
        height: 116px;
        line-height: 116px;
        vertical-align: bottom;
        font-size: 88px;
        font-weight: bold;
        color: #fff;
        position: relative;
        i.i_line{
          position: absolute;
          top: 56px;
          left: 15px;
          display: inline-block;
          width: 100px;
          font-size: 0;
          height: 0;
          border-top: 1px solid rgba(0,0,0,.1);
          border-bottom: 1px solid rgba(0,0,0,.2);
        }
      }
      span{
        color: #dfdfdf;
        font-size: 42px;
        font-weight: bold;
      }
    }
  }
  p.p1{
    font-size: 78px;
    text-align: center;
    color: #faf9c0;
  }
}
