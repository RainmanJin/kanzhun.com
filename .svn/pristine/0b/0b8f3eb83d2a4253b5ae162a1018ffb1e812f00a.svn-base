html {
  height: 100%;
}
body {
  font: normal 14px/1.5 Arial, Helvetica, "Microsoft YaHei", STHeiti, "Droid Sans Fallback", sans-serif;
  color: #343434;
  height: 100%;
}
input,
button,
textarea,
select {
  font: normal 14px/1.5 Arial, Helvetica, "Microsoft YaHei", STHeiti, "Droid Sans Fallback", sans-serif;
}
a {
  color: #343434;
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
}
a.a_ul {
  text-decoration: underline;
}
h1,
h2,
h3,
h4,
h5,
h6 {
  font-size: 1em;
  line-height: 1;
}
html {
  font-size: 14px;
}
body {
  background: url("../images/step_bg.jpg") #06488e no-repeat top center;
  background-size: 100% auto;
}
.content {
  position: relative;
}
.content .title {
  padding-top: 30px;
  text-align: center;
}
.content .title .mark {
  width: 80%;
}
.content .title .vertical {
  margin-top: -20px;
  width: 75%;
}
.content .title .word {
  position: relative;
  top: -1.8em;
  font-size: 1.5rem;
  color: red;
  font-weight: bold;
}
.content .form {
  width: 80%;
  margin: 0 auto;
}
.content .form input[type=text] {
  width: 100%;
  background-color: #fefefe;
  border: 0px;
  padding: 10px 5px;
  font-size: 1.2rem;
  color: #999999;
  margin-top: -10px;
}
.content .form .tip {
  color: #fefefe;
  margin: 5px 0px 20px 0px;
}
.content .form .center-wrap {
  text-align: center;
  color: #fff;
}
.content .form .center-wrap .h2 {
  font-size: 1.3rem;
  font-weight: bold;
}
.content .form .center-wrap .re-wrap {
  border: 1px solid #ffffff;
  border-radius: 6px;
  background-color: rgba(124, 178, 40, 0);
  width: 60%;
  margin: 5px auto;
  padding: 10px 0px;
  font-size: 1.2rem;
  position: relative;
}
.content .form .center-wrap .re-wrap label {
  position: relative;
  z-index: 3;
}
.content .form .center-wrap .re-wrap input[type=radio] {
  display: none;
}
.content .form .center-wrap .re-wrap input:checked ~ .bg {
  background-color: #ffaa12;
  border: 1px solid #ffaa12;
}
.content .form .center-wrap .re-wrap .bg {
  position: absolute;
  width: 100%;
  height: 100%;
  top: -1px;
  left: -1px;
  border-radius: 6px;
}
.content .form .center-wrap .next {
  color: #fff;
  background-color: #e77200;
  width: 50%;
  margin: 0 auto;
  margin-top: 10px;
  padding: 10px 0px;
  display: inline-block;
  border-radius: 6px;
  font-size: 1.3rem;
  font-weight: bold;
}
