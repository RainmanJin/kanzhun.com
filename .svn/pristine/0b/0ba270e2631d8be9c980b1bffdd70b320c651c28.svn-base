div.wrap{width:1199px;margin:0 auto}div.bg_one{height:650px;background:url(../images/temp/index_img_2.jpg) no-repeat left top}div.bg_two{height:1070px;background:url(../images/temp/index_img_4.jpg) no-repeat left top}section.header{height:240px;background:url(../images/temp/index_img_1.jpg) no-repeat left top;padding:140px 0 30px 0}section.header p{width:26em;margin:0 auto;font-size:16px;color:#e1ebfe;text-align:center}section.header div{padding-top:40px;text-align:center}section.header div a{display:inline-block;width:315px;height:64px;line-height:64px;color:#019875;font-size:24px;background-color:#f2f2f2;border-radius:5px;-webkit-box-shadow:0 1px 2px #000;-moz-box-shadow:0 1px 2px #000;-ms-box-shadow:0 1px 2px #000;box-shadow:0 1px 2px #000;-webkit-transition:all .1s linear;-moz-transition:all .1s linear;-ms-transition:all .1s linear;-o-transition:all .1s linear;transition:all .1s linear}section.header div a i{display:inline-block;width:22px;height:26px;font-size:0;line-height:0;overflow:hidden;background:url(../images/icons.png) no-repeat -31px top;vertical-align:-4px}section.header div a:hover{background-color:#019875;color:#fff;text-decoration:none;-webkit-box-shadow:0 0 0 #000;-moz-box-shadow:0 0 0 #000;-ms-box-shadow:0 0 0 #000;box-shadow:0 0 0 #000}section.terminus{height:240px;padding:540px 0 0 525px;background:url(../images/temp/index_img_3.jpg) no-repeat left top}section.terminus h1{font-size:50px;text-align:right;padding-right:118px}section.terminus p{width:26em;color:#e1ebfe;text-align:right;font-size:22px}section.terminus p.info{font-size:16px;padding-left:10em}section.terminus_two{height:1390px;background:url(../images/temp/index_img_5.jpg) no-repeat left top}section.terminus_two h1{font-size:50px}section.terminus_two p{width:26em;margin:25px auto 0 auto;color:#e1ebfe;text-align:center;font-size:22px}section.terminus_two p.info{font-size:16px}h1.title{font-size:66px;font-weight:bold;text-align:center;-webkit-text-shadow:0 2px 2px #000;-moz-text-shadow:0 2px 2px #000;-ms-text-shadow:0 2px 2px #000;text-shadow:0 2px 2px #000}