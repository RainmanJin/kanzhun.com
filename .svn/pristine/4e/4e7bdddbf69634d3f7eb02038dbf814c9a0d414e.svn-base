@import "common/var";
@import "modules/company/common";
@import "modules/dialog";
@import "modules-v2/maskui-v3";  //第三种弹窗样式
@zhiji_icons: url(../images/zhiji_icons.png);
.body{
	background-color:#eaf2f3;
}

div {
  &.h_rop {
    width: 100%;
    background-color: #62bb6a;

    div {
      width: 990px;
      margin: 0 auto;
      height: 30px;
      line-height: 30px;

      a {
        color: #fff;
      }

      em {
        color: #9ad5a0;
        margin: 0 10px;
        vertical-align: 1px;
      }
    }
  }
}

header.zhiji {
  width: 100%;
  background-color: #fff;
  border-bottom: 1px solid #dddddd;
  z-index: 20;

  div {
    width: 990px;
    margin: 0 auto;
    padding: 15px 0;

    a {
      float: left;
      width: 125px;
      height: 48px;
      margin-right: 30px;
    }

    span{
      font-size: 18px;
      margin-top: 15px;
      float: right;
    }
  }
}

//首页
section.index_info{
	width: 990px;
	position: relative;
	margin: 15px auto;
	text-align: center;
	color: @grey_99;
	a.close{
		position: absolute;
		top: 2px;
		right: 210px;
		width: 20px;
		height: 20px;
		display: inline-block;
		background: url(../images/close.png) no-repeat;

    .transition(all .2s linear);
    .transform(rotate(0deg));


    &:hover{
      .transform(rotate(90deg));
    }

	}
	p{
		a{
			font-size: 16px;
		}
	}
}
section.banner{
	min-width: 990px;
	height: 528px;
	margin: 0 auto;
	background: url(../images/zhiji_banner.jpg?v=20141013) no-repeat center #e6f5f7;
	overflow: hidden;
	position: relative;
	.explain{
		position: absolute;
		left: 50%;
		margin: 50px 0 0 170px;
		line-height: 1.2;
		.p1{
			font-size: 28px;
		}
		.p2{
			font-size: 36px;
		}
		.p3{
			font-size: 64px;
			font-weight: bold;
		}
		.p4{
			font-size: 32px;
		}
		.enter{
			margin-top: 30px;
			a.round{
				width: 224px;
				height: 224px;
				display: inline-block;
				background: url(../images/zhiji_bg_round.png) no-repeat;
				position: relative;
				span{
					margin:95px 0 0 25px; 
					display: inline-block;
					color: #fff;
					font-size: 36px;
					font-weight: bold;
				}
				i.right{
					position: absolute;
					top: 50%;
					right: 20px;
					margin-top: -13px;
					width: 26px;
					height: 35px;
					display: inline-block;
					background: @zhiji_icons no-repeat;
					background-position: -160px 0;
				}
				&:hover{
					background-position: 0 -240px;					
				}
			}
		}
		p.look{
			margin: 10px 0 0 75px;
		}
	}
}
.index_content{
	width: 990px;
	margin: 40px auto 0;
	.content_img{
		height: 90px;
		background: url(../images/zhiji_img_1.png) no-repeat;
	}
	ul.content_info{
		margin-top: 20px;
		overflow: hidden;
		text-align: center;
		li{
			float: left;
			&.li1{
				width: 330px;
			}
			&.li2{
				width: 320px;
			}
			&.li3{
				padding-left:95px;
				text-align: left;
				h3 {
					text-align: left;
					padding-left:58px;
				}
			}			
		}
	}
}

//答题页
.title{
	h1{
		text-align: center;
		font-size: 22px;
		font-weight: normal;
		margin-top: 25px;
	}
}
.quest_nav{
	margin-top:10px;
	background-color: #fff;
	padding: 50px 0 20px 0;
	ul{
		position: relative;
		width:  550px;
		margin:0 auto;
		height: 10px;
		border-top: 2px solid #d0dfde;
		li{
			position: relative;
			i{
				position: absolute;
				top: -8px;
				z-index: 1;
				width: 16px;
				height: 16px;
				display: inline-block;
				background: @zhiji_icons no-repeat;	
				&.start{
					background-position: -20px 0;
					left: -8px;
				}
				&.end{
					background-position: -20px 0;
					right: -8px;
				}			
			}
			p{
				display: none;
			}
			&.current{
				.start{
					background-position: 0 0;
				}
				p{
					z-index: 2;
					position: absolute;
					border-top: 2px solid #4eccc0;
					top: -2px;
					display: block;
					.center{
						background-position: 0 0;
						right:-8px;
					}
					.prompt{
						position: absolute;
						top: -45px;
						right: -20px;
						width: 44px;
						height: 35px;
						line-height: 30px;
						display: inline-block;
						background: @zhiji_icons no-repeat;	
						background-position: -100px 0;
						text-align: center;
						color: #fff;
					}				
				}				
			}
		}
	}
}


section.zhiji_quest{
