.grade_star_b {
  width: 118px;
  height: 22px;
  overflow: hidden;
  display: inline-block;
  font-size: 0;
  background: url(../images/manage_icons.png) no-repeat 0 0;
  vertical-align: middle;
  line-height: 0;
}
.grade_star_b i {
  display: inline-block;
  width: 118px;
  height: 22px;
  overflow: hidden;
  font-size: 0;
  background: url(../images/manage_icons.png) no-repeat 0 -24px;
}
section.co_header {
  position: relative;
  width: 990px;
  background-color: #fff;
  margin: 0 auto 0 auto;
}
section.co_header .co_logo {
  position: absolute;
  width: 150px;
  height: 150px;
  display: block;
  padding: 1px;
  background-color: #fff;
  border: 1px solid #e7e7e7;
  top: 20px;
  left: 20px;
  overflow: hidden;
  font-size: 0;
}
section.co_header .co_logo:after {
  content: '';
}
section.co_header .co_logo:after,
section.co_header .co_logo span {
  display: inline-block;
  *display: inline;
  *zoom: 1;
  width: 0;
  height: 100%;
  vertical-align: middle;
}
section.co_header .co_logo img {
  width: 150px;
  display: inline-block;
  *display: inline;
  *zoom: 1;
  vertical-align: middle;
}
section.co_header .co_info {
  padding: 40px 0 25px 195px;
}
section.co_header .co_info h2 {
  font: bold 24px/1.5 Arial, STHeiti, Microsoft YaHei, simhei, simsun;
}
section.co_header .co_info em {
  margin: 0 8px;
  color: #e7e7e7;
}
section.co_header .msg {
  font-size: 14px;
  height: 22px;
  line-height: 22px;
  padding: 5px 0;
  color: #999999;
}
section.co_header .msg strong {
  font-size: 18px;
  color: #666666;
}
section.co_header .msg .grade_star_b {
  vertical-align: -5px;
  *vertical-align: middle;
  margin-right: 10px;
}
section.co_header .params {
  color: #999999;
  font-size: 14px;
}
section.co_header nav {
  height: 40px;
  line-height: 40px;
  background-color: #6e6e6e;
  padding-left: 200px;
}
section.co_header nav a {
  font-size: 16px;
  padding: 0 35px;
  float: left;
  color: #fff;
  text-decoration: none;
  border-right: 1px solid #999999;
  white-space: nowrap;
}
section.co_header nav a:hover {
  text-decoration: none;
}
section.co_header nav a.current {
  background-color: #7cb228;
  border: 1px solid #7cb228;
  border-radius: 2px;
  position: relative;
  top: -1px;
  left: -1px;
}
aside.manageLeft {
  width: 204px;
}
aside.manageLeft a.send_jobs {
  background-color: #f38100;
}
aside.manageLeft a.send_jobs:hover {
  background-color: #fa8712;
}
aside.manageLeft dl {
  border: 1px solid #e7e7e7;
}
aside.manageLeft dl dt {
  height: 40px;
  line-height: 40px;
  color: #999999;
  padding-left: 15px;
}
aside.manageLeft dl dt span {
  font-size: 14px;
  vertical-align: -2px;
  margin-left: 5px;
}
aside.manageLeft dl dd {
  height: 40px;
  line-height: 40px;
  border-top: 1px solid #e7e7e7;
}
aside.manageLeft dl dd a {
  color: #000;
  display: block;
  padding-left: 50px;
}
aside.manageLeft dl dd.current {
  background-color: #ededed;
}
aside.manageLeft dl dd.current a {
  color: #7cb228;
}
aside.manageLeft dl dd.current a:hover {
  text-decoration: none;
}
.maskui_dialog {
  min-width: 300px;
  background-color: #e9e9e9;
  background-color: rgba(0, 0, 0, 0.3);
  padding: 4px;
  border-radius: 4px;
  display: none;
}
.maskui_dialog h3 {
  font-size: 16px;
  color: #666666;
}
.maskui_dialog h3 .little {
  font-size: 12px;
  color: #999999;
  font-weight: normal;
}
.maskui_dialog .dialog_con {
  position: relative;
  min-height: 100px;
  _height: 100px;
  background-color: #fff;
  padding: 20px;
  border-radius: 4px;
}
.maskui_dialog .dialog_close {
  right: 5px;
  top: 5px;
  position: absolute;
  background-image: url(../images/icons_v2.png);
  background-position: -126px -48px;
  background-repeat: no-repeat;
  width: 20px;
  height: 20px;
  display: block;
  cursor: pointer;
}
.maskui_dialog .dialog_close:hover {
  background-position: -126px -26px;
}
.maskui_dialog .dialog_ac {
  padding: 10px 0 10px;
  text-align: center;
}
.maskui_dialog .dialog_ac p {
  margin-top: 20px;
}
.maskui_dialog .dialog_btn {
  border: 1px solid #dfdfdf;
  height: 33px;
  line-height: 33px;
  display: inline-block;
  padding: 0 30px;
  text-align: center;
  font-size: 14px;
  color: #333333;
  border-radius: 3px;
  cursor: pointer;
  white-space: nowrap;
  text-decoration: none;
}
.maskui_dialog .dialog_btn:hover {
  border: 1px solid #d3d3d3;
  background: #f6f6f6 url('');
  text-decoration: none;
}
.maskui_dialog .confirm_ok {
  margin-right: 10px;
}
.webuploader-container {
  position: relative;
}
.webuploader-pick {
  position: relative;
  display: inline-block;
  width: 100%;
  z-index: 0;
}
.webuploader-element-invisible {
  position: absolute !important;
  clip: rect(1px 1px 1px 1px);
  clip: rect(1px, 1px, 1px, 1px);
  width: 100%;
  height: 100%;
}
div.s_wrap_default {
  padding: 10px 13px 10px 13px;
  margin-bottom: 15px;
  background-color: #f6f6f6;
  border: 1px solid #e2e2e2;
  -webkit-box-shadow: 0 1px 2px #f5f5f5;
  -moz-box-shadow: 0 1px 2px #f5f5f5;
  -ms-box-shadow: 0 1px 2px #f5f5f5;
  box-shadow: 0 1px 2px #f5f5f5;
}
div.s_wrap_default p.cmp_tt {
  padding-bottom: 5px;
}
div.s_wrap_default p.ta_hint {
