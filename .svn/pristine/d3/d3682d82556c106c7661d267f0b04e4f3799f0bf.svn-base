html {
  height: 100%;
}
body {
  font: normal 14px/1.5 Arial, Helvetica, "Microsoft YaHei", STHeiti, "Droid Sans Fallback", sans-serif;
  color: #343434;
  height: 100%;
}
input,
button,
textarea,
select {
  font: normal 14px/1.5 Arial, Helvetica, "Microsoft YaHei", STHeiti, "Droid Sans Fallback", sans-serif;
}
a {
  color: #343434;
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
}
a.a_ul {
  text-decoration: underline;
}
h1,
h2,
h3,
h4,
h5,
h6 {
  font-size: 1em;
  line-height: 1;
}
html {
  font-size: 14px;
}
body {
  width: 100%;
  background-color: #fefefe;
  position: absolute;
  overflow: hidden;
}
.nav {
  height: 30px;
  line-height: 30px;
  background-color: #fefefe;
  width: 100%;
}
.nav .home {
  margin-left: 15px;
}
.nav .home img {
  width: 50px;
  vertical-align: top;
}
.nav .l_trangle {
  width: 0;
  height: 0;
  border: 5px dashed transparent;
  border-right: 5px dashed #333333;
  display: inline-block;
  margin-right: 5px;
}
.nav .tip {
  font-size: 14px;
  color: #333333;
  font-weight: bold;
}
.content {
  /*@media screen and (max-width: 400px){
    .rank-info {
      background: url("@{img-path}/info_bg.jpg") center center no-repeat;
      color: #fff;

      .info-detail {
