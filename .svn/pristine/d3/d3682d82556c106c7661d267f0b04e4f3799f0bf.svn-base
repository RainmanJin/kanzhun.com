      }
    }
  }*/
}
.content .rank-info {
  background: url("/images/activity/jinpingmei/info_bg.jpg") center center no-repeat;
  color: #fff;
}
.content .rank-info .info-detail {
  padding-top: 30px;
  margin: 0 auto;
  width: 90%;
  overflow: hidden;
}
.content .rank-info .info-detail .c_logo {
  width: 80px;
  float: left;
  clear: right;
}
.content .rank-info .info-detail .word {
  margin-left: 90px;
}
.content .rank-info .info-detail .word .prize {
  color: #fefefe;
  font-weight: bold;
  font-size: 1.3rem;
}
.content .rank-info .info-detail .word .prize .ranking {
  color: #e77200;
  padding-left: 5px;
}
.content .rank-info .info-detail .word .industry .indu {
  background: rgba(255, 255, 255, 0.2);
  padding: 2px 10px;
  font-size: 1.2rem;
}
.content .rank-info .info-detail .word .corporation {
  font-size: 1.3rem;
}
.content .rank-info .info-detail .oper {
  width: 100%;
  overflow: hidden;
  text-align: center;
  padding: 30px 0px;
}
.content .rank-info .info-detail .oper button {
  background-color: #4564ba;
  color: #fff;
  padding: 10px 30px;
  font-size: 1.5rem;
  font-weight: bold;
  border-radius: 5px;
}
.content .rank-info .info-detail .oper .up {
  background-color: #e77200;
  float: left;
}
.content .rank-info .info-detail .oper .down {
  float: right;
}
@media screen and (max-width: 320px) {
  .content .rank-info .info-detail .word .prize {
    font-size: 1.1rem;
  }
  .content .rank-info .info-detail .word .industry .indu {
    font-size: 1rem;
  }
  .content .rank-info .info-detail .word .corporation {
    font-size: 1rem;
  }
  .content .rank-info .info-detail .oper {
    width: 100%;
    overflow: hidden;
    text-align: center;
    padding: 30px 0px;
  }
  .content .rank-info .info-detail .oper button {
    background-color: #4564ba;
    color: #fff;
    padding: 10px 20px;
    font-size: 1.5rem;
    font-weight: bold;
    border-radius: 5px;
  }
  .content .rank-info .info-detail .oper .up {
    background-color: #e77200;
    float: left;
  }
  .content .rank-info .info-detail .oper .down {
    float: right;
  }
}
.content .rank-comment {
  padding: 10px;
}
.content .rank-comment ul li {
  margin-top: 15px;
}
.content .rank-comment .relation-wrap {
  height: 100%;
}
.content .rank-comment .relation1 {
  background-color: #575097;
  height: 40px;
  width: 40px;
  float: left;
  line-height: 40px;
  text-align: center;
  border-radius: 50%;
  color: #ffffff;
}
.content .rank-comment .relation2 {
  background-color: #e77200;
  height: 40px;
  width: 40px;
  float: left;
  line-height: 40px;
  text-align: center;
  border-radius: 50%;
  color: #ffffff;
}
.content .rank-comment .relation3 {
  background-color: #999999;
  height: 40px;
  width: 40px;
  float: left;
  line-height: 40px;
  text-align: center;
  border-radius: 50%;
  color: #ffffff;
}
.content .rank-comment .comment {
  background-color: #f3f3f3;
  border-radius: 5px;
  margin-left: 50px;
  font-size: 1.3rem;
  padding: 5px 10px;
}
.content .rank-comment .comment .standard {
  color: #999999;
}
.content .rank-comment .comment .diy {
  margin-top: 5px;
  font-size: 1.2rem;
  font-weight: bold;
  color: #666666;
}
.relation {
  height: 40px;
  width: 40px;
  float: left;
  line-height: 40px;
  text-align: center;
  border-radius: 50%;
  color: #ffffff;
}
