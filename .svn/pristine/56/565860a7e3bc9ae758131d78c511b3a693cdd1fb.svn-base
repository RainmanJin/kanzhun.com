.autocomplete-suggestions {
  border: 1px solid #e7e7e7;
  background: #FFF;
  cursor: default;
  overflow: auto;
}
.autocomplete-suggestions strong {
  font-weight: bold;
  color: #007ead;
}
.autocomplete-suggestions dl {
  background-color: #f5f5f5;
}
.autocomplete-suggestions dl dt {
  width: 2em;
  padding: 0 1em;
  float: left;
  color: #999999;
  padding-top: 5px;
}
.autocomplete-suggestions dl dt.bd {
  border-top: 1px solid #eeeeee;
}
.autocomplete-suggestions dl dd {
  border-left: 1px solid #eeeeee;
  margin-left: 4em;
  background-color: #fff;
}
.autocomplete-suggestions dl dd.bd {
  border-top: 1px solid #eeeeee;
}
.autocomplete-suggestion {
  height: 35px;
  line-height: 35px;
  padding-left: 5px;
  white-space: nowrap;
  overflow: hidden;
}
.autocomplete-selected {
  background: #007ead;
  color: #fff;
}
.autocomplete-selected strong {
  color: #ffd;
}
.ps_header {
  padding: 8px 0;
  background-color: #e5e5e5;
}
.ps_header div {
  text-align: center;
  white-space: nowrap;
  position: relative;
  z-index: 6;
}
.ps_header div .select {
  position: relative;
  display: inline-block;
  *display: inline;
  *zoom: 1;
  text-align: center;
  vertical-align: middle;
  width: 121px;
  height: 30px;
  background: url("/images/icons.png") repeat-x 0 -162px;
  border: 1px solid #cfcfcf;
  z-index: 9;
}
.ps_header div .select dt {
  height: 30px;
  background: none;
}
.ps_header div .select input {
  *display: inline-block;
  height: 16px;
  line-height: 16px;
  border: 0;
  background: none;
  margin-top: 7px;
  width: 100%;
  color: #999999;
}
.ps_header div .select span {
  width: 35px;
  height: 16px;
  padding-top: 14px;
  position: absolute;
  right: 0;
  top: 0;
  display: block;
  background: none;
  border: none;
  font-size: 0;
  text-align: center;
  cursor: pointer;
}
.ps_header div .select span i {
  display: inline-block;
  font-size: 0;
  width: 9px;
  height: 5px;
  overflow: hidden;
  -webkit-transition: all 0.2s linear;
  -moz-transition: all 0.2s linear;
  -ms-transition: all 0.2s linear;
  -o-transition: all 0.2s linear;
  transition: all 0.2s linear;
  -webkit-transform: rotate(0deg);
  -moz-transform: rotate(0deg);
  -ms-transform: rotate(0deg);
  -o-transform: rotate(0deg);
  transform: rotate(0deg);
  background: transparent url("/images/trangle.png") no-repeat;
}
.ps_header div .select dd {
  top: 31px;
  z-index: 4;
}
.ps_header div .select dd a {
  text-align: left;
  color: #343434;
}
.ps_header div .select dd a:hover {
  color: #fff;
}
.ps_header div input.ps_ipt {
  border: none;
  width: 176px;
  height: 16px;
  line-height: 16px;
  padding: 7px 0 7px 13px;
  border-top: 1px solid #cfcfcf;
  border-bottom: 1px solid #cfcfcf;
  background-color: #fff;
  vertical-align: middle;
}
.ps_header div input.ps_ipt.yibao {
  width: 233px;
  border-right: 1px solid #cfcfcf;
}
.ps_header div .btn_grey_ps {
  border: none;
  background: none;
  height: 30px;
  line-height: 30px;
  background: url("/images/icons.png") repeat-x 0 -162px;
  border: 1px solid #cfcfcf;
  padding: 0;
  text-align: center;
  color: #343434;
  cursor: pointer;
  white-space: nowrap;
  vertical-align: middle;
  display: inline-block;
}
.ps_header div .btn_grey_ps input,
.ps_header div .btn_grey_ps button {
  height: 30px;
  cursor: pointer;
  display: block;
  padding: 0 25px;
  border: 0;
  background-color: transparent;
  width: 100%;
  overflow: visible;
  color: #343434;
}
.ps_header div .btn_grey_ps:hover {
  text-decoration: none;
  color: #999999;
}
