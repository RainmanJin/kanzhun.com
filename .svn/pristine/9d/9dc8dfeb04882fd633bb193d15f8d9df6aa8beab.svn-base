textarea.text {
  font-size: 14px;
  width: 490px;
  padding: 9px 10px;
  border: 1px solid #cfcfcf;
  border-width: 2px 2px 1px 1px;
  vertical-align: middle;
  border-radius: 2px;
}
textarea.text.focus {
  border-color: #7cb228;
}
textarea.text.error {
