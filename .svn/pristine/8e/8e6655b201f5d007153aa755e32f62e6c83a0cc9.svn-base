.wrapper {
  position: relative;
  top: 0;
  left: 0;
  overflow-x: hidden;
}
.wrapper .container {
  position: relative;
  z-index: 1;
  height: 100%;
  width: 100%;
}
div.slide-mask {
  position: fixed;
  z-index: 2;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.4);
  -webkit-transition: all 0.3s ease-out;
  -moz-transition: all 0.3s ease-out;
  -ms-transition: all 0.3s ease-out;
  -o-transition: all 0.3s ease-out;
  transition: all 0.3s ease-out;
  display: none;
}
aside.slide-wrapper {
  position: absolute;
  z-index: 3;
  right: 0;
  top: 0;
  width: 80%;
  -webkit-transform: translate3d(100%, 0, 0);
  -moz-transform: translate3d(100%, 0, 0);
  -ms-transform: translate3d(100%, 0, 0);
  -o-transform: translate3d(100%, 0, 0);
  transform: translate3d(100%, 0, 0);
  -webkit-transition: all 0.3s ease-out;
  -moz-transition: all 0.3s ease-out;
  -ms-transition: all 0.3s ease-out;
  -o-transition: all 0.3s ease-out;
  transition: all 0.3s ease-out;
  display: none;
}
aside.slide-wrapper > div {
  min-height: 100%;
}
aside.slide-wrapper > div.slide_bg {
  background: #e5e5e5 url(../images/field/f_img5.jpg) no-repeat center 300px;
  -webkit-background-size: 98px 66px;
  -moz-background-size: 98px 66px;
  -o-background-size: 98px 66px;
  background-size: 98px 66px;
}
aside.slide-wrapper > div.white_bg {
  background-color: #fff;
}
aside.slide-wrapper > div.white_bg h2 {
  height: 47px;
  padding-left: 20px;
  line-height: 47px;
  background-color: #67bf57;
  color: #fff;
  font-size: 16x;
}
aside.slide-wrapper > div.white_bg h2 i {
  margin-right: 5px;
}
aside.slide-wrapper > div.white_bg h2 i.i_cmp {
  vertical-align: -2px;
}
aside.slide-wrapper > div.white_bg h2 i.i_position {
  vertical-align: -4px;
}
aside.slide-wrapper > div p {
  height: 42px;
  line-height: 42px;
  color: #666666;
}
aside.slide-wrapper > div > div.shadow_area {
  padding: 0 15px;
}
aside.slide-wrapper > div > div.shadow_area p i {
  margin-right: 8px;
}
aside.slide-wrapper > div > div.shadow_area p i.i_home {
  vertical-align: 0;
}
aside.slide-wrapper > div > div.shadow_area p i.i_weixin {
  vertical-align: -3px;
}
aside.slide-wrapper > div > div.shadow_area p a {
  display: block;
  color: #666666;
}
aside.slide-wrapper > div > div.shadow_area div.line {
  margin-right: -15px;
  overflow: hidden;
  height: 0;
  border-bottom: 1px solid #e5e5e5;
}
aside.slide-wrapper > div > div.select_item {
  padding: 0 0 0 20px;
}
aside.slide-wrapper > div > div.select_item p {
  border-bottom: 1px solid #e5e5e5;
}
aside.slide-wrapper > div > div.select_item p em.checkbox {
  position: relative;
  display: inline-block;
  width: 11px;
  height: 11px;
  border: 1px solid #ccc;
  vertical-align: -1px;
  margin-right: 10px;
}
aside.slide-wrapper > div > div.select_item p em.checkbox i {
  position: absolute;
  top: -3px;
  left: 0;
  display: none;
}
aside.slide-wrapper > div > div.select_item p.checked em.checkbox {
  border: 1px solid #67bf57;
}
aside.slide-wrapper > div > div.select_item p.checked em.checkbox i {
  display: inline-block;
}
aside.slide-wrapper > div > div.select_item p:last-child {
  border: none;
}
aside.slide-wrapper.moved {
  -webkit-transform: translate3d(0, 0, 0);
  -moz-transform: translate3d(0, 0, 0);
  -ms-transform: translate3d(0, 0, 0);
  -o-transform: translate3d(0, 0, 0);
  transform: translate3d(0, 0, 0);
}
aside.slide-wrapper li > a {
  padding: 15px 10px;
  border-top: 1px solid #4d4d4d;
  border-bottom: 1px solid #262626;
  font-size: 15px;
  display: block;
}
aside.slide-wrapper li.current > a {
  background-color: #262626;
}
a.bt_submit {
  color: #fff;
  display: inline-block;
  border: 1px solid #fff;
  border-radius: 2px;
  padding: 0 13px;
  height: 27px;
  line-height: 27px;
}
