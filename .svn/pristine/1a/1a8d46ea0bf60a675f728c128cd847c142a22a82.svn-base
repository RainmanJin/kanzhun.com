}
.f16 {
  font-size: 16px;
}
::-webkit-input-placeholder {
  color: #f0f0f0;
}
:-ms-input-placeholder {
  color: #f0f0f0;
}
.cube {
  -webkit-transform: translateZ(0);
  -moz-transform: translateZ(0);
  -ms-transform: translateZ(0);
  -o-transform: translateZ(0);
  transform: translateZ(0);
}
.display-flex {
  display: -webkit-flex;
  display: flex;
}
.box-sizing {
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  -ms-box-sizing: border-box;
  box-sizing: border-box;
}
html {
  height: 100%;
}
body {
  font: normal 14px/1.5 Arial, Helvetica, "Microsoft YaHei", STHeiti, "Droid Sans Fallback", sans-serif;
  color: #ffffff;
  overflow-x: hidden;
  height: 100%;
}
input,
button,
textarea,
select {
  font: normal 14px/1.5 Arial, Helvetica, "Microsoft YaHei", STHeiti, "Droid Sans Fallback", sans-serif;
}
a {
  color: #ffffff;
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
}
h1,
h2,
h3,
h4,
h5,
h6 {
  font-weight: normal;
  font-size: 1em;
  line-height: 1;
}
.swiper-container {
  height: 100%;
}
.swiper-wrapper {
  height: 100%;
}
.page {
  width: 100%;
  height: 100%;
  position: relative;
}
#page1 {
  background-color: #000000;
}
#page1 img {
  display: block;
  width: 100%;
  margin: 0 auto;
}
#page2 {
  background: #130e20 url("/images/activity/ali/bg.jpg") no-repeat;
  -webkit-background-size: 100%;
  -moz-background-size: 100%;
  -o-background-size: 100%;
  background-size: 100%;
}
#page2 img {
  margin-top: 10px;
  display: block;
  width: 100%;
  margin: 10px auto 10px;
}
@media screen and (max-height: 480px) {
  #page1 img {
    display: block;
    width: 90%;
    margin: 0 auto;
  }
  #page2 img {
    display: block;
    width: 90%;
    margin: 10px auto 0;
  }
}
a.test-bot {
  width: 100%;
  display: block;
  height: 50px;
  font-size: 16px;
  text-align: center;
  line-height: 50px;
  background-color: rgba(255, 255, 255, 0.3);
  z-index: 100;
}
.slide_up {
  position: absolute;
  bottom: 20px;
  left: 50%;
  margin-left: -13px;
  z-index: 20;
}
.slide_up i {
  display: inline-block;
  width: 25px;
  height: 15px;
  background: url("/images/activity/ali/up.png") no-repeat;
  background-size: 25px;
  -webkit-animation: arrowsUp 1s infinite linear;
}
@-webkit-keyframes arrowsUp {
  0% {
    -webkit-transform: translate(0, 0);
  }
  50% {
    -webkit-transform: translate(0, -5px);
  }
  100% {
    -webkit-transform: translate(0, 0);
  }
}
