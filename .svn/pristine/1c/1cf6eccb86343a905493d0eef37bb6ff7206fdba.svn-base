html {
  height: 100%;
}
body {
  font: normal 14px/1.5 Arial, Helvetica, "Microsoft YaHei", STHeiti, "Droid Sans Fallback", sans-serif;
  color: #343434;
  height: 100%;
}
input,
button,
textarea,
select {
  font: normal 14px/1.5 Arial, Helvetica, "Microsoft YaHei", STHeiti, "Droid Sans Fallback", sans-serif;
}
a {
  color: #343434;
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
}
a.a_ul {
  text-decoration: underline;
}
h1,
h2,
h3,
h4,
h5,
h6 {
  font-size: 1em;
  line-height: 1;
}
.wrapper {
  width: 100%;
  min-height: 100%;
  height: auto;
}
body {
  background-color: #ababab;
}
div.container {
  width: 100%;
  -webkit-box-sizing: border-box;
  -ms-box-sizing: border-box;
  box-sizing: border-box;
  padding: 20px;
}
div.container section {
  width: 100%;
  position: relative;
}
div.container section ul {
  position: absolute;
  top: 28%;
  left: 8.75%;
  font-size: 20px;
  color: #ffffff;
  line-height: 1.5em;
}
div.container section img {
  display: block;
}
div.container section img.kw-bg {
  width: 100%;
  -webkit-box-shadow: 10px 10px 25px #333333;
  -ms-box-shadow: 10px 10px 25px #333333;
  box-shadow: 10px 10px 25px #333333;
}
div.container section img.kw-tt {
  position: absolute;
  width: 82.5%;
  top: 10px;
  left: 8.75%;
}
div.container section img.kw-ft {
  position: absolute;
  width: 58.5%;
  bottom: 20px;
  right: 15px;
}
a {
  border: 0;
  background-color: transparent;
}
a.go {
  margin-top: 30px;
  display: block;
  font-size: 18px;
  color: #fff;
  text-align: center;
  width: 100%;
  -webkit-box-sizing: border-box;
  -ms-box-sizing: border-box;
  box-sizing: border-box;
  border: 1px solid #bfbfbf;
  height: 40px;
  line-height: 40px;
  border-radius: 20px;
}
.wrapper {
  height: 100%;
}
.wrapper .steps,
.wrapper form {
  height: 100%;
  width: 100%;
}
.wrapper section {
  position: relative;
  width: 100%;
  height: 100%;
}
.wrapper section img {
  width: 82px;
  display: block;
  margin: 0 auto;
  position: absolute;
  bottom: 50%;
  left: 50%;
  margin-left: -41px;
  margin-bottom: 48px;
}
.wrapper input.info {
  font-weight: 100;
  width: 100%;
  font-size: 20px;
  padding: 10px 0;
  text-align: center;
  border: 0;
  background-color: #fff;
  display: block;
  position: absolute;
  top: 50%;
  margin-top: -20px;
}
.wrapper button[type=button] {
  color: #707070;
  font-size: 18px;
  padding: 15px 0;
  width: 100%;
  border: 0;
  background-color: transparent;
  position: absolute;
  bottom: 0;
  left: 0;
}
.kw-dialog {
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 0;
  background-color: rgba(0, 0, 0, 0.8);
  display: none;
  text-align: right;
}
.kw-dialog img {
  width: 205px;
  margin-top: 10px;
}
@-webkit-keyframes twinkle {
  0% {
    background-color: #ffffff;
  }
  20% {
    background-color: #ffb4b4;
  }
  40% {
    background-color: #ffdede;
  }
  60% {
    background-color: #ffb4b4;
  }
  80% {
    background-color: #ffdede;
  }
  100% {
    background-color: #ffc8c8;
  }
}
@keyframes twinkle {
  0% {
    background-color: #ffffff;
  }
  20% {
    background-color: #ffb4b4;
  }
  40% {
    background-color: #ffdede;
  }
  60% {
    background-color: #ffb4b4;
  }
  80% {
    background-color: #ffdede;
  }
  100% {
    background-color: #ffc8c8;
  }
}
.shine {
  -webkit-animation: twinkle 0.6s ease none;
  -ms-animation: twinkle 0.6s ease none;
  animation: twinkle 0.6s ease none;
}
