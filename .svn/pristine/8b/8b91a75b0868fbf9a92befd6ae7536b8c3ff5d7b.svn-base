<script src="js/utils/idangerous.swiper.js"></script>
<script>
  $(function(){
    var mySwiper = new Swiper('.swiper-container', {
      //pagination: '.pagination',
      //loop: true,
      //autoplay: 5000,
      //paginationClickable: true,
      calculateHeight: true,
      //autoplayDisableOnInteraction: false,//手动滑动后继续自动滑动
      onSlideChangeEnd: function (swipe) {
        var _index=swipe.activeLoopIndex;
        $('.pk_content dt li.current').removeClass('current');
        $('.pk_content dt li:eq('+_index+')').addClass('current');
      }
    });
    $(".pk_content .tabs li").on('touchstart mousedown',function(e){
      e.preventDefault();
      $('.pk_content dt li.current').removeClass('current');
      $(this).addClass('current');
      mySwiper.swipeTo( $(this).index() );
    });
  });

  function initScaleLine() {
    var scaleLineTop = $('.scaleLine').offset().top;
    $(document).on('scroll', function () {
      var sT = $(document).scrollTop();
      if (sT > scaleLineTop) {
        $('.scaleLine').addClass('fixTop');
      } else {
        $('.scaleLine').removeClass('fixTop');
      }
    });
  }
  initScaleLine();
</script>
