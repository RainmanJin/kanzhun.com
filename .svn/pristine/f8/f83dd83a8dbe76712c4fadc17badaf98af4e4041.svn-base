@import "module/var";
@img: "../images";

body {
  .wrapper {
    background-color: #fff;
  }
}

.f_12 {
  font-size: 12px;
}

header.normal {
  width: 100%;

  ul {
    overflow: hidden;
    
    li {
      float: left;
      width: 25%;

      a {
        display: block;
        width: 62.5px;
        height: 62.5px;

        img {
          width: 62.5px;
          height: 62.5px;
        }
      }

      &.explain_one {
        img {
          width: 65px;
          height: 60px;
        }
      }

      &.explain_two {
        img {
          width: 68.5px;
          height: 60px;
        }
      }

      &.explain_three {
        img {
          width: 65px;
          height: 60.5px;
        }
      }
    }
  }
}

ul {
  &.boss_tags {
    margin-top: -3px;
    overflow: hidden;
    background-color: #f3f3f3;
    padding: 15px 0 15px 10px;

    li {
      position: relative;
      float: left;
      background-color: #ff7e00;
      border-radius: 5px;
      color: #fff;
      font-weight: bold;
      font-size: 12px;
      height: 20px;
      line-height: 20px;
      padding: 0 5px;
      margin-right: 10px;

      &:last-child {
        margin-right: 0;
      }

      span {
        position: absolute;
        left: 10px;
        top: -4px;
        font-size: 0;
        line-height: 0;
        overflow: hidden;
        border-width: 0 3px 5px 3px;
        border-style: solid dashed;
        border-color: transparent transparent #ff7e00 transparent;

        &.xie_trangle {
          .transform(rotate(90deg));
          top: -2px;
          border-width: 0 4px 5px 4px;
        }
      }
    }
  }
}

p{
  &.guide {
    text-align: center;
    background-color: #4ec2bb;
    height: 25px;
    line-height: 25px;
    color: #fff;
  }
}

dl {
  &.download {
    overflow: hidden;
    background-color: #f3f3f3;

    img {
      border: 1px solid #4ec2bb;
      width: 52.5px;
      height: 52.5px;
    }

    dt {
      float: left;
      width: 55%;


      & > div {
        position: relative;
        padding: 15px 0 15px 5px;
        border-right: 2px solid #fff;

        & > span {
          position: absolute;
          right: -17px;
          top: 0;
          font-size: 0;
          line-height: 0;
          overflow: hidden;
          border-width: 9px 16px 9px 16px;
          border-style: solid dashed;
          border-color: #4ec2bb transparent transparent transparent;
        }

        div {
          overflow: hidden;
        }
        img {
          float: left;
        }
      }

      .down_btn {
        float: left;
        padding-left: 10px;

        a {
          display: block;
          width: 70px;
          height: 20px;
          line-height: 20px;
          color: #8e8e8e;
          border: 1px solid #909090;
          padding-left: 20px;
          font-size: 12px;

          &.iPhone {
            background: url("@{img}/toilet/toilet2.jpg") no-repeat 3px center;
            .background-size(14px, 16px);
            margin-bottom: 10px;
          }

          &.android {
            background: url("@{img}/toilet/toilet3.jpg") no-repeat 3px center;
            .background-size(13.5px, 16px);
          }
        }
      }
    }

    dd {
      float: left;
      width: 45%;
      padding-top: 15px;

      img {
        display: block;
        margin: 0 auto;
      }
    }
  }

  &.main_con {
    overflow: hidden;

    dt {
      float: right;
      width: 40%;
      padding-top: 20px;

      img {
        display: block;
        margin: 0 auto;
      }
    }

    dd {
      margin-right: 40%;
      color: #999999;
      padding: 20px 5px;

      p.tt {
        font-size: 16px;
        color: #333333;

        span {
          font-size: 14px;
          color: #999999;
        }
      }
    }
  }

  &.main_two {
    background-color: #f2f4ee;
    padding: 20px 0;
    dt {
      float: left;
      padding-top: 10px;
    }

    dd {
      margin-left: 40%;
      padding: 0;
      margin-right: 0;
    }
  }

  &.three {
    dt {
      padding-top: 0;
    }
  }
}

//代码版
div.code_wrapper {
  min-height: 100%;
  width: 100%;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    -ms-box-sizing: border-box;
    box-sizing: border-box;
  border-width: 1px 1px 1px 1px;
    border-style: solid;
  border-color: #cfcfd1;
  background-color: #5d6164;
  padding: 0 4px 4px 4px;
  border-radius: 2px;
  /*background: #000 url("@{img}/toilet/toilet_bg.png") no-repeat 0;
  .background-size(100%, 100%);*/

  & > div {
    min-height: 100%;
    width: 100%;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    -ms-box-sizing: border-box;
    box-sizing: border-box;
    border-width: 0 1px 1px 1px;
    border-style: solid;
    border-color: #959799;
    background-color: #000;
    border-radius: 2px;
    /*width: 100%;
    height: 100%;
    background: url("@{img}/toilet/toilet_bg_bottom.png") no-repeat 0 bottom;
    .background-size(100%);*/

    & > div {
      position: relative;
      width: 100%;
      min-height: 100%;
      -webkit-box-sizing: border-box;
      -moz-box-sizing: border-box;
      -ms-box-sizing: border-box;
      box-sizing: border-box;
      background-color: #393939;
      border: 1px solid #000;
      border-radius: 2px;
    }
  }
}

header.code {
  position: absolute;
  top: 0;
  left: -5px;
  right: -5px;
  height: 30px;
  line-height: 30px;
  padding-left: 10px;
  background: -webkit-gradient(linear, left top, left bottom, from(#5a5f63), color-stop(0.5, #737679), to(#62666a));

  img {
    width: 15px;
    height: 15px;
    margin-right: 10px;
    vertical-align:  -2px;
  }
  span {
    color: #000;
  }
}

div.code_con {
  margin-top: 30px;
  border-radius: 2px;
  border-top: 1px solid #959799;
  padding-bottom: 20px;
}

dl {
  &.introduct {
    border-top: 1px solid #000;

    dt {
      float: left;
      width: 72.5px;
      height: 72.5px;
      border-bottom: 1px solid #000;
    }

    dd {
      margin-left: 73px;
      font-size: 12px;

      p {
        height: 23px;
        line-height: 23px;
        background-color: #3c3e40;
        border-bottom: 1px solid #000;
        border-top: 1px solid #545454;
        color: #fff;

        &:first-child {
          border-top: none;
        }
      }
    }
  }

  &.write_codes {
    overflow: hidden;
    border-bottom: 1px solid #545454;

    dt {
      width: 3px;
      float: left;
    }

    dd {
      padding-top: 15px;
      padding-left: 5px;
      border-left: 1px solid #aa9961;
      margin-left: 3px;

      p {
        color: #a7a78f;
        font-size: 12px;
        line-height: 1.7em;

        &:last-child {
          padding: 20px 0 10px 0;
        }
      }
    }
  }
}

div.code_tt {
  height: 26px;
  background-color: #3c3e3e;
  border-bottom: 1px solid #585858;

  img {
    border-bottom: -1px;
  }
}

.er_code {
  overflow: hidden;
  border-top: 1px solid #000;
  padding-top: 10px;
  padding-left: 25px;
  padding-bottom: 10px;

    img {
      display: block;
      float: left;
    }

    p {
      float: left;
      padding-left: 10px;

      a {
        display: block;
        width: 80px;
        height: 22px;
        line-height: 22px;
        border: 1px solid #8d9392;
        color: #fff;
        font-size: 12px;
        padding-left: 25px;

        &.iphone {
          background: url("@{img}/toilet/code_toilet8.jpg") no-repeat 5px center;
          .background-size(14px, 16px);
          margin-bottom: 4px;
        }

        &.android {
          background: url("@{img}/toilet/code_toilet9.jpg") no-repeat 5px center;
          .background-size(13.5px, 16px);
        }
      }

      &:last-child {
        color: #fff;
        height: 52px;
        line-height: 52px;
      }
    }
  }

.code_footer {
  position: absolute;
  bottom: 0;
  width: 100%;
  height: 20px;
  background: #3c3e40 url("@{img}/toilet/code_13.jpg") no-repeat 10px center;
}