@import "common/var";

section.mian_con {
  width: 628px;
  margin: 0 auto;
}

p {
  &.noresult {
    margin: 70px 0 20px 0;
    font-size: 18px;
  }

  &.hadsend {
    font-size: 18px;
  }

  &.verification_suss {
    padding-left: 70px;
    background: url(../images/img_02.png) no-repeat left center;
    font-size: 14px;

    em {
      font-size: 18px;
      color: @def;
    }
  }

 /* &.emialaddress {
    input {
      width: 260px;
    }
  }*/
}

dl {
  &.searchcon{
    padding: 20px 0;
    border-bottom: 1px dashed #e3e3e3;

    dt {
      float: left;
      text-align: center;

      a {
        display: block;
        width: 96px;
        height: 96px;
        line-height: 96px;
        font-size: 0;
        overflow: hidden;
        background-color: #fff;
        border: 1px solid #E7E7E7;
        img{
          width: 96px;
          height: 96px;
        }
      }
    }

    dd {
      position: relative;
      padding: 5px 0 0 113px;
      *padding-left: 20px;

      div.msgs {
        overflow: hidden;
        padding-top: 5px;

        em {
          float: left;
          line-height: 1em;
          padding-top: 2px;
          padding-right: 5px;
          font-size: 20px;
          color: @grey_33;
        }

        & > span {
          float: left;
          margin-right: 10px;
          line-height: 1em;
        }

        a {
          float: left;
          padding-top: 6px;
          line-height: 1em;
        }
      }

      p {
        line-height: 2em;

        em {
          margin: 0 5px;
          color: @grey_e7;
        }
      }

      aside {
        position: absolute;
        top: 50%;
        right: 0;
        margin-top: -18px;

        a {
          width: 134px;
          padding: 0;
        }
      }
    }
  }
}

ul {
  &.registerData {
    li {
      margin-top: 20px;

      input[type="checkbox"] {
        vertical-align: middle;
        margin-right: 5px;
      }

      &.paddingleft {
        padding-left: 6em;
      }
    }
  }

  &.createcmp {
    li {
      zoom: 1;
      .red{
        height: 34px;
        line-height: 34px;
      }
    }
  }

  &.emailregister {
    em.filewrap {
      position: relative;
      display: inline-block;
      width: 79px;
      height: 35px;
      line-height: 35px;
      //overflow: hidden;
      vertical-align: middle;
      cursor: pointer;

      .btn_grey {
      }
    }

    label {
      width: 5em;

      i {
        vertical-align: middle;
        margin-right: 3px;
      }
    }

    .upload {
      position: absolute;
      width: 78px;
      height: 34px;
      cursor: pointer;
      filter:alpha(opacity=0); //IE
      opacity: 0; //MOZ , FF
    }
  }

  &.resetpwd {
    padding-left: 128px;

    li {
      margin: 0 0 20px 0;
      position: relative;
      
      span {
        position: absolute;
        left: 380px;
        top: 6px;
        white-space: nowrap;
      }
    }
  }
}

input {
  &.emailipt {
    width: 160px;
  }
}

h2 {
  &.creat_tt {
    width: 120px;
    height: 50px;
    margin: 30px 0 20px -20px;
    line-height: 50px;
    background: #f3f3f3 url(../images/img_01.jpg) no-repeat right center;
    font-size: 14px;
    font-weight: normal;
    text-align: center;

    i {
      display: inline-block;
      line-height: 0;
      overflow: hidden;
      *zoom: 1;
      width: 0;
      height: 28px;
      margin-left: 25px;
      border: 1px dashed #E6E0E0;
      vertical-align: middle;
    }
  }
}

dl {
  &.selectwrap {
    float: left;
    width: 330px;
    vertical-align: middle;
  }
}

a {
  &.backlogin {
    margin-left: 20px;
    color: @grey_99;
  }
}

