//登录页
body.bg_color_ea{
  background-color: #eaeaea;
}

.auth_view{
  width: 978px;
  margin: 45px auto;
  overflow: hidden;
  zoom: 1;
  border: 6px solid #e2e2e2;

  .auth_bg{
    float: left;
    width: 535px;
    height: 371px;
    //background: url(../images/img_03.jpg) no-repeat center;

    img {
      width: 535px;
      height: 371px;
    }
  }
}

div.auth_ipts {
  float: right;
  width: 443px;
  height: 371px;
  position: relative;
  overflow: hidden;

  &.login {
    .a_login {
      z-index: 2;
    }

    .a_register {
      z-index: 1;
    }
  }

  &.register  {
    .a_login {
      z-index: 1;
    }

    .a_register {
      z-index: 2;
    }
  }
}
section{
  &.auth{
    position: absolute;
    background-color: #ffffff;
    padding: 0 50px;
    width: 343px;
    height: 371px;
    text-align: left;
    top: 0;
    left: 0;

    h3{
      padding-top: 45px;
      font-size: 24px;
      font-weight: normal;
      height: 34px;
      position: relative;

      a{
        float: right;
        font-size: 18px;
        color: @green;
        line-height: 1em;
        padding-top: 13px;

        &:hover{
          color: #96ca46;
          text-decoration: none;
        }
      }
    }


    ul{
      div{
        border-width: 1px;
        border-style: solid;
        border-color: #d5d5d5 #e1e1e1 #e0e0e0  #dadada;
        height: 26px;
        border-radius: 4px;
        padding: 8px 0;
        background-color: #fff;
        margin-bottom: 15px;
        position: relative;
        .transition(all .2s ease);
        background-color: #f7f7f7;

        &.focus{
          border-width: 1px;
          border-style: solid;
          border-color: @green;
          background-color: #ffffff;

          .ifd {
            background-color: #ffffff;

            /*&:-webkit-autofill {
              -webkit-box-shadow:0 0 0 50px white inset; *//* Change the color to your own background color *//*
              -webkit-text-fill-color: @grey_99;
            }*/
          }
        }

        &.error{
          border-width: 1px;
          border-style: solid;
          border-color: @red;
          background-color: #ffffff;

          .ifd {
            background-color: #ffffff;

            /*&:-webkit-autofill {
              -webkit-box-shadow:0 0 0 50px white inset; *//* Change the color to your own background color *//*
              -webkit-text-fill-color: @grey_99;
            }*/
          }
        }
      }

      label{
        width: 18px;
        padding: 2px 12px 0 12px;
        height: 26px;
        border-right: 1px solid #e1e1e1;
        float: left;
        font-size: 0;

        &.emailaddress {
          width: 22px;
          height: 22px;
          padding: 4px 9px 0 12px;
        }
      }
      .ifd{
        border: 0;
        background-color: transparent;
        font-size: 14px;
        color: #999;
        width: 276px;
        padding: 5px 10px;
        margin-left: 42px;
        display: block;

        /*&:-webkit-autofill {
          -webkit-box-shadow:0 0 0 50px #f7f7f7 inset; *//* Change the color to your own background color *//*
          -webkit-text-fill-color: @grey_99;
        }*/
      }

      /*.emailipt {
        width: 200px;
      }*/

      p.err{
        display: none;
        width: 122px;
        height: 28px;
        position: absolute;
        left: 188px;
        top: -15px;
        color: @red;
        padding-left: 32px;
        line-height: 22px;
        overflow: hidden;
        background: @icons no-repeat -140px top;
        z-index: 2;
      }

      .code{
        div{
          width: 220px;
          *display: inline;
          *zoom: 1;
          vertical-align: middle;
          margin-top: 0;
        }

        .ifd{
          width: 156px;
          margin: 0;
          background-color: #f7f7f7;
        }

        img{
          height: 44px;
          float: right;
          border-radius: 4px;
        }
      }
    }


    .agree{
      font-size: 14px;
      color: #999999;
      margin-bottom: 15px;

      input{
        vertical-align: -2px;
        margin-right: 5px;
      }

      a{
        color: #2c84cc;
      }

      label {
        color: @grey_66;
      }
    }

    .auth_btn{
      width: 100%;
      height: 42px;
      border: 0;
      background-color: @green;
      color: #fff;
      line-height: 42px;
      font-size: 20px;
      cursor: pointer;
      border-radius: 2px;

      .transition(background-color .2s ease);

      &:hover{
        background-color: #96ca46;
      }
    }
  }

  &.a_login {
    z-index: 2;
  }

  &.a_register {
    z-index: 1;
  }
}

.sys_err{
  color: #d60000;
  height: 16px;
  text-align: right;
  font-weight: bold;
  padding-bottom: 5px;
}
span.middle-helper{
  display: inline-block;
  width: 1px;
  height: 100%;
  margin-right: -1px;
  vertical-align: middle;
}

//上传按钮
.webuploader-pick{
  position:relative;
  display: inline-block;
  height: 32px;
  line-height: 32px;
  background: @btn_bgs;
  border: 1px solid @grey_e5;
  padding: 0 25px;
  cursor: pointer;
}
.webuploader-element-invisible {
  position: absolute !important; 
  clip: rect(1px 1px 1px 1px); 
  clip: rect(1px,1px,1px,1px);
}

//链接失效
section.links_con {
  width: 890px;
  margin: 0 auto;
}
div.links_disabled {
  overflow: hidden;
  padding-top: 100px;
  padding-bottom: 35px;
  margin-bottom: 35px;
  border-bottom: 1px dashed #e7e7e7;

  img {
    width: 54px;
    height: 73px;
    float: left;
    margin-left: 299px;
  }

  p {
    margin-left: 360px;
    padding-top: 15px;
  }
}

.l_d_btn {
  a {
    display: inline-block;
    width: 120px;
    font-size: 14px;
    
    &.a_yellow_btn {
      height: 35px;
      line-height: 35px;
      margin-right: 50px;
    }
  }
}