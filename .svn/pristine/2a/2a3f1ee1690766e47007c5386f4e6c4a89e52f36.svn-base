@import "var";

.container{
  background-color: #f0eff5;

  header{
    background-color: @green;
    height: 45px;
    padding: 0 10px;

    strong{
      font-size: 20px;
      float: left;
      height: 45px;
      line-height: 45px;
      color: #ffffff;
      font-weight: normal;

      &.channel{
        i{
          border-left: 1px solid #ffffff;
          border-top: 1px solid #ffffff;
          width: 13px;
          height: 13px;
          margin-left: 5px;
          margin-right: -3px;
          display: inline-block;
          .transform(rotate(-45deg));
        }
        font-size: 16px;
      }
    }

    a{
      float: right;
      padding: 12.5px 5px 0;
      height: 32.5px;

      
      &:nth-of-type(2){
        margin-right: 10px;
      }
    }
    img{
      height: 18px;

    }
  }
}

footer{
  &.page_ft{
    background-color: #6f6e6e;
    text-align: center;
    padding: 7px 0;
    color: #828282;
    width: 100%;
    height: 36px;
    margin-top: -50px;
    position: relative;
    clear: both;

    a{
      margin: 0 8px;

      &.a-green{
        color: @green;
      }
    }
  }
}