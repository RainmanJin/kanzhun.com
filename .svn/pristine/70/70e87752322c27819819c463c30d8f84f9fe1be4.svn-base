@import "../common/var";
@import "../modules/reset";
@import "../common/common";

html,body{
  height: 100%;
}

.snail{
  width: 100%;
  height: 100%;
  overflow: hidden;
  min-height: 768px;
}

.banner{
  width: 100%;
  height: 64%;
  background: url("../../images/download/banner.jpg")  no-repeat center 0;
  position: relative;
  overflow: hidden;

  .banner-wrap{
    position: absolute;
    bottom: 0;
    overflow: hidden;
    width: 200%;
    height: 91.5%;
    text-align: center;
  }

  .banner-con{
    float: left;
    width: 50%;
    height: 100%;
  }

  .phone{
    height: 100%;
    margin-right: 80px;
    vertical-align: top;
  }

  .info{
    display: inline-block;
    *display: inline;
    *zoom: 1;
    vertical-align: top;
    height: 100%;

    li{
      height: 18%;
      text-align: left;

      img{
        height: 100%;
      }

      &.tt{
        margin-top: 9%;
      }

      &.code{
        height: 27%;
      }
    }
  }

}

.download{
  height: 26%;
  background-color: #f5f8fa;
  text-align: center;

  a{
    display: inline-block;
    background-image: url("../../images/download/btns.jpg");
    background-repeat: no-repeat;
    vertical-align: middle;
    margin: 0 120px;

    &.ios{
      width: 130px;
      height: 162px;
      background-position: 0 -184px;

      &.current{
        background-position: 0 0;
      }
    }

    &.android{
      width: 145px;
      height: 162px;
      background-position: -360px 0;

      &.current{
        background-position: -360px -184px;
      }
    }
  }
}

.footer{
  height: 10%;
  background-color: #687884;
  text-align: center;
  color: #ccd9e2;
  font-size: 12px;
  line-height: 100%;
}

.middle-helper{
  display: inline-block;
  width: 1px;
  height: 100%;
  margin-right: -1px;
  vertical-align: middle;
}

@-webkit-keyframes flipInX {
  0% {
    -webkit-transform: perspective(400px) rotate3d(1, 0, 0, 90deg);
    transform: perspective(400px) rotate3d(1, 0, 0, 90deg);
    -webkit-transition-timing-function: ease-in;
    transition-timing-function: ease-in;
    opacity: 0;
  }

  40% {
    -webkit-transform: perspective(400px) rotate3d(1, 0, 0, -20deg);
    transform: perspective(400px) rotate3d(1, 0, 0, -20deg);
    -webkit-transition-timing-function: ease-in;
    transition-timing-function: ease-in;
  }

  60% {
    -webkit-transform: perspective(400px) rotate3d(1, 0, 0, 10deg);
    transform: perspective(400px) rotate3d(1, 0, 0, 10deg);
    opacity: 1;
  }

  80% {
    -webkit-transform: perspective(400px) rotate3d(1, 0, 0, -5deg);
    transform: perspective(400px) rotate3d(1, 0, 0, -5deg);
  }

  100% {
    -webkit-transform: perspective(400px);
    transform: perspective(400px);
  }
}

@keyframes flipInX {
  0% {
    -webkit-transform: perspective(400px) rotate3d(1, 0, 0, 90deg);
    -ms-transform: perspective(400px) rotate3d(1, 0, 0, 90deg);
    transform: perspective(400px) rotate3d(1, 0, 0, 90deg);
    -webkit-transition-timing-function: ease-in;
    -ms-transition-timing-function: ease-in;
    transition-timing-function: ease-in;
    opacity: 0;
  }

  40% {
    -webkit-transform: perspective(400px) rotate3d(1, 0, 0, -20deg);
    -ms-transform: perspective(400px) rotate3d(1, 0, 0, -20deg);
    transform: perspective(400px) rotate3d(1, 0, 0, -20deg);
    -webkit-transition-timing-function: ease-in;
    -ms-transition-timing-function: ease-in;
    transition-timing-function: ease-in;
  }

  60% {
    -webkit-transform: perspective(400px) rotate3d(1, 0, 0, 10deg);
    -ms-transform: perspective(400px) rotate3d(1, 0, 0, 10deg);
    transform: perspective(400px) rotate3d(1, 0, 0, 10deg);
    opacity: 1;
  }

  80% {
    -webkit-transform: perspective(400px) rotate3d(1, 0, 0, -5deg);
    -ms-transform: perspective(400px) rotate3d(1, 0, 0, -5deg);
    transform: perspective(400px) rotate3d(1, 0, 0, -5deg);
  }

  100% {
    -webkit-transform: perspective(400px);
    -ms-transform: perspective(400px);
    transform: perspective(400px);
  }
}

.flipInX {
  -webkit-backface-visibility: visible !important;
  -ms-backface-visibility: visible !important;
  backface-visibility: visible !important;
  -webkit-animation-name: flipInX;
  -ms-animation-name: flipInX;
  animation-name: flipInX;
  -webkit-animation-duration: 1.8s;
  -ms-animation-duration: 1.8s;
  animation-duration: 1.8s;
  -webkit-animation-fill-mode: both;
  -ms-animation-fill-mode: both;
  animation-fill-mode: both;
}




@-webkit-keyframes lightSpeedIn {
  0% {
    -webkit-transform: translate3d(100%, 0, 0) skewX(-30deg);
    transform: translate3d(100%, 0, 0) skewX(-30deg);
    opacity: 0;
  }

  60% {
    -webkit-transform: skewX(20deg);
    transform: skewX(20deg);
    opacity: 1;
  }

  80% {
    -webkit-transform: skewX(-5deg);
    transform: skewX(-5deg);
    opacity: 1;
  }

  100% {
    -webkit-transform: none;
    transform: none;
    opacity: 1;
  }
}

@keyframes lightSpeedIn {
  0% {
    -webkit-transform: translate3d(100%, 0, 0) skewX(-10deg);
    transform: translate3d(100%, 0, 0) skewX(-10deg);
    opacity: 0;
  }

  60% {
    -webkit-transform: skewX(30deg);
    transform: skewX(30deg);
    opacity: 1;
  }

  80% {
    -webkit-transform: skewX(-5deg);
    transform: skewX(-5deg);
    opacity: 1;
  }

  100% {
    -webkit-transform: none;
    transform: none;
    opacity: 1;
  }
}

.lightSpeedIn {
  -webkit-animation-name: lightSpeedIn;
  animation-name: lightSpeedIn;
  -webkit-animation-timing-function: ease-in-out;
  animation-timing-function: ease-in-out;
  -webkit-animation-duration: 1s;
  animation-duration: 1s;
  -webkit-animation-fill-mode: both;
  animation-fill-mode: both;
}


