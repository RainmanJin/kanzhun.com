section.auth {
  position: absolute;
  background-color: #ffffff;
  padding: 0 50px;
  width: 343px;
  height: 371px;
  text-align: left;
  top: 0;
  left: 0;
}
section.auth h3 {
  padding-top: 45px;
  font-size: 24px;
  font-weight: normal;
  height: 34px;
  position: relative;
}
section.auth h3 a {
  float: right;
  font-size: 18px;
  color: #7cb228;
  line-height: 1em;
  padding-top: 13px;
}
section.auth h3 a:hover {
  color: #96ca46;
  text-decoration: none;
}
section.auth ul {
  /*.emailipt {
        width: 200px;
      }*/
}
section.auth ul div {
  border-width: 1px;
  border-style: solid;
  border-color: #d5d5d5 #e1e1e1 #e0e0e0  #dadada;
  height: 26px;
  border-radius: 4px;
  padding: 8px 0;
  background-color: #fff;
  margin-bottom: 15px;
  position: relative;
  -webkit-transition: all 0.2s ease;
  -moz-transition: all 0.2s ease;
  -ms-transition: all 0.2s ease;
  -o-transition: all 0.2s ease;
  transition: all 0.2s ease;
  background-color: #f7f7f7;
}
section.auth ul div.focus {
  border-width: 1px;
  border-style: solid;
  border-color: #7cb228;
  background-color: #ffffff;
}
section.auth ul div.focus .ifd {
  background-color: #ffffff;
  /*&:-webkit-autofill {
              -webkit-box-shadow:0 0 0 50px white inset; */
  /* Change the color to your own background color */
  /*
              -webkit-text-fill-color: @grey_99;
            }*/
}
section.auth ul div.error {
  border-width: 1px;
  border-style: solid;
  border-color: #d60000;
  background-color: #ffffff;
}
section.auth ul div.error .ifd {
  background-color: #ffffff;
  /*&:-webkit-autofill {
              -webkit-box-shadow:0 0 0 50px white inset; */
  /* Change the color to your own background color */
  /*
              -webkit-text-fill-color: @grey_99;
            }*/
}
section.auth ul label {
  width: 18px;
  padding: 2px 12px 0 12px;
  height: 26px;
  border-right: 1px solid #e1e1e1;
  float: left;
  font-size: 0;
}
section.auth ul label.emailaddress {
  width: 22px;
  height: 22px;
  padding: 4px 9px 0 12px;
}
section.auth ul .ifd {
  border: 0;
  background-color: transparent;
  font-size: 14px;
  color: #999;
  width: 276px;
  padding: 5px 10px;
  margin-left: 42px;
  display: block;
  /*&:-webkit-autofill {
          -webkit-box-shadow:0 0 0 50px #f7f7f7 inset; */
  /* Change the color to your own background color */
  /*
          -webkit-text-fill-color: @grey_99;
        }*/
}
section.auth ul p.err {
  display: none;
  width: 122px;
  height: 28px;
  position: absolute;
  left: 188px;
  top: -15px;
  color: #d60000;
  padding-left: 32px;
  line-height: 22px;
  overflow: hidden;
  background: url(../images/icons.png) no-repeat -140px top;
  z-index: 2;
}
section.auth ul .code div {
  width: 220px;
  *display: inline;
  *zoom: 1;
  vertical-align: middle;
  margin-top: 0;
}
section.auth ul .code .ifd {
  width: 156px;
  margin: 0;
  background-color: #f7f7f7;
}
section.auth ul .code img {
  height: 44px;
  float: right;
  border-radius: 4px;
}
section.auth .agree {
  font-size: 14px;
  color: #999999;
  margin-bottom: 15px;
}
section.auth .agree input {
  vertical-align: -2px;
  margin-right: 5px;
}
section.auth .agree a {
  color: #2c84cc;
}
section.auth .agree label {
  color: #666666;
}
section.auth .auth_btn {
  width: 100%;
  height: 42px;
  border: 0;
  background-color: #7cb228;
  color: #fff;
  line-height: 42px;
  font-size: 20px;
  cursor: pointer;
  border-radius: 2px;
  -webkit-transition: background-color 0.2s ease;
  -moz-transition: background-color 0.2s ease;
  -ms-transition: background-color 0.2s ease;
  -o-transition: background-color 0.2s ease;
  transition: background-color 0.2s ease;
}
section.auth .auth_btn:hover {
  background-color: #96ca46;
}
section.a_login {
  z-index: 2;
}
section.a_register {
