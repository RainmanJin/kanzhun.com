div.r_wrapper {
  width: 100%;
  min-height: 100%;
  height: auto;
}
div.additional {
  height: 65px;
}
section.probably {
  height: 182px;
  border-bottom: 1px solid #53cac3;
}
section.probably h1 {
  text-align: center;
  font-size: 18px;
  font-weight: normal;
  margin-bottom: 20px;
  padding-top: 15px;
}
section.probably div {
  position: relative;
}
section.probably div a {
  position: absolute;
  left: 10px;
  top: 10px;
  color: #ffffff;
  font-size: 16px;
}
section.probably div a i {
  margin-right: 5px;
  vertical-align: -5px;
}
section.probably div.user_img {
  display: block;
  width: 64px;
  height: 64px;
  margin: 0 auto 10px auto;
  border-radius: 100%;
  border: 2px solid #fff;
}
section.probably div.user_img img {
  width: 64px;
  height: 64px;
  border-radius: 64px;
}
section.probably p:nth-of-type(1) {
  font-size: 14px;
  text-align: center;
  margin-bottom: 5px;
}
section.probably p:last-child {
  text-align: center;
}
section.probably p:last-child span {
  display: inline-block;
  padding: 0 15px;
  background-color: #676059;
  height: 20px;
  line-height: 20px;
  font-size: 12px;
  border-radius: 10px;
}
section.probably.noborder {
  border-bottom: none;
}
