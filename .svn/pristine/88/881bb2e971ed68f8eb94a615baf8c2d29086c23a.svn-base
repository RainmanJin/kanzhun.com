@import "../common/var";

#shareDialog{
  z-index: 4;
  display: none;
  position: absolute;
  background-color: #fff;
  padding-left: 10px;
  border: 1px solid @grey_99;
  border-radius: 2px;

  i.s_close{
    background: url("@{img-path}/profiles.png") no-repeat -392px -1px;
    width: 8px;
    height: 8px;
    overflow: hidden;
    display: inline-block;
    margin: 2px 5px;
    cursor: pointer;
  }

}