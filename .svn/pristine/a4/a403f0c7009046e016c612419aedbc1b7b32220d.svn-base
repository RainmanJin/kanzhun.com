.btn_grey {
  display: inline-block;
  height: 32px;
  line-height: 32px;
  background: url(../images/btn_bgs.png);
  border: 1px solid #e5e5e5;
}
.btn_grey input {
  height: 30px;
  border: 0;
  background-color: transparent;
  padding: 0 25px;
  cursor: pointer;
  overflow: visible;
}
