.grey_green {
  border: 1px solid #dfdfdf;
  height: 33px;
  line-height: 33px;
  background: url(../images/btn_bgs.png) repeat-x 0 -42px;
  display: inline-block;
  padding: 0 25px;
  text-align: center;
  font-size: 14px;
  color: #343434;
  border-radius: 3px;
  cursor: pointer;
  white-space: nowrap;
  vertical-align: middle;
}
.grey_green input,
.grey_green button {
  height: 33px;
  cursor: pointer;
  display: block;
  border: 0;
  background-color: transparent;
  width: 100%;
  overflow: visible;
  color: #343434;
}
.grey_green:hover {
  border-color: #7cb228;
