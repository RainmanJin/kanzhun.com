}
.grey_79 {
  color: #797979;
}
.grey_af {
  color: #afafaf;
}
.green {
  color: #62bb6a;
}
div.container {
  width: 990px;
  padding: 35px 0 25px 0;
  margin: 25px auto 0 auto;
  background-color: #fff;
  -webkit-box-shadow: 0 5px 10px rgba(4, 0, 0, 0.2);
  -moz-box-shadow: 0 5px 10px rgba(4, 0, 0, 0.2);
  -ms-box-shadow: 0 5px 10px rgba(4, 0, 0, 0.2);
  box-shadow: 0 5px 10px rgba(4, 0, 0, 0.2);
}
