@font-face {
  font-family: 'AVGARDNI';
  src: url('../images/AVGARDNI.TTF');
}
body {
  font: normal 14px/1.5 Arial, STHeiti, Microsoft YaHei, simsun, Helvetica, sans-serif;
  color: #303030;
  background-color: #f2f2f2;
}
input,
button,
textarea {
  font: normal 14px/1.5 Arial, STHeiti, Microsoft YaHei, simsun, Helvetica, sans-serif;
}
a {
  color: #007ead;
}
.red {
  color: #ff0000;
}
.black {
  color: #303030;
}
.blue {
  color: #007ead;
}
.grey_99 {
