html {
  height: 100%;
}
body {
  font: normal 1rem/1.5 Arial, Helvetica, "Microsoft YaHei", STHeiti, "Droid Sans Fallback", sans-serif;
  color: #343434;
  height: 100%;
}
input,
button,
textarea,
select {
  font: normal 1rem/1.5 Arial, Helvetica, "Microsoft YaHei", STHeiti, "Droid Sans Fallback", sans-serif;
}
a {
  color: #343434;
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
}
a.a_ul {
  text-decoration: underline;
}
h1,
h2,
h3,
h4,
h5,
h6 {
  font-size: 1em;
  line-height: 1;
}
.wrapper {
  width: 100%;
  min-height: 100%;
  height: auto;
}
img.block {
  display: block;
}
html {
  font-size: 14px;
}
body.grey {
  background-color: #e9efef;
}
body.blue {
  background-color: #53cac3;
}
dl.ol dt {
  background-color: #53cac3;
  color: #ffffff;
  padding: 1rem 0 .3rem;
  width: 100%;
}
dl.ol dt span {
  font-size: 0.93rem;
  width: 30%;
  display: inline-block;
  text-align: center;
}
dl.ol dt strong {
  width: 40%;
  display: inline-block;
  font-size: 1.143rem;
  text-align: center;
}
dl.ol dd {
  background-color: #3a3e4b;
  position: relative;
}
dl.ol dd img.counts {
  width: 100%;
}
dl.ol dd ul {
  position: absolute;
  width: 100%;
  height: 100%;
  left: 0;
  top: 0;
}
dl.ol dd ul li {
  text-align: center;
  float: left;
  color: #ffffff;
  height: 100%;
}
dl.ol dd ul li span {
  position: relative;
  top: 20%;
  width: 100%;
}
dl.ol dd ul li:nth-of-type(1) {
  font-size: 1.5rem;
  width: 34%;
}
dl.ol dd ul li:nth-of-type(2) {
  font-size: 2.2rem;
  width: 32%;
}
dl.ol dd ul li:nth-of-type(2) span {
  top: 32%;
}
dl.ol dd ul li:nth-of-type(3) {
  width: 34%;
  font-size: 1.5rem;
}
.barrage {
  background-color: #3a3e4b;
  -webkit-box-shadow: inset 0 -10px 10px #333333;
  -ms-box-shadow: inset 0 -10px 10px #333333;
  box-shadow: inset 0 -10px 10px #333333;
  padding: 20px 0px;
  overflow: hidden;
  height: 80px;
  position: relative;
}
.barrage > div {
  position: absolute;
  left: 100%;
  z-index: 2;
}
.barrage > div p {
  white-space: nowrap;
  z-index: 5;
  margin-bottom: 100px;
  margin-bottom: 10px;
}
.barrage > div a {
  display: inline-block;
  color: #00cdae;
  margin-right: 20px;
  padding: 2px 5px;
  font-size: 20px;
  color: rgba(255, 255, 255, 0.8);
  border-radius: 5px;
}
.barrage > div .up a {
  background-color: #4eaeab;
}
.barrage > div .down a {
  background-color: #cebc43;
}
ul.jobs-list {
  padding: 10px;
}
ul.jobs-list li {
  background-color: #fff;
  margin-bottom: 10px;
  padding: 10px;
  -webkit-box-shadow: 1px 1px 2px #d2d8d8;
  -ms-box-shadow: 1px 1px 2px #d2d8d8;
  box-shadow: 1px 1px 2px #d2d8d8;
  overflow: hidden;
}
ul.jobs-list li > span {
  float: right;
  position: relative;
  width: 40px;
}
ul.jobs-list li > span img {
  display: block;
  width: 40px;
  height: 40px;
  position: relative;
  z-index: 2;
  border: 1px solid #cbcbcb;
  -webkit-box-sizing: border-box;
  -ms-box-sizing: border-box;
  box-sizing: border-box;
}
ul.jobs-list li > span em {
  width: 56.56px;
  height: 56.56px;
  position: absolute;
  z-index: 0;
  background-color: #f2f2f2;
  display: block;
  -webkit-transform: rotate(45deg);
  -ms-transform: rotate(45deg);
  transform: rotate(45deg);
  top: 12px;
  left: 12px;
}
ul.jobs-list li h4 {
  font-size: 1.2rem;
  margin-bottom: .8rem;
  font-weight: normal;
}
ul.jobs-list li p {
  font-size: 0.93rem;
  color: #585858;
  line-height: 1em;
}
ul.jobs-list li i {
  width: 14px;
  height: 11.5px;
  display: inline-block;
  vertical-align: middle;
  margin-right: .2rem;
}
ul.jobs-list li i.b {
  background: url("../images/i-boss.png") no-repeat;
  -webkit-background-size: 14px;
  background-size: 14px;
}
ul.jobs-list li i.c {
  background: url("../images/i-chat.png") no-repeat;
  -webkit-background-size: 14px;
  background-size: 14px;
  margin-left: 1rem;
}
.mars-tt {
  background-color: #f46563;
  height: 34px;
  line-height: 34px;
  position: relative;
}
.mars-tt img {
  width: 50px;
  position: absolute;
  left: 50%;
  margin-left: -25px;
  top: -6px;
}
.mars-tt span {
  width: 50%;
  display: inline-block;
  text-align: center;
  height: 34px;
  color: #ffffff;
  font-size: 0.93rem;
  -webkit-box-sizing: border-box;
  -ms-box-sizing: border-box;
  box-sizing: border-box;
}
.mars-tt span:nth-of-type(1) {
  padding-right: 25px;
}
.mars-tt i {
  width: 14px;
  height: 12px;
  display: inline-block;
  vertical-align: middle;
  margin-right: .5rem;
}
.mars-tt i.m-b {
  background: url("../images/m-b.jpg") no-repeat;
  -webkit-background-size: 14px;
  background-size: 14px;
}
.mars-tt i.m-c {
  background: url("../images/m-c.jpg") no-repeat;
  -webkit-background-size: 14px;
  background-size: 14px;
  margin-left: 1rem;
}
.mars-content {
  padding: 20px 10px 10px;
}
.mars-content .mars-search select {
  border: 1px solid #ffffff;
  border-radius: 4px;
  background-color: transparent;
  height: 38px;
  width: 100%;
  color: #ffffff;
  background: url("../images/m-s.png") no-repeat 10px 10px;
  -webkit-background-size: 14px;
  background-size: 14px;
  padding-left: 2rem;
  -webkit-box-sizing: border-box;
  -ms-box-sizing: border-box;
  box-sizing: border-box;
  margin-right: -30%;
  position: relative;
  z-index: 2;
}
.mars-content .mars-search span {
  width: 30%;
  display: inline-block;
  height: 38px;
  background-color: #fff;
  color: #53cac3;
  vertical-align: middle;
  border-top-right-radius: 4px;
  border-bottom-right-radius: 4px;
  text-align: center;
  line-height: 38px;
}
table.mars-tb {
  width: 100%;
  color: #53cac3;
  font-size: 1rem;
  margin: 10px 0;
}
table.mars-tb td {
  -webkit-box-sizing: border-box;
  -ms-box-sizing: border-box;
  box-sizing: border-box;
  width: 33.33%;
  border-right: 1px solid #53cac3;
  border-bottom: 1px solid #53cac3;
  background-color: #fff;
  text-align: center;
  padding: .5rem 0;
}
table.mars-tb td.current {
  -webkit-box-shadow: 0 0 5px red;
  -ms-box-shadow: 0 0 5px red;
  box-shadow: 0 0 5px red;
  position: relative;
  z-index: 2;
  color: #e85253;
}
.mars-list li {
  background-color: #fff;
  padding: 10px;
  margin-bottom: 10px;
  position: relative;
}
.mars-list li > img {
  width: 55px;
  height: 55px;
  float: left;
  margin-top: 10px;
  border-radius: 50%;
}
.mars-list li div {
  margin-left: 65px;
}
.mars-list li div p {
  font-size: 1rem;
}
.mars-list li div p:nth-of-type(1) {
  font-size: 1.143rem;
  color: #53cac3;
}
.mars-list li div p:nth-of-type(2) {
  color: #797979;
}
.mars-list li div p:nth-of-type(3) {
  color: #797979;
}
.mars-list li div p:nth-of-type(3) em {
  color: #e85253;
  font-size: 1.2rem;
}
.mars-list li div p img {
  height: 12px;
  margin: 0 .2rem 0 1rem;
  vertical-align: middle;
}
.mars-list li > strong {
  width: 80px;
  height: 26px;
  line-height: 22px;
  font-size: 0.8rem;
  color: #ffffff;
  position: absolute;
  right: -3px;
  top: 10px;
  padding-left: 23px;
  -webkit-box-sizing: border-box;
  -ms-box-sizing: border-box;
  box-sizing: border-box;
  background: url("../images/tip.png") no-repeat;
  -webkit-background-size: 100%;
  background-size: 100%;
  white-space: nowrap;
  overflow: hidden;
}
