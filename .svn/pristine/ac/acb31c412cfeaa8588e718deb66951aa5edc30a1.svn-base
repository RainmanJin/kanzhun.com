  padding-top: 20px;
}
body.err section.err_content .r {
  margin-left: 50px;
}
body.err section.err_content .r h2 {
  font-size: 60px;
}
body.err section.err_content .r h3 {
  border-top: 1px solid #d1d1d1;
  border-bottom: 1px solid #d1d1d1;
  padding: 10px 0;
}
body.err section.err_content .r h4 {
  margin-top: 15px;
  color: #999999;
  font-weight: normal;
}
body.err section.err_content .r p {
  margin-top: 10px;
}
body.err section.err_content .r p .btn {
  display: inline-block;
  width: 150px;
  padding: 5px 0;
  background-color: #e5e5e5;
  border: 1px solid #7cb228;
  border-radius: 10px;
  text-align: center;
}
i.i_exclam {
  display: inline-block;
  background: url(../images/icon_err.png);
  width: 38px;
  height: 89px;
}
a.ugc_prompt {
  display: block;
  padding: 10px 5px 10px 10px;
  margin: 0 10px 20px 10px;
  border: 1px solid #fdd6ab;
  border-radius: 5px;
  background-color: #fff7ed;
  -webkit-background-size: 37px 36.5px;
  -moz-background-size: 37px 36.5px;
  -o-background-size: 37px 36.5px;
  background-size: 37px 36.5px;
  overflow: hidden;
  *zoom: 1;
}
a.ugc_prompt p {
  float: left;
  width: 17em;
}
a.ugc_prompt div {
  float: right;
  width: 22px;
  height: 32px;
  margin-top: 2px;
  padding: 4px 0 0 14px;
  border: 1px solid #ffe5c5;
  background-color: #fff;
  border-radius: 20px;
}
a.ugc_prompt div span {
  width: 0;
  height: 0;
  overflow: hidden;
  font-size: 0;
  border-width: 10px 0 10px 15px;
  border-style: dashed solid;
}
a.ugc_prompt div span.white_bg {
  margin-left: -22px;
  border-color: transparent transparent transparent #fff;
}
a.ugc_prompt div span.yellow_bg {
  border-color: transparent transparent transparent #f7a74c;
}
