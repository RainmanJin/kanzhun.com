  font-size: 24px;
  color: #999999;
  width: 16px;
  text-align: center;
}
section.searchCondition_content ul li.ok {
  background: #f1fdde;
}
section.searchCondition_content ul li.ok em {
  color: #000;
}
section.searchCondition_content ul li.no_ok {
  background: #ffffff;
}
section.searchCondition_content ul li.no_ok em {
  color: #000;
}
nav.resume_nav {
  height: 90px;
  background: #ffffff;
  padding: 20px 10px 0;
  position: relative;
}
nav.resume_nav .nav_l {
  position: absolute;
}
nav.resume_nav .nav_r {
  padding: 10px 0 0 75px;
}
nav.resume_nav .nav_r h1 {
  color: #999999;
}
nav.resume_nav .nav_r p {
  padding: 10px 0 0 0;
  text-align: center;
}
.doughnua_chart {
  width: 69px;
  height: 69px;
  position: relative;
}
.doughnua_chart span {
  position: absolute;
  left: 0;
  top: 0;
  display: block;
  width: 69px;
  height: 69px;
  line-height: 69px;
  text-align: center;
  font-size: 16px;
  font-weight: 700;
}
.field {
  margin: 10px;
  padding: 15px 10px;
  background: #ffffff;
}
.headPic a {
  display: inline-block;
  position: relative;
}
.headPic a img {
  width: 76px;
  height: 76px;
  padding: 3px;
  border: 1px solid #d1d1d1;
  background: #ffffff;
  vertical-align: middle;
}
.headPic a span {
  display: inline-block;
  position: absolute;
  bottom: 3px;
  left: 4px;
  width: 76px;
  height: 24px;
  background: rgba(0, 0, 0, 0.3);
  color: #ffffff;
  text-align: center;
}
.headPic a [type="file"] {
  position: absolute;
  top: 0;
  left: 0;
  display: inline-block;
  width: 100%;
  height: 100%;
  opacity: 0;
}
.resumeAdd {
  height: 65px;
  position: relative;
}
.resumeAdd .icon {
  display: inline-block;
  width: 48px;
  height: 48px;
  position: absolute;
  top: 10px;
}
.resumeAdd h3 {
  padding: 15px 0 0 60px;
  color: #999999;
}
.resumeShow {
  position: relative;
}
.resumeShow ul.set {
  position: absolute;
  right: 10px;
  top: 10px;
}
.resumeShow ul.set li {
  float: left;
  margin-left: 10px;
}
.resumeShow ul.set li a {
  display: inline-block;
}
.resumeShow dt {
  margin-top: 15px;
  font-size: 16px;
}
.resumeShow dd em {
  color: #e5e5e5;
}
.mask {
  position: absolute;
  width: 100%;
  height: 100%;
  z-index: 99;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.5);
}
.dialog {
  position: fixed;
  z-index: 100;
  top: 50%;
  left: 50%;
  margin-top: -75px;
  margin-left: -150px;
  padding: 15px 10px;
  width: 280px;
  background: #ffffff;
  border: 1px solid grey;
}
.dialog p {
  text-align: center;
}
.dialog div.set {
  text-align: center;
}
/*404和500页面*/
body.err .wrapper {
  background-color: #ffffff;
}
body.err section.err_content {
  width: 280px;
  margin: 0 auto;
  padding: 0 20px;
  padding-top: 30px;
  position: relative;
}
body.err section.err_content .l {
