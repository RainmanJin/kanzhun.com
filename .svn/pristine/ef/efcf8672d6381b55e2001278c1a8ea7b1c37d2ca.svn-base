.bisys .pagination {
  vertical-align: middle;
}
.bisys .bi-pa {
  margin-left: 20px;
}
.bisys .bi-pa .form-control {
  width: 35px;
}
.bisys .table th {
  background-color: #f5f5f5;
}
.bisys .table td {
  padding: 0;
  vertical-align: bottom;
}
.bisys .table td div {
  padding: 8px 8px 2px;
}
.bisys .percent {
  width: 100%;
  height: 8px;
  margin: 0;
}
.bisys .percent i {
  background-color: #ede907;
  display: block;
  height: 8px;
  width: 0;
  overflow: hidden;
  -webkit-transition: all .3s ease;
  -moz-transition: all .3s ease;
  -ms-transition: all .3s ease;
  -o-transition: all .3s ease;
  transition: all .3s ease;
}
.bisys .chart {
  border: 1px solid #ccc;
  min-height: 300px;
  width: 100%;
}
.bisys .chart-control {
  width: 300px;
  vertical-align: top;
  padding-left: 20px;
}
.bisys .chart-control > p > .btn {
  width: 128px;
  margin-bottom: 4px;
}
.bisys .chart-control-b {
  vertical-align: bottom;
}
